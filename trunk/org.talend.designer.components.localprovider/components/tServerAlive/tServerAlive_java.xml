<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"         
	COMPATIBILITY="ALL"
    AUTHOR="Talend"
 	RELEASE_DATE="20070210A"
    STARTABLE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Misc</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROTOCOL"
      FIELD="CLOSED_LIST"
      NUM_ROW="1"
    >
      <ITEMS DEFAULT="PING">
        <ITEM NAME="PING" VALUE="PING" />
        <ITEM NAME="TELNET" VALUE="TELNET"/>
      </ITEMS>
    </PARAMETER>
          
    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="3"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="3"
      SHOW_IF="PROTOCOL == 'TELNET'"
    >
      <DEFAULT>23</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TIMEOUT_INTERVAL"
      FIELD="TEXT"
      NUM_ROW="4"
    >
      <DEFAULT>5</DEFAULT>
    </PARAMETER>

    <PARAMETER
   	  NAME="NOTE"
   	  FIELD="LABEL"
   	  NUM_ROW="4"
    >
   	  <DEFAULT>second(s)</DEFAULT>
    </PARAMETER>            
  </PARAMETERS>
   
  <ADVANCED_PARAMETERS>
  	<PARAMETER 
  	  NAME="ENABLE_PROXY_SERVER" 
  	  FIELD="CHECK"
	  NUM_ROW="10"
	  SHOW_IF="PROTOCOL == 'TELNET'">
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>
	
    <PARAMETER
      NAME="PROXY_HOST"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW_IF="(PROTOCOL == 'TELNET') and (ENABLE_PROXY_SERVER == 'true')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PROXY_PORT"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW_IF="(PROTOCOL == 'TELNET') and (ENABLE_PROXY_SERVER == 'true')"
    >
      <DEFAULT>"23"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PROXY_USER"
      FIELD="TEXT"
      NUM_ROW="40"
      SHOW_IF="(PROTOCOL == 'TELNET') and (ENABLE_PROXY_SERVER == 'true')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PROXY_PASS"
      FIELD="TEXT"
      NUM_ROW="40"
      SHOW_IF="(PROTOCOL == 'TELNET') and (ENABLE_PROXY_SERVER == 'true')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>        	    
  </ADVANCED_PARAMETERS>

  <CODEGENERATION/>

  <RETURNS>
 	<RETURN NAME="SERVER_ALIVE_RESULT"  TYPE="id_Boolean" AVAILABILITY="AFTER"/>
  </RETURNS>

</COMPONENT>
