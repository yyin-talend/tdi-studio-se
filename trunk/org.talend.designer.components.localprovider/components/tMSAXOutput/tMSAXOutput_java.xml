<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070313A"
    STARTABLE="false"
    DATA_AUTO_PROPAGATE="false"
    HAS_CONDITIONAL_OUTPUTS="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Business/Microsoft_AX</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:MSAX"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
 	<PARAMETER
      NAME="DOMAIN"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="DOMAIN"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER	
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="5"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DATA_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="6"
    >
      <ITEMS DEFAULT="INSERT">
        <ITEM NAME="INSERT"           VALUE="INSERT" />
        <ITEM NAME="UPDATE"           VALUE="UPDATE"/>
        <ITEM NAME="INSERT_OR_UPDATE" VALUE="INSERT_OR_UPDATE"/>
        <ITEM NAME="UPDATE_OR_INSERT" VALUE="UPDATE_OR_INSERT"/>
        <ITEM NAME="DELETE"           VALUE="DELETE"/>
      </ITEMS>
    </PARAMETER>
          
    <PARAMETER
      NAME="SCHEMA_FLOW"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="7"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="SCHEMA_REJECT"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="7"
      CONTEXT="REJECT"
      SHOW="true">
      <TABLE READONLY="true">
        <COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>

    <PARAMETER
      NAME="ADD_COLS"
      FIELD="TABLE"
      REQUIRED="false" 
      NUM_ROW="9"
      NB_LINES="3"
    >
      <ITEMS>

        <ITEM NAME="NAME" VALUE='"colname"'/>
        <ITEM NAME="OPERATOR" FIELD="CLOSED_LIST" NOT_SHOW_IF="(DATA_ACTION=='INSERT')">
          <ITEMS DEFAULT="EQUAL">
            <ITEM NAME="EQUAL" VALUE="=="/>
            <ITEM NAME="NOT_EQUAL" VALUE="!="/>
            <ITEM NAME="LESS_THAN" VALUE="&lt;"/>
            <ITEM NAME="LESS_EQUAL_THAN" VALUE="&lt;="/>
            <ITEM NAME="GREAT_THAN" VALUE="&gt;"/>
            <ITEM NAME="GREAT_EQUAL_THAN" VALUE="&gt;="/>
          </ITEMS>
        </ITEM>	
        <ITEM NAME="DATATYPE" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="String">
            <ITEM NAME="String" VALUE="String"/>
            <ITEM NAME="Char" VALUE="Char"/>
            <ITEM NAME="Int" VALUE="Int"/>
            <ITEM NAME="Float" VALUE="Float"/>
            <ITEM NAME="Double" VALUE="Double"/>
            <ITEM NAME="Long" VALUE="Long"/>
            <ITEM NAME="Short" VALUE="Short"/>
            <ITEM NAME="Data" VALUE="Data"/>
            <ITEM NAME="Boolean" VALUE="Boolean"/>
            <ITEM NAME="Object" VALUE="Object"/>
          </ITEMS>
        </ITEM>		
        <ITEM NAME="SQL" VALUE='"demostring"' />

        <ITEM NAME="POS" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="BEFORE">
            <ITEM NAME="BEFORE" VALUE="BEFORE"/>
            <ITEM NAME="AFTER" VALUE="AFTER"/>
            <ITEM NAME="REPLACE" VALUE="REPLACE"/>
          </ITEMS>
        </ITEM>

        <ITEM NAME="REFCOL" FIELD="COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

	<PARAMETER
      NAME="DIE_ON_ERROR"
      FIELD="CHECK"
      NUM_ROW="7"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
        
  </PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
       <IMPORT NAME="MSAX-JInterop" MODULE="a-j-interop.jar" REQUIRED="true" />
       <IMPORT NAME="MSAX-Iwonbat" MODULE="b-iwombat.jar" REQUIRED="true" />
       <IMPORT NAME="MSAX-Jarapac" MODULE="b-jarapac.jar" REQUIRED="true" />
       <IMPORT NAME="MSAX-Jcifs" MODULE="b-jcifs-1.2.9.jar" REQUIRED="true" />
       <IMPORT NAME="MSAX-Jcifs-ntlm-auth" MODULE="b-jcifs-ntlm-auth.jar" REQUIRED="true" />
       <IMPORT NAME="MSAX-Ncacn_ip_tcp" MODULE="b-ncacn_ip_tcp.jar" REQUIRED="true" />
       <IMPORT NAME="MSAX-Ncacn_np" MODULE="b-ncacn_np.jar" REQUIRED="true" />
       <IMPORT NAME="MSAX-Ntlm-securityt" MODULE="b-ntlm-security.jar" REQUIRED="true" />
     </IMPORTS>
  </CODEGENERATION>
   
  <RETURNS>
    <RETURN NAME="NB_LINE"          TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_UPDATED"  TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_INSERTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_DELETED"  TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>