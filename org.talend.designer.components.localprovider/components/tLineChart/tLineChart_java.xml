<COMPONENT>
<HEADER
	PLATEFORM="ALL"
	SERIAL=""
	VERSION="0.102"
	STATUS="ALPHA"
	COMPATIBILITY="ALL"
	AUTHOR="stefeclipse.free.fr/blog"
	RELEASE_DATE="20070611A"
	STARTABLE="true"
	>
	<SIGNATURE/>
</HEADER>

<FAMILIES>
	<FAMILY>Business_Intelligence/Charts</FAMILY>
</FAMILIES>

<DOCUMENTATION>
	<URL/>
</DOCUMENTATION>

<CONNECTORS>
	<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
	<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
	<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
	<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="0" />
	<CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="0"/>
	<CONNECTOR CTYPE="COMPONENT_ERROR" MAX_INPUT="0"/>
	<CONNECTOR CTYPE="RUN_IF" MAX_INPUT="0"/>
</CONNECTORS>

<PARAMETERS>

<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1">
	<TABLE  READONLY="true">
		<COLUMN NAME="serie" TYPE="id_String" LENGTH="50"/>
		<COLUMN NAME="x" TYPE="id_Integer"/>
		<COLUMN NAME="y" TYPE="id_Integer"/>
	</TABLE>
</PARAMETER>

<PARAMETER NAME="GENERATED_IMAGE_PATH" FIELD="FILE" NUM_ROW="2" REQUIRED="true">
	<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.png"</DEFAULT>
</PARAMETER>

<PARAMETER NAME="CHART_TITLE" FIELD="STRING" NUM_ROW="3" REQUIRED="true">
	<DEFAULT>"Chart title"</DEFAULT>
</PARAMETER>

<PARAMETER NAME="DOMAIN_AXIS_LABEL" FIELD="String" NUM_ROW="3" REQUIRED="true">
	<DEFAULT>"Domain axis label"</DEFAULT>
</PARAMETER>

<PARAMETER NAME="RANGE_AXIS_LABEL" FIELD="String" NUM_ROW="3" REQUIRED="true">
	<DEFAULT>"Range axis label"</DEFAULT>
</PARAMETER>

<PARAMETER NAME="PLOT_ORIENTATION" FIELD="String" NUM_ROW="4" REQUIRED="true">
	<DEFAULT>org.jfree.chart.plot.PlotOrientation.VERTICAL</DEFAULT>
</PARAMETER>

<PARAMETER NAME="INCLUDE_LEGEND" FIELD="BOOLEAN" NUM_ROW="4" REQUIRED="true">
	<DEFAULT>true</DEFAULT>
</PARAMETER>

<PARAMETER NAME="IMAGE_WIDTH" FIELD="INTEGER" NUM_ROW="5" REQUIRED="true">
	<DEFAULT>500</DEFAULT>
</PARAMETER>

<PARAMETER NAME="IMAGE_HEIGHT" FIELD="INTEGER" NUM_ROW="5" REQUIRED="true">
	<DEFAULT>500</DEFAULT>
</PARAMETER>

<PARAMETER NAME="MOVING_AVERAGE" FIELD="CHECK" NUM_ROW="6">
	<DEFAULT>false</DEFAULT>
</PARAMETER>

<PARAMETER NAME="MOVING_AVERAGE_PERIOD" FIELD="TEXT" NUM_ROW="6" SHOW_IF="MOVING_AVERAGE == 'true'">
	<DEFAULT>20</DEFAULT>
</PARAMETER>

<PARAMETER NAME="LOWER_BOUND" FIELD="TEXT" NUM_ROW="7">
	<DEFAULT></DEFAULT>
</PARAMETER>

<PARAMETER NAME="UPPER_BOUND" FIELD="TEXT" NUM_ROW="7">
	<DEFAULT></DEFAULT>
</PARAMETER>

<PARAMETER NAME="BACKGROUND_CHART" FIELD="TEXT" NUM_ROW="8">
	<DEFAULT></DEFAULT>
</PARAMETER>

<PARAMETER NAME="BACKGROUND_PLOT" FIELD="TEXT" NUM_ROW="9">
	<DEFAULT></DEFAULT>
</PARAMETER>

</PARAMETERS>

<CODEGENERATION>
	<IMPORTS>
		<IMPORT NAME="JFreeChart" MODULE="jfreechart-1.0.3.jar" REQUIRED="true"/>
		<IMPORT NAME="JCommon" MODULE="jcommon-1.0.6.jar" REQUIRED="true"/>
	</IMPORTS>
</CODEGENERATION>

<RETURNS>
	<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
</RETURNS>

</COMPONENT>