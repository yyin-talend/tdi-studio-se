<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    
    DATA_AUTO_PROPAGATE="false"
   	HAS_CONDITIONAL_OUTPUTS="true"
   	SCHEMA_AUTO_PROPAGATE="true"

    STARTABLE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Business/Marketo</FAMILY>
	<FAMILY>Cloud</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <!-- <CONNECTOR NAME="MAIN" CTYPE="FLOW" MAX_OUTPUT="1" BASE_SCHEMA="FLOW"/>-->
    <CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" 
     NOT_SHOW_IF="(OPERATION=='syncMultipleLeads') or (DIE_ON_ERROR == 'true')"/>
    <CONNECTOR CTYPE="ITERATE" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

	<PARAMETER
      NAME="SCHEMA_FLOW"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="1">
      <TABLE>
        <COLUMN NAME="Id" TYPE="id_Integer" READONLY="true" />
		<COLUMN NAME="Email" TYPE="id_String" />
		<COLUMN NAME="ForeignSysPersonId" TYPE="id_String" />
		<COLUMN NAME="ForeignSysType" TYPE="id_String" />
      </TABLE>
    </PARAMETER>
    
    <!-- <PARAMETER
      NAME="SCHEMA_FLOW"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="1"
      CONTEXT="MAIN">
      <TABLE>
        <COLUMN NAME="SYNC_STATUS" TYPE="id_String" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>
    -->
    <PARAMETER
      NAME="SCHEMA_REJECT"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="1"
      CONTEXT="REJECT"
      SHOW="true">
      <TABLE READONLY="true">
        <COLUMN NAME="ERROR_MSG" TYPE="id_String" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>

	<PARAMETER NAME="ENDPOINT" FIELD="TEXT" NUM_ROW="10" REQUIRED="true">
		<DEFAULT>"https://na-c.marketo.com/soap/mktows/1_5"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="SECRET_KEY" FIELD="TEXT" NUM_ROW="20" REQUIRED="true">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="CLIENT_ACCESSID" FIELD="TEXT" NUM_ROW="20" REQUIRED="true" >
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="OPERATION" FIELD="CLOSED_LIST" NUM_ROW="30"
			REQUIRED="true">
		<ITEMS DEFAULT="syncLead">
			<ITEM NAME="syncLead" VALUE="syncLead" />
			<ITEM NAME="syncMultipleLeads" VALUE="syncMultipleLeads" />
		</ITEMS>
	</PARAMETER>
	<PARAMETER NAME="MAPPING_INPUT" FIELD="TABLE" NUM_ROW="40" NB_LINES="6" REQUIRED="true">
		<ITEMS BASED_ON_SCHEMA="true">
			<ITEM NAME="PARAMETER_NAME" FIELD="String" VALUE='""'/>
		</ITEMS>	
	</PARAMETER>
	
	<PARAMETER NAME="DE_DUPENABLED" FIELD="CHECK" NUM_ROW="50" REQUIRED="true" SHOW_IF="OPERATION=='syncMultipleLeads'">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="BATCH_SIZE" FIELD="TEXT" NUM_ROW="60" REQUIRED="true" SHOW_IF="OPERATION=='syncMultipleLeads'">
		<DEFAULT>100</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="TIMEOUT" FIELD="TEXT" NUM_ROW="70" REQUIRED="true">
		<DEFAULT>600000</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="100" REQUIRED="true">
		<DEFAULT>true</DEFAULT>
	</PARAMETER>
  </PARAMETERS>
  
  <CODEGENERATION>
 	 <IMPORTS>
		<IMPORT NAME="SOAP-axis" MODULE="axis.jar" REQUIRED="true" />
		<IMPORT NAME="SOAP-discovery" MODULE="commons-discovery-0.2.jar" REQUIRED="true" />
		<IMPORT NAME="SOAP-jaxrpc" MODULE="jaxrpc.jar" REQUIRED="true" />
		<IMPORT NAME="SOAP-saaj" MODULE="saaj.jar" REQUIRED="true" />
		<IMPORT NAME="SOAP-wsdl" MODULE="wsdl4j-1.6.2.jar" REQUIRED="true" />
		<IMPORT NAME="SOAP-logging" MODULE="commons-logging-1.1.jar" REQUIRED="true" />
		<IMPORT NAME="SOAP-JavaMail" MODULE="mail.jar" REQUIRED="true" />
		<IMPORT NAME="SOAP-JAF" MODULE="activation.jar" REQUIRED="true" />
		<IMPORT NAME="XML-DOM4J" MODULE="dom4j-1.6.1.jar" REQUIRED="true" />
		<IMPORT NAME="XML-XPATH-JAXEN" MODULE="jaxen-1.1.1.jar" REQUIRED="true" />
		<IMPORT NAME="TALEND-MARKETO" MODULE="talend-marketo.jar" REQUIRED="true" />
	</IMPORTS>
  </CODEGENERATION>

  <RETURNS>
      <RETURN NAME="NB_CALL" TYPE="id_Integer" AVAILABILITY="AFTER" />
  </RETURNS>
  	
</COMPONENT>
