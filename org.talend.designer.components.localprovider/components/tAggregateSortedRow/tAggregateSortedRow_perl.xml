<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    SCHEMA_AUTO_PROPAGATE="false"
    VISIBLE="true"
    HAS_CONDITIONAL_OUTPUTS="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="WARNING"
      FIELD="LABEL"
      REQUIRED="true"
      NUM_ROW="1"
      CONTEXT="255;0;0">
      <DEFAULT>Notice: Last aggregation key won't be present in output. Add a dummy last key (with tExternalSortRow for example).</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="10"
    >
      <DEFAULT/> 
    </PARAMETER>

    <PARAMETER
      NAME="GROUPBYS"
      FIELD="TABLE"
      NUM_ROW="4"
      NB_LINES="20"
    >
      <ITEMS>
        <ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" />
        <ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="OPERATIONS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="30"
      NB_LINES="3"
    >
      <ITEMS>
        <ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" />
        <ITEM NAME="FUNCTION" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="COUNT">
            <ITEM NAME="COUNT"  VALUE="count"/>
            <ITEM NAME="MIN"    VALUE="min"/>
            <ITEM NAME="MAX"    VALUE="max"/>
            <ITEM NAME="AVG"    VALUE="avg"/>
            <ITEM NAME="SUM"    VALUE="sum"/>
            <ITEM NAME="FIRST"    VALUE="first"/>
            <ITEM NAME="LAST"    VALUE="last"/>
          </ITEMS>
        </ITEM>
        <ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
