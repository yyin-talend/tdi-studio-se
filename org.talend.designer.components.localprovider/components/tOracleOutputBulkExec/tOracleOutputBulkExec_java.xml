<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="2"
      REPOSITORY_VALUE="DATABASE:ORACLE"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PORT"
    >
      <DEFAULT>"1521"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SID"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="USERNAME"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PASSWORD"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="6"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DATA_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="6"
      REQUIRED="true"
    >
      <ITEMS DEFAULT="INSERT">
        <ITEM NAME="INSERT"           VALUE="INSERT" />
        <ITEM NAME="APPEND"           VALUE="APPEND" />
        <ITEM NAME="REPLACE"          VALUE="REPLACE"/>
        <ITEM NAME="TRUNCATE"         VALUE="TRUNCATE"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="6"
      REQUIRED="true"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

<!-- following property should not be displayed -->

    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="14"
      REQUIRED="true"
    >
      <DEFAULT>"C:/temp/oracle_data.dat"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELDSEPARATOR"
      FIELD="TEXT"
      NUM_ROW="16"
      REPOSITORY_VALUE="FIELD_SEPARATOR"
    >
      <DEFAULT>";"</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION>
    <TEMPLATES INPUT="tOOB" OUTPUT="tOBE">

      <TEMPLATE NAME="tOOB" COMPONENT="tOracleOutputBulk">
        <LINK_TO NAME="tOBE" CTYPE="BEFORE"/>
      </TEMPLATE>
      
      <TEMPLATE NAME="tOBE" COMPONENT="tOracleBulkExec"/>
      
      <TEMPLATE_PARAM SOURCE="self.HOST"   TARGET="tOBE.HOST" />
      <TEMPLATE_PARAM SOURCE="self.DBNAME" TARGET="tOBE.DBNAME" />
      <TEMPLATE_PARAM SOURCE="self.PORT"   TARGET="tOBE.PORT"   />
      <TEMPLATE_PARAM SOURCE="self.USER"   TARGET="tOBE.USER" />
      <TEMPLATE_PARAM SOURCE="self.PASS"   TARGET="tOBE.PASS" />
      <TEMPLATE_PARAM SOURCE="self.TABLE"  TARGET="tOBE.TABLE" />
	  <TEMPLATE_PARAM SOURCE="self.DATA_ACTION"  TARGET="tOBE.DATA_ACTION" />
	  
      <TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tOOB.FILENAME" />
      <TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tOBE.DATA" />
      
      <TEMPLATE_PARAM SOURCE="self.ENCODING" TARGET="tOOB.ENCODING" />
      <TEMPLATE_PARAM SOURCE="self.ENCODING" TARGET="tOBE.ENCODING" />

      <TEMPLATE_PARAM SOURCE="self.FIELDSEPARATOR"  TARGET="tOOB.FIELDSEPARATOR" />
      <TEMPLATE_PARAM SOURCE="self.FIELDSEPARATOR"  TARGET="tOBE.FIELDS_TERMINATED_BY" />

    </TEMPLATES>
  </CODEGENERATION>
  <RETURNS/>
</COMPONENT>
