<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    IS_MULTIPLYING_OUTPUTS="true"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MIN_OUTPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      READONLY="true"
      NUM_ROW="1"
    >
    </PARAMETER>


    <PARAMETER
      NAME="NORMALIZE_COLUMN"
      FIELD="PREV_COLUMN_LIST"
      REQUIRED="true"
      NUM_ROW="2"
    >
    </PARAMETER>

    <PARAMETER
      NAME="ITEMSEPARATOR"
      FIELD="TEXT"
      NUM_ROW="2"
    >
      <DEFAULT>","</DEFAULT>
    </PARAMETER>
  </PARAMETERS>

  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
