<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    IS_MULTIPLYING_OUTPUTS="true"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MIN_OUTPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      READONLY="true"
      NUM_ROW="10"
    >
    </PARAMETER>


    <PARAMETER
      NAME="NORMALIZE_COLUMN"
      FIELD="COLUMN_LIST"
      REQUIRED="true"
      NUM_ROW="20"
    >
    </PARAMETER>

    <PARAMETER
      NAME="ITEMSEPARATOR"
      FIELD="TEXT"
      NUM_ROW="30"
    >
      <DEFAULT>','</DEFAULT>
    </PARAMETER>
  </PARAMETERS>

  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="NB_LINE_INPUT" TYPE="INTEGER" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_OUTPUT" TYPE="INTEGER" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
