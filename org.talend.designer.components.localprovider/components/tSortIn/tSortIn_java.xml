<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
    VISIBLE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="ORIGIN"
      FIELD="TEXT"
      NUM_ROW="2"
      REQUIRED="true"
    >
      <DEFAULT>array_tArray_1</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CRITERIA"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="3"
      NB_LINES="3"
    >
      <ITEMS>
        <ITEM NAME="COLNAME" FIELD="COLUMN_LIST"/>

        <ITEM NAME="SORT" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="ALPHA">
            <ITEM NAME="NUM" VALUE="num"/>
            <ITEM NAME="ALPHA" VALUE="alpha"/>
          </ITEMS>
        </ITEM>

        <ITEM NAME="ORDER" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="ASC">
            <ITEM NAME="ASC" VALUE="asc"/>
            <ITEM NAME="DESC" VALUE="desc"/>
          </ITEMS>
        </ITEM>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="4"
    >
      <DEFAULT/> 
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>