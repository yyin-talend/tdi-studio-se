<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE />
  </HEADER>

  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>

   <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="XML_FILE"
      FIELD="FILE"
      NUM_ROW="1"
      REQUIRED="true"
    >
    <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/myfile.xml"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="XSLT_FILE"
      FIELD="FILE"
      NUM_ROW="2"
      REQUIRED="true"
    > 
	<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/myfile.xsl"</DEFAULT>  
    </PARAMETER>
    <PARAMETER
      NAME="OUTPUT_FILE"
      FIELD="FILE"
      NUM_ROW="3"
      REQUIRED="true"      
     >
     <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/myfile.out"</DEFAULT>
    </PARAMETER>
  </PARAMETERS>
  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="XML-LibXML" MODULE="XML::LibXML" REQUIRED="true" />
      <IMPORT NAME="XML-LibXSLT" MODULE="XML::LibXSLT" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>
  <RETURNS>
  	<RETURN NAME="OUTPUT_FILEPATH" TYPE="id_String" AVAILABILITY="FLOW"/>
  	<RETURN NAME="OUTPUT_FILENAME" TYPE="id_String" AVAILABILITY="FLOW"/>
  </RETURNS>
</COMPONENT>