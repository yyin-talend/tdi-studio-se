<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
       SERIAL=""
      VERSION="0.101"
       STATUS="ALPHA"
         
COMPATIBILITY="ALL"
       AUTHOR="Talend"
 RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1" READONLY="true">
      <TABLE>
        <COLUMN NAME="moment" TYPE="id_Date" PATTERN='"y-M-d"'/>
        <COLUMN NAME="pid" TYPE="id_Long" LENGTH="10"/>
        <COLUMN NAME="master_pid" TYPE="id_Long" LENGTH="10"/>
        <COLUMN NAME="project" TYPE="id_String" LENGTH="50"/>
        <COLUMN NAME="job" TYPE="id_String" LENGTH="50"/>
        <COLUMN NAME="context" TYPE="id_String" LENGTH="50"/>
        <COLUMN NAME="priority" TYPE="id_Integer" LENGTH="3"/>
        <COLUMN NAME="type" TYPE="id_String" LENGTH="10"/>
        <COLUMN NAME="origin" TYPE="id_String" LENGTH="255"/>
        <COLUMN NAME="message" TYPE="id_String" LENGTH="255"/>
        <COLUMN NAME="code" TYPE="id_Integer" LENGTH="3"/>
        <COLUMN NAME="java_error_message" TYPE="id_String" LENGTH="255"/>
        <COLUMN NAME="java_error_code" TYPE="id_Integer" LENGTH="3"/>
      </TABLE>
    </PARAMETER>

    <PARAMETER NAME="CATCH_JAVA_EXCEPTION" FIELD="CHECK" REQUIRED="true" NUM_ROW="2" REPOSITORY_VALUE="CATCH_JAVA_EXCEPTION">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="CATCH_TDIE" FIELD="CHECK" REQUIRED="true" NUM_ROW="2" REPOSITORY_VALUE="CATCH_TDIE">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="CATCH_TWARN" FIELD="CHECK" REQUIRED="true" NUM_ROW="2" REPOSITORY_VALUE="CATCH_TWARN">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
   </PARAMETERS>

   <CODEGENERATION/>

   <RETURNS/>

</COMPONENT>
