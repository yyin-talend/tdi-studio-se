<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE />
  </HEADER>

  <FAMILIES>
    <FAMILY>File/Input</FAMILY>
  </FAMILIES>
  
  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>
  
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>
  
  <PARAMETERS>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="10"
      READONLY="true"
    >
      <TABLE>
        <COLUMN NAME="host"    TYPE="string" LENGTH="50"/>
        <COLUMN NAME="user"    TYPE="string" LENGTH="50"/>
        <COLUMN NAME="date"    TYPE="date" LENGTH="13"/>
        <COLUMN NAME="time"    TYPE="string" LENGTH="10"/>
        <COLUMN NAME="diffgmt" TYPE="string" LENGTH="10"/>
        <COLUMN NAME="rtype"   TYPE="string" LENGTH="10"/>
        <COLUMN NAME="file"    TYPE="string" LENGTH="255"/>
        <COLUMN NAME="proto"   TYPE="string" LENGTH="10"/>
        <COLUMN NAME="code"    TYPE="int"    LENGTH="3"/>
        <COLUMN NAME="bytes"   TYPE="int"    LENGTH="11"/>
        <COLUMN NAME="refer"   TYPE="string" LENGTH="255"/>
        <COLUMN NAME="agent"   TYPE="string" LENGTH="255"/>
      </TABLE>
    </PARAMETER>

    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="20"
      REQUIRED="true"
    >
      <DEFAULT>'__COMP_DEFAULT_FILE_DIR__/in.csv'</DEFAULT>
    </PARAMETER>
    
  </PARAMETERS>
  
  <CODEGENERATION>
     <IMPORTS>
       <IMPORT
         NAME="Parse-AccessLogEntry"
         MODULE="Parse::AccessLogEntry"
         REQUIRED="true"
       />
     </IMPORTS>
  </CODEGENERATION>
  
  <RETURNS/>

</COMPONENT>