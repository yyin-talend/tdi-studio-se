<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
		STARTABLE="false" DATA_AUTO_PROPAGATE="false" HAS_CONDITIONAL_OUTPUTS="true">
		<SIGNATURE />
	</HEADER>

  <FAMILIES>
    <FAMILY>Databases/LDAP</FAMILY>
  </FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0"
			MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" NOT_SHOW_IF="(DATA_ACTION == 'INSERT' OR DIE_ON_ERROR == 'true')"/>
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="1" REPOSITORY_VALUE="LDAP" />
		<PARAMETER NAME="HOST" FIELD="TEXT" NUM_ROW="2" REQUIRED="true" REPOSITORY_VALUE="HOST">
			<DEFAULT>"ldap.openldap.org"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="3" REQUIRED="true" REPOSITORY_VALUE="PORT">
			<DEFAULT>389</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="BASEDN" FIELD="TEXT" NUM_ROW="4" REPOSITORY_VALUE="BASEDN">
			<DEFAULT>"dc=OpenLDAP,dc=org"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROTOCOL" FIELD="CLOSED_LIST" NUM_ROW="5"
			REQUIRED="true" REPOSITORY_VALUE="PROTOCOL">
			<ITEMS DEFAULT="LDAP">
				<ITEM NAME="LDAP" VALUE="LDAP" />
				<ITEM NAME="LDAPS" VALUE="LDAPS" />
				<ITEM NAME="TLS" VALUE="TLS" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="ADVANCEDCA" FIELD="CHECK" NUM_ROW="6"
			SHOW_IF=" ( PROTOCOL=='LDAPS' or PROTOCOL=='TLS')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="STORECA" FIELD="FILE" NUM_ROW="7"
			SHOW_IF="( ADVANCEDCA=='true' and ( PROTOCOL=='LDAPS' or PROTOCOL=='TLS'))">
			<DEFAULT>"c:/mycacerts"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="STORECAPWD" FIELD="TEXT" NUM_ROW="7" 
				SHOW_IF="( ADVANCEDCA=='true' and PROTOCOL=='LDAPS')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USER" FIELD="TEXT" NUM_ROW="8" REPOSITORY_VALUE="USER">
			<DEFAULT>"cn=Directory Manager"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PASSWD" FIELD="TEXT" NUM_ROW="9" REPOSITORY_VALUE="PASSWD">
			<DEFAULT>"password"</DEFAULT>
		</PARAMETER>
				<PARAMETER NAME="ALIASES" FIELD="CLOSED_LIST" NUM_ROW="11" REPOSITORY_VALUE="ALIASES">
			<ITEMS DEFAULT="ALWAYS">
				<ITEM NAME="ALWAYS" VALUE="always" />
				<ITEM NAME="NEVER" VALUE="never" />
				<ITEM NAME="SEARCHING" VALUE="searching" />
				<ITEM NAME="FINDING" VALUE="finding" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="REFERRALS" FIELD="CLOSED_LIST" NUM_ROW="12" REPOSITORY_VALUE="REFERRALS">
			<ITEMS DEFAULT="IGNORE">
				<ITEM NAME="IGNORE" VALUE="ignore" />
				<ITEM NAME="FOLLOW" VALUE="follow" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="REMANE_DN_OLD" FIELD="COLUMN_LIST" NUM_ROW="14" SHOW="true"></PARAMETER>
	    <PARAMETER NAME="REMANE_DN_NEW" FIELD="COLUMN_LIST" NUM_ROW="15" SHOW="true"></PARAMETER>
		
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="15">
			<TABLE>
				<COLUMN NAME="oldDN" TYPE="id_String" LENGTH="255" />
				<COLUMN NAME="newDN" TYPE="id_String" LENGTH="255" />
				
			</TABLE>
      <!--SHOW_IF="PROPERTY_TYPE=='BUILT_IN'"-->
		</PARAMETER>
		<PARAMETER NAME="SCHEMA_REJECT" FIELD="SCHEMA_TYPE"
			REQUIRED="true" NUM_ROW="16" CONTEXT="REJECT" SHOW="true">
			<TABLE READONLY="true">
				<COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255"
					READONLY="false" CUSTOM="true" />
				<COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255"
					READONLY="false" CUSTOM="true" />
			</TABLE>
		</PARAMETER>
		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="17">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</PARAMETERS>

	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>
	
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="ldap" MODULE="talendssl.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>
	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>