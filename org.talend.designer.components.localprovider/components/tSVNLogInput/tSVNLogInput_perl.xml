<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="URL"
      FIELD="TEXT"
      NUM_ROW="10"
      REQUIRED="true"
    >
      <DEFAULT>'http://svn.gna.org/svn/phpwebgallery'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FROM_REV"
      FIELD="TEXT"
      NUM_ROW="20"
    >
      <DEFAULT>1</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SINGLE_REV"
      FIELD="CHECK"
      NUM_ROW="20"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TO_REV"
      FIELD="TEXT"
      NUM_ROW="40"
      SHOW_IF="SINGLE_REV == 'false'"
    >
      <DEFAULT>'HEAD'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="50"
      READONLY="true"
    >
      <TABLE>
        <COLUMN NAME="revision" TYPE="int"/>
        <COLUMN NAME="author" TYPE="String" LENGTH="50"/>
        <COLUMN NAME="date" TYPE="Day"/>
        <COLUMN NAME="message" TYPE="String" LENGTH="10000"/>
        <COLUMN NAME="nb_file_added" TYPE="int"/>
        <COLUMN NAME="nb_file_modified" TYPE="int"/>
        <COLUMN NAME="nb_file_deleted" TYPE="int"/>
      </TABLE>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="SVN-Log" MODULE="SVN::Log" REQUIRED="true" />
     </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER"/>
  </RETURNS>

</COMPONENT>