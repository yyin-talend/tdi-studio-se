<!-- $Id: tRSSInput_java.xml 696 2006-11-23 05:45:10Z nrousseau $ -->
<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
   <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="1"
    >
      <TABLE READONLY="true">
        <COLUMN NAME="TITLE" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      	<COLUMN NAME="DESCRIPTION" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
       	<COLUMN NAME="PUBDATE" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
       	 <COLUMN NAME="LINK" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>
    <PARAMETER
      NAME="RSS"
      FIELD="TEXT"
      NUM_ROW="2"
      REQUIRED="true"
    >
    <DEFAULT>"http://rss.sina.com.cn/sports/global/italy.xml"</DEFAULT>
    </PARAMETER>
    
     <PARAMETER
      NAME="ARTICLE_DATE"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="3"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER
      NAME="DATE"
      FIELD="TEXT"
      REQUIRED="true"
      NUM_ROW="3"
      SHOW_IF="ARTICLE_DATE == 'true'"
    >
    <DEFAULT>2007-5-30   08:12:12</DEFAULT>
    </PARAMETER>
    
     <PARAMETER
      NAME="ARTICLE_AMOUNT"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="4"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="AMOUNT"
      FIELD="TEXT"
      REQUIRED="true"
      NUM_ROW="4"
      SHOW_IF="ARTICLE_AMOUNT == 'true'"
    >
    <DEFAULT>10</DEFAULT>
    </PARAMETER>
  </PARAMETERS>
  
	<CODEGENERATION>
    <IMPORTS>
       <IMPORT NAME="Java-RSS" MODULE="rsslib4j-0.2.jar" REQUIRED="true" />
     </IMPORTS>
  </CODEGENERATION>
  
  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>