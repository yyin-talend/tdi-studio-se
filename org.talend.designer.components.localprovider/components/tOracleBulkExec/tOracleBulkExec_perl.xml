<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="RUN_ERROR" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="RUN_IF" MAX_OUTPUT="1"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE"
    />

    <PARAMETER
      NAME="SERVICE_NAME"
      FIELD="TEXT"
      NUM_ROW="3"
      REQUIRED="true"
    >
      <DEFAULT>'TALEND_ORA'</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="4"
      REQUIRED="true"
      REPOSITORY_VALUE="USERNAME"
    >
      <DEFAULT>'scott'</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="4"
      REQUIRED="true"
      REPOSITORY_VALUE="PASSWORD"
    >
      <DEFAULT>'tiger'</DEFAULT>
    </PARAMETER>


    <PARAMETER
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="6"
      REQUIRED="true"
    >
      <DEFAULT>'emp'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DATA_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="6"
      REQUIRED="true"
    >
      <ITEMS DEFAULT="INSERT">
        <ITEM NAME="INSERT"           VALUE="INSERT" />
        <ITEM NAME="APPEND"           VALUE="APPEND" />
        <ITEM NAME="REPLACE"          VALUE="REPLACE"/>
        <ITEM NAME="TRUNCATE"         VALUE="TRUNCATE"/>
      </ITEMS>
    </PARAMETER>



    <PARAMETER
      NAME="DATA"
      FIELD="FILE"
      NUM_ROW="7"
      REQUIRED="true"
    >
      <DEFAULT>'C:\talend_files\emp_bulk.txt'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELDS_TERMINATED_BY"
      FIELD="TEXT"
      NUM_ROW="8"
      REQUIRED="true"
    >
      <DEFAULT>';'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELDS_ENCLOSED_BY"
      FIELD="TEXT"
      NUM_ROW="8"
      REQUIRED="false"
    >
      <DEFAULT>'"'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="10"
      REQUIRED="true"
    >
      <DEFAULT>'ISO-8859-15'</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="OUTPUT" FIELD="CLOSED_LIST" NUM_ROW="10">
       <ITEMS DEFAULT="OUTPUT_TO_CONSOLE">
          <ITEM NAME="OUTPUT_TO_CONSOLE" VALUE="OUTPUT_TO_CONSOLE" />
          <ITEM NAME="RETRIEVE_OUTPUT"   VALUE="RETRIEVE_OUTPUT" />
       </ITEMS>
    </PARAMETER>


   </PARAMETERS>

   <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="DBI" MODULE="DBI" REQUIRED="true" />
       <IMPORT NAME="DBD-Oracle" MODULE="DBD::Oracle" REQUIRED="true" />
     </IMPORTS>
   </CODEGENERATION>

   <RETURNS>
      <RETURN NAME="NB_LINE_DATA" TYPE="INTEGER" AVAILABILITY="AFTER"/>
      <RETURN NAME="NB_LINE_BAD" TYPE="INTEGER" AVAILABILITY="AFTER"/>
      <RETURN NAME="NB_LINE_INSERTED" TYPE="INTEGER" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>
