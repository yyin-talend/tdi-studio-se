        serviceUrl_<%=cid%> = "(description=(address=(protocol=tcp)(host=" + host_<%=cid%> + ")(port=" + <%=port%> + "))(connect_data=(service_name=" + dbname_<%=cid%> + ")))";
        url_<%=cid%> = "jdbc:oracle:oci8:@" + <%=localServiceName%>; 
        <%
    }
