<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="RUN_ERROR" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="RUN_IF" MAX_OUTPUT="1"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:ORACLE"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    />
    
    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="2"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      FILTER="tOracleConnection"
      NUM_ROW="2"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />        
    
    <PARAMETER
      NAME="CONNECTION_TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="3"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <ITEMS DEFAULT="ORACLE_SID">
        <ITEM NAME="ORACLE_SID" VALUE="ORACLE_SID" />
        <ITEM NAME="ORACLE_SERVICE_NAME" VALUE="ORACLE_SERVICE_NAME"/>
      </ITEMS>
    </PARAMETER>    

   <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"      
    >
      <DEFAULT>"Talend-dbms"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"      
    >
      <DEFAULT>"1521"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="SID"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"      
    >
      <DEFAULT>"Talend"</DEFAULT>
    </PARAMETER>

    <PARAMETER 
      NAME="SCHEMA_DB"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="SCHEMA"
    >
      <DEFAULT>"ROOT"</DEFAULT>
    </PARAMETER>
          
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="6"
      REQUIRED="true"
      REPOSITORY_VALUE="USERNAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"      
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="6"
      REQUIRED="true"
      REPOSITORY_VALUE="PASSWORD"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"      
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="9"
    >
      <ITEMS DEFAULT="NONE">
        <ITEM NAME="NONE" VALUE="NONE" />
        <ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE"/>
        <ITEM NAME="CREATE" VALUE="CREATE"/>
        <ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS"/>
        <ITEM NAME="CLEAR" VALUE="CLEAR"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="9"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>     

    <PARAMETER
      NAME="DATA_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="9"
      REQUIRED="true"
    >
      <ITEMS DEFAULT="INSERT">
        <ITEM NAME="INSERT"           VALUE="INSERT" />
        <ITEM NAME="APPEND"           VALUE="APPEND" />
        <ITEM NAME="REPLACE"          VALUE="REPLACE"/>
        <ITEM NAME="TRUNCATE"         VALUE="TRUNCATE"/>
      </ITEMS>
    </PARAMETER>



    <PARAMETER
      NAME="DATA"
      FIELD="FILE"
      NUM_ROW="10"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.csv"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELDS_TERMINATED_BY"
      FIELD="TEXT"
      NUM_ROW="11"
      REQUIRED="true"
    >
      <DEFAULT>";"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELDS_ENCLOSED_BY"
      FIELD="TEXT"
      NUM_ROW="11"
      REQUIRED="false"
    >
      <DEFAULT>"""</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="OPTIONS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="12"
      NB_LINES="5"
    >
      <ITEMS>
        <ITEM NAME="KEY" FIELD="CLOSED_LIST" >
          <ITEMS DEFAULT="BINDSIZE">
            <ITEM NAME="BINDSIZE"  VALUE="BINDSIZE"/>
            <ITEM NAME="COLUMNARRAYROWS"    VALUE="COLUMNARRAYROWS"/>
            <ITEM NAME="DIRECT"    VALUE="DIRECT"/>
            <ITEM NAME="ERRORS"    VALUE="ERRORS"/>
            <ITEM NAME="LOAD"    VALUE="LOAD"/>
            <ITEM NAME="MULTITHREADING"    VALUE="MULTITHREADING"/>
           	<ITEM NAME="PARFILE"    VALUE="PARFILE"/>
            <ITEM NAME="PARALLEL"    VALUE="PARALLEL"/>
            <ITEM NAME="READSIZE"    VALUE="READSIZE"/>
            <ITEM NAME="RESUMABLE"  VALUE="RESUMABLE"/>
            <ITEM NAME="RESUMABLE_NAME"    VALUE="RESUMABLENAME"/>
            <ITEM NAME="RESUMABLE_TIMEOUT"    VALUE="RESUMABLETIMEOUT"/>
            <ITEM NAME="ROWS"    VALUE="ROWS"/>
            <ITEM NAME="SILENT"    VALUE="SILENT"/>
            <ITEM NAME="SKIP"    VALUE="SKIP"/>
            <ITEM NAME="SKIP_INDEX_MAINTENANCE"    VALUE="SKIPINDEXMAINTENANCE"/>
          	<ITEM NAME="SKIP_UNUSABLE_INDEXES"    VALUE="SKIPUNUSABLEINDEXES"/>
          	<ITEM NAME="STREAMSIZE"    VALUE="STREAMSIZE"/>
          </ITEMS>
        </ITEM>
        <ITEM NAME="VALUE" FIELD="TEXT"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="13"
      REQUIRED="true"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="OUTPUT" FIELD="CLOSED_LIST" NUM_ROW="13">
       <ITEMS DEFAULT="OUTPUT_TO_CONSOLE">
          <ITEM NAME="OUTPUT_TO_CONSOLE" VALUE="OUTPUT_TO_CONSOLE" />
          <ITEM NAME="RETRIEVE_OUTPUT"   VALUE="RETRIEVE_OUTPUT" />
       </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="14">
      <DEFAULT/> 
    </PARAMETER>

	<PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="15"
      SHOW="false"
    >       
      <DEFAULT>oracle_id</DEFAULT>
    </PARAMETER>

   </PARAMETERS>

   <CODEGENERATION>
     <IMPORTS>
		<IMPORT NAME="Driver-Oracle" MODULE="ojdbc14.jar" REQUIRED="true" />
		<IMPORT NAME="Delimited_File_Reader" MODULE="file_delimited.jar" REQUIRED="true"/>
     </IMPORTS>
   </CODEGENERATION>

   <RETURNS>
      <RETURN NAME="NB_LINE_DATA" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="NB_LINE_BAD" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="NB_LINE_INSERTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>
