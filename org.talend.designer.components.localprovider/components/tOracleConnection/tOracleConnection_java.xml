<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/Oracle</FAMILY>
    <FAMILY>ELT/Connections</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

    <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:ORACLE"
	  NOT_SHOW_IF="(USE_TNS_FILE == 'true') AND (CONNECTION_TYPE == 'ORACLE_RAC')"
    />
    <PARAMETER
      NAME="CONNECTION_TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="20"    
      REPOSITORY_VALUE="CONNECTION_TYPE" 
    >
      <ITEMS DEFAULT="ORACLE_SID">
        <ITEM NAME="ORACLE_SID" VALUE="ORACLE_SID" />
        <ITEM NAME="ORACLE_SERVICE_NAME" VALUE="ORACLE_SERVICE_NAME"/>
        <ITEM NAME="ORACLE_OCI" VALUE="ORACLE_OCI"/>
		<ITEM NAME="ORACLE_RAC" VALUE="ORACLE_RAC"/>
		<ITEM NAME="ORACLE_WALLET" VALUE="ORACLE_WALLET"/>
      </ITEMS>
    </PARAMETER>
	<PARAMETER NAME="DB_VERSION" FIELD="CLOSED_LIST" NUM_ROW="25"
		REPOSITORY_VALUE="DB_VERSION">
		<ITEMS DEFAULT="ORACLE_11">
			<ITEM NAME="ORACLE_11-6" VALUE="ojdbc6-11g.jar" />
			<ITEM NAME="ORACLE_11" VALUE="ojdbc5-11g.jar" />
			<ITEM NAME="ORACLE_10" VALUE="ojdbc14-10g.jar" />
			<ITEM NAME="ORACLE_9" VALUE="ojdbc14-9i.jar" />
			<ITEM NAME="ORACLE_8" VALUE="ojdbc12-8i.jar" />
		</ITEMS>
	</PARAMETER>
	<PARAMETER
		NAME="RAC_URL"
		FIELD="TEXT"
		NUM_ROW="26"
		REPOSITORY_VALUE="RAC_URL"
		REQUIRED="true"
		SHOW_IF="CONNECTION_TYPE == 'ORACLE_RAC'"
		>
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="USE_TNS_FILE" FIELD="CHECK"
		NUM_ROW="28"
		NOT_SHOW_IF="CONNECTION_TYPE == 'ORACLE_RAC'"
		>
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

    <PARAMETER
      NAME="TNS_FILE"
      FIELD="FILE"
      NUM_ROW="29"
      SHOW_IF="(USE_TNS_FILE == 'true') AND (CONNECTION_TYPE != 'ORACLE_RAC')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>	
	
	<PARAMETER
      NAME="TNS_EDITOR"
	  FIELD="TNS_EDITOR"
      NUM_ROW="29"      
	  SHOW_IF="(USE_TNS_FILE == 'true') AND (CONNECTION_TYPE != 'ORACLE_RAC')"
    >      
    </PARAMETER>
	

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="SERVER_NAME"
      NOT_SHOW_IF="(CONNECTION_TYPE == 'ORACLE_OCI') OR (CONNECTION_TYPE == 'ORACLE_WALLET') OR (CONNECTION_TYPE == 'ORACLE_RAC')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
	<PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>Oracle</DEFAULT>
    </PARAMETER>
    
	<PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="PORT"
      NOT_SHOW_IF="(CONNECTION_TYPE == 'ORACLE_OCI') OR (CONNECTION_TYPE == 'ORACLE_WALLET') OR (CONNECTION_TYPE == 'ORACLE_RAC')"
    >
      <DEFAULT>"1521"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      NOT_SHOW_IF="(CONNECTION_TYPE == 'ORACLE_OCI') OR (CONNECTION_TYPE == 'ORACLE_WALLET') OR (CONNECTION_TYPE == 'ORACLE_RAC')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="LOCAL_SERVICE_NAME"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      SHOW_IF="CONNECTION_TYPE == 'ORACLE_OCI'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER 
      NAME="SCHEMA_DB"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SCHEMA"
	  NOT_SHOW_IF="(CONNECTION_TYPE == 'ORACLE_WALLET') OR (CONNECTION_TYPE == 'ORACLE_RAC')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="50"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
	  NOT_SHOW_IF="CONNECTION_TYPE == 'ORACLE_WALLET'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="50"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
	  NOT_SHOW_IF="CONNECTION_TYPE == 'ORACLE_WALLET'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="80"
      REQUIRED="true"
      SHOW="false"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>
    
	<PARAMETER 
		NAME="PROPERTIES" 
		FIELD="TEXT" 
		NUM_ROW="85" 
		REPOSITORY_VALUE="PROPERTIES_STRING"
		NOT_SHOW_IF="(CONNECTION_TYPE == 'ORACLE_WALLET')">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER 
		NAME="NOTE" 
		FIELD="LABEL" 
		NUM_ROW="86"
		NOT_SHOW_IF="(CONNECTION_TYPE == 'ORACLE_WALLET') OR (CONNECTION_TYPE == 'ORACLE_RAC')">
			<DEFAULT>*Note: Example for Additional JDBC Parameters: "parameterName1=value1&amp;&amp;parameterName2=value2"</DEFAULT>

	</PARAMETER>
	
	<PARAMETER
      NAME="USE_SHARED_CONNECTION"
	  FIELD="CHECK"
      NUM_ROW="90"
      REQUIRED="true"
    >      
	<DEFAULT>false</DEFAULT>
    </PARAMETER>
	
	<PARAMETER
      NAME="SHARED_CONNECTION_NAME"
	  FIELD="TEXT"
      NUM_ROW="90"
      REQUIRED="true"
	  SHOW_IF="USE_SHARED_CONNECTION == 'true'"
    >      
	<DEFAULT></DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="JDBC_URL"
	  FIELD="TEXT"
      NUM_ROW="45"
      REQUIRED="true"
	  SHOW_IF="CONNECTION_TYPE == 'ORACLE_WALLET'"
    > 
	<DEFAULT>"jdbc:oracle:thin:USER/MDP@server"</DEFAULT>
	   </PARAMETER>
   </PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="AUTO_COMMIT" FIELD="CHECK" NUM_ROW="10">  
			<DEFAULT>false</DEFAULT>    
    	</PARAMETER>
	</ADVANCED_PARAMETERS>

   <CODEGENERATION>
    <IMPORTS>
       <IMPORT NAME="TNS" MODULE="tns.jar" REQUIRED="true" />    
    </IMPORTS>   
   </CODEGENERATION>

   <RETURNS/>
</COMPONENT>