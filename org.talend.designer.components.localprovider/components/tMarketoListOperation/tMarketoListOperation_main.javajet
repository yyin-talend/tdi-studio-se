					nb_call_<%=cid%>++;		
				}
    		}
			<%
	        if (strict) {
	        %>
	            throw(ex_<%=cid%>);
	        <%
	        } else {
	        %>
	        	System.err.println(ex_<%=cid%>.getMessage());
