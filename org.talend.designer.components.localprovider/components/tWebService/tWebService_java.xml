			<DEFAULT>"C:/clientTrustStore.trustStore"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SSL_TRUSTSERVER_PASSWORD" FIELD="TEXT" NUM_ROW="61" SHOW_IF="NEED_SSL_TO_TRUSTSERVER == 'true'">
			<DEFAULT>"password"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER
	      NAME="DIE_ON_ERROR"
	      FIELD="CHECK"
	      NUM_ROW="100">
	      <DEFAULT>true</DEFAULT>
