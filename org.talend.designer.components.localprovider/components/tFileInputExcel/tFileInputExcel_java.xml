<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
	HAS_CONDITIONAL_OUTPUTS="true"    
  >
    <SIGNATURE />
  </HEADER>

  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" />
    <CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />    
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE" />
    <CONNECTOR CTYPE="RUN_OK" />
    <CONNECTOR CTYPE="RUN_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="20"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.xls"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SHEETNAME"
      FIELD="TEXT"
      NUM_ROW="30"
      REQUIRED="true"
    >
      <DEFAULT>"Sheet1"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="HEADER"
      FIELD="TEXT"
      NUM_ROW="40"
    >
      <DEFAULT>0</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FOOTER"
      FIELD="TEXT"
      NUM_ROW="40"
    >
      <DEFAULT>0</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="LIMIT"
      FIELD="TEXT"
      NUM_ROW="40"
    >
      <DEFAULT />
    </PARAMETER>

    <PARAMETER
      NAME="FIRST_COLUMN"
      FIELD="TEXT"
      NUM_ROW="50"
    >
      <DEFAULT>1</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="LAST_COLUMN"
      FIELD="TEXT"
      NUM_ROW="50"
    >
      <DEFAULT/>
    </PARAMETER>

    <PARAMETER
      NAME="DIE_ON_ERROR"
      FIELD="CHECK"
      NUM_ROW="48"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="60"
    />
    
    <PARAMETER
      NAME="SCHEMA_REJECT"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="60"
      CONTEXT="REJECT"
      SHOW="true">
      <TABLE READONLY="true">
        <COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>    

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="70"
      REQUIRED="true"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
      <IMPORT
        NAME="Java_Excel"
        MODULE="jxl.jar"
        REQUIRED="true"
      />
    </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
  </RETURNS>
</COMPONENT>