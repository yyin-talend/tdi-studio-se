
<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
		STARTABLE="true" HAS_CONDITIONAL_OUTPUTS="true">
		<SIGNATURE />
	</HEADER>

  <FAMILIES>
    <FAMILY>File/Input</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" />
		<CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0"
			MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="10" REPOSITORY_VALUE="EXCEL" />
			
		<PARAMETER NAME="VERSION_2007" FIELD="CHECK" REQUIRED="true" NUM_ROW="15">
      		<DEFAULT>false</DEFAULT>
    	</PARAMETER>
			
		<PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="20"
			REQUIRED="true" REPOSITORY_VALUE="FILE_PATH">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.xls"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ALL_SHEETS" FIELD="CHECK" REQUIRED="true" NUM_ROW="30" REPOSITORY_VALUE="SELECT_ALL_SHEETS">
      		<DEFAULT>false</DEFAULT>
    	</PARAMETER>
		
		<PARAMETER NAME="SHEETLIST" FIELD="TABLE" REQUIRED="true" NUM_ROW="35"
      	  	NB_LINES="5" SHOW_IF="ALL_SHEETS == 'false'" REPOSITORY_VALUE="SHEET_LIST">
      		<ITEMS>
        		<ITEM NAME="SHEETNAME" REPOSITORY_ITEM="EXCEL_SHEETNAME"/>
        		<ITEM NAME="USE_REGEX" FIELD="CHECK"/>
      		</ITEMS>
    	</PARAMETER>

		<PARAMETER NAME="HEADER" FIELD="TEXT" NUM_ROW="40" REPOSITORY_VALUE="HEADER">
			<DEFAULT>0</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FOOTER" FIELD="TEXT" NUM_ROW="40" REPOSITORY_VALUE="FOOTER">
			<DEFAULT>0</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="LIMIT" FIELD="TEXT" NUM_ROW="40" REPOSITORY_VALUE="LIMIT">
			<DEFAULT />
		</PARAMETER>
		
		<PARAMETER NAME="AFFECT_EACH_SHEET" FIELD="CHECK" NUM_ROW="45">
			<DEFAULT />
		</PARAMETER>		

		<PARAMETER NAME="FIRST_COLUMN" FIELD="TEXT" NUM_ROW="50" REPOSITORY_VALUE="FIRST_COLUMN">
			<DEFAULT>1</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="LAST_COLUMN" FIELD="TEXT" NUM_ROW="50" REPOSITORY_VALUE="LAST_COLUMN">
			<DEFAULT />
		</PARAMETER>

		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="48">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="60" />

		<PARAMETER NAME="SCHEMA_REJECT" FIELD="SCHEMA_TYPE"
			REQUIRED="true" NUM_ROW="60" CONTEXT="REJECT" SHOW="true">
			<TABLE READONLY="true">
				<COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255"
					READONLY="false" CUSTOM="true" />
				<COLUMN NAME="errorMessage" TYPE="id_String"
					LENGTH="255" READONLY="false" CUSTOM="true" />
			</TABLE>
		</PARAMETER>
		
	</PARAMETERS>

	<ADVANCED_PARAMETERS>
	
		<PARAMETER NAME="ADVANCED_SEPARATOR" FIELD="CHECK"
			REQUIRED="true" NUM_ROW="49" REPOSITORY_VALUE="ADVANCED_SEPARATOR">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="THOUSANDS_SEPARATOR" FIELD="TEXT"
			REQUIRED="true" NUM_ROW="49"
			SHOW_IF="(ADVANCED_SEPARATOR == 'true')" REPOSITORY_VALUE="THOUSANDS_SEPARATOR">
			<DEFAULT>","</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DECIMAL_SEPARATOR" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="49" SHOW_IF="(ADVANCED_SEPARATOR == 'true')" REPOSITORY_VALUE="DECIMAL_SEPARATOR">
			<DEFAULT>"."</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="70"
			REQUIRED="true" REPOSITORY_VALUE="ENCODING">
			<DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="READ_REAL_VALUE" FIELD="CHECK"
			REQUIRED="true" NUM_ROW="80" REPOSITORY_VALUE="READ_REAL_VALUE" SHOW_IF="(VERSION_2007 == 'false')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="STOPREAD_ON_EMPTYROW" FIELD="CHECK" REQUIRED="true" NUM_ROW="90" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="NOVALIDATE_ON_CELL" FIELD="CHECK" REQUIRED="true" NUM_ROW="91" SHOW_IF="(VERSION_2007 == 'false')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>		
		
		<PARAMETER NAME="SUPPRESS_WARN" FIELD="CHECK" REQUIRED="true" NUM_ROW="92" SHOW_IF="(VERSION_2007 == 'false')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>	
				
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="Java_Excel" MODULE="jxl.jar" REQUIRED="true" />
			
			<IMPORT NAME="dom4j" MODULE="dom4j-1.6.1.jar" REQUIRED="true" />
			<IMPORT NAME="jsr173" MODULE="jsr173_1.0_api.jar" REQUIRED="true" />
			<IMPORT NAME="log4j" MODULE="log4j-1.2.9.jar" REQUIRED="true" />
			<IMPORT NAME="ooxml-schemas" MODULE="ooxml-schemas-1.0.jar" REQUIRED="true" />
			<IMPORT NAME="poi" MODULE="poi-3.5-FINAL-20090928.jar" REQUIRED="true" />
			<IMPORT NAME="poi-ooxml" MODULE="poi-ooxml-3.5-FINAL-20090928.jar" REQUIRED="true" />
			<IMPORT NAME="xmlbeans" MODULE="xmlbeans-2.3.0.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
		<RETURN NAME="CURRENT_SHEET" TYPE="id_String" AVAILABILITY="FLOW" />
	</RETURNS>
</COMPONENT>