<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE />
  </HEADER>

  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>

   <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="10"
      REQUIRED="true"
      REPOSITORY_VALUE="FILE_PATH"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.csv"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DESTINATION"
      FIELD="DIRECTORY"
      NUM_ROW="20"
      REQUIRED="true"
      REPOSITORY_VALUE="DESTINATION_PATH"
    > 
    <DEFAULT>"__COMP_DEFAULT_FILE_DIR__"</DEFAULT>  
    </PARAMETER>
    
    <PARAMETER
      NAME="RENAME"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="30"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>    
    
    <PARAMETER
      NAME="DESTINATION_RENAME"
      FIELD="TEXT"
      NUM_ROW="30"
      REQUIRED="true"
      SHOW_IF="RENAME == 'true'"
    > 
    <DEFAULT>"NewName.temp"</DEFAULT>  
    </PARAMETER>
    
   <PARAMETER
      NAME="REMOVE_FILE"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="40"
      REPOSITORY_VALUE="REMOVE_FILE"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER
      NAME="REPLACE_FILE"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="40"
      REPOSITORY_VALUE="REPLACE_FILE"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
    <PARAMETER
      NAME="CREATE_DIRECTORY"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="40"
      REPOSITORY_VALUE="CREATE_DIRECTORY"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
  </PARAMETERS>
  
  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="filecopy" MODULE="filecopy.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>
  
  <RETURNS/>
  
</COMPONENT>