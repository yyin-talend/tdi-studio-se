<COMPONENT>

	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20080104"
		STARTABLE="true" HAS_CONDITIONAL_OUTPUTS="true"
		EXTENSION="org.talend.designer.hl7">
		<SIGNATURE />
	</HEADER>

  <FAMILIES>
  	<FAMILY>Business/Healthcare</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR MULTI_SCHEMA="true" CTYPE="FLOW" MAX_INPUT="0" MIN_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
  	<PARAMETER
    	NAME="NOTE"
    	FIELD="LABEL"
    	NUM_ROW="1"
    >
    	<DEFAULT>this component only support HL7 versin2.x</DEFAULT>
    </PARAMETER>
	    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      NUM_ROW="2"
      REPOSITORY_VALUE="HL7"
    />
	
		<PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="5" SHOW_IF="false" REPOSITORY_VALUE="FILE_PATH">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.csv"</DEFAULT>
		</PARAMETER>
			
	    <PARAMETER NAME="START_MSG" FIELD="TEXT" NUM_ROW="10" SHOW_IF="false" REPOSITORY_VALUE="START_MSG">
	    	<DEFAULT>'\u000b'</DEFAULT>
	    </PARAMETER>  

	    <PARAMETER NAME="END_MSG" FIELD="TEXT" NUM_ROW="10"  SHOW_IF="false" REPOSITORY_VALUE="END_MSG">
	    	<DEFAULT>'\u001c'</DEFAULT>
	    </PARAMETER>
	    
		<PARAMETER NAME="MESSAGE" FIELD="TABLE" NUM_ROW="25" SHOW_IF="false">
			<ITEMS>
				<ITEM NAME="MSGITEM" FIELD="TEXT" />
			</ITEMS>
     	</PARAMETER>
     
		<PARAMETER NAME="MULTI" FIELD="EXTERNAL" NUM_ROW="30">
            <DEFAULT/>  
        </PARAMETER>
        
		<PARAMETER NAME="SCHEMAS" FIELD="TABLE" NUM_ROW="40" REPOSITORY_VALUE="XML_MAPPING">
              <ITEMS>
                <ITEM NAME="SCHEMA" FIELD="SCHEMA_TYPE"/>
                <ITEM NAME="MAPPING" FIELD="SCHEMA_XPATH_QUERYS"/>
              </ITEMS>
        </PARAMETER>
        
        <PARAMETER NAME="NOVALIDATE" FIELD="CHECK" NUM_ROW="50">
        	<DEFAULT>false</DEFAULT>
        </PARAMETER>

	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="ADVANCED_SEPARATOR" FIELD="CHECK" REQUIRED="true" NUM_ROW="10" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="THOUSANDS_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="10"
			SHOW_IF="(ADVANCED_SEPARATOR == 'true')">
			<DEFAULT>","</DEFAULT>
		</PARAMETER>		
		
		<PARAMETER NAME="DECIMAL_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="10"
			SHOW_IF="(ADVANCED_SEPARATOR == 'true')">
			<DEFAULT>"."</DEFAULT>
		</PARAMETER>	
		
		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="20"
			REQUIRED="true" REPOSITORY_VALUE="ENCODING">
			<DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="commons_cli" MODULE="commons-cli-1.0.jar" REQUIRED="true" />
			<IMPORT NAME="commons_lang" MODULE="commons-lang-1.0.jar" REQUIRED="true" />
			<IMPORT NAME="commons_logging" MODULE="commons-logging-1.0.4.jar" REQUIRED="true" />
			<IMPORT NAME="hapi_base" MODULE="hapi-base-1.0.1.jar" REQUIRED="true" />
			<IMPORT NAME="hapi_structures_V21" MODULE="hapi-structures-v21-1.0.1.jar" REQUIRED="true" />
			<IMPORT NAME="hapi_structures_V22" MODULE="hapi-structures-v22-1.0.1.jar" REQUIRED="true" />
			<IMPORT NAME="hapi_structures_V23" MODULE="hapi-structures-v23-1.0.1.jar" REQUIRED="true" />
			<IMPORT NAME="hapi_structures_V231" MODULE="hapi-structures-v231-1.0.1.jar" REQUIRED="true" />
			<IMPORT NAME="hapi_structures_V24" MODULE="hapi-structures-v24-1.0.1.jar" REQUIRED="true" />
			<IMPORT NAME="hapi_structures_V25" MODULE="hapi-structures-v25-1.0.1.jar" REQUIRED="true" />
			<IMPORT NAME="hapi_structures_V251" MODULE="hapi-structures-v251-1.0.1.jar" REQUIRED="true" />
			<IMPORT NAME="hapi_structures_V26" MODULE="hapi-structures-v26-1.0.1.jar" REQUIRED="true" />
			<IMPORT NAME="jboss_j2ee" MODULE="jboss-j2ee-4.0.2.jar" REQUIRED="true" />
			<IMPORT NAME="jdom" MODULE="jdom-1.1.jar" REQUIRED="true" />
			<IMPORT NAME="log4j" MODULE="log4j-1.2.15.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>