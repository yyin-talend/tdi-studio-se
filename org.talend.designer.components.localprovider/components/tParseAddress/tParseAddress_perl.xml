<COMPONENT>
   <HEADER 
       PLATEFORM="ALL" 
       SERIAL="" 
       VERSION="0.103" 
       STATUS="ALPHA" 
       COMPATIBILITY="WIN32" 
       AUTHOR="Talend" 
       RELEASE_DATE="20050320A" 
       STARTABLE="false"
       DATA_AUTO_PROPAGATE="false"
       HAS_CONDITIONAL_OUTPUTS="true"
   >
      <SIGNATURE></SIGNATURE>
   </HEADER>

  <FAMILIES>
    <FAMILY>Data_Quality</FAMILY>
  </FAMILIES>
   <DOCUMENTATION>
      <URL/>
   </DOCUMENTATION>
   
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MIN_INPUT="1" MAX_INPUT="1" MIN_OUTPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" COLOR="FF0000" BASE_SCHEMA="FLOW" NOT_SHOW_IF="(DATA_ACTION == 'INSERT' OR DIE_ON_ERROR == 'true')"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="1"
    >
      <TABLE READONLY="true">
        <COLUMN NAME="POST_BOX" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="ROAD_BOX" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="SUB_PROPERTY_IDENTIFIER" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="PROPERTY_IDENTIFIER" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="PROPERTY_NAME" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="STREET" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="STREET_TYPE" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="STREET_DIRECTION" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="SUBURB" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="SUBCOUNTRY" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="POST_CODE" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="COUNTRY" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA_REJECT"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="2"
      CONTEXT="REJECT"
      SHOW="true"
    >
      <TABLE READONLY="true">
        <COLUMN NAME="POST_BOX" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="ROAD_BOX" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="SUB_PROPERTY_IDENTIFIER" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="PROPERTY_IDENTIFIER" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="PROPERTY_NAME" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="STREET" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="STREET_TYPE" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="STREET_DIRECTION" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="SUBURB" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="SUBCOUNTRY" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="POST_CODE" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="COUNTRY" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="NON_MATCHING" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>

    <PARAMETER
      NAME="PARSE"
      FIELD="COLUMN_LIST"
      REQUIRED="true"
      NUM_ROW="10"
    >
    </PARAMETER>

    <PARAMETER NAME="COUNTRY" FIELD="CLOSED_LIST"
        REQUIRED="true" NUM_ROW="20">
        <ITEMS DEFAULT="US">
            <ITEM NAME="AU" VALUE="Australia" />
            <ITEM NAME="CA" VALUE="Canada" />
            <ITEM NAME="GB" VALUE="United Kingdom" />
            <ITEM NAME="US" VALUE="United States" />
        </ITEMS>
    </PARAMETER>

 
  </PARAMETERS>

  <ADVANCED_PARAMETERS>

    <PARAMETER
      NAME="CORRECT_CASE"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="40"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="AUTO_CLEAN"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="50"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ABBREVIATE_SUBCOUNTRY"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="60"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ABBREVIATED_SUBCOUNTRY_ONLY"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="70"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>


  </ADVANCED_PARAMETERS>


  <CODEGENERATION/>

   <RETURNS>
      <RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>
