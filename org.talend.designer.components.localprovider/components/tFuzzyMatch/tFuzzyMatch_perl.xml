<COMPONENT>
   <HEADER 
       PLATEFORM="ALL" 
       SERIAL="" 
       VERSION="0.103" 
       STATUS="ALPHA" 
       COMPATIBILITY="WIN32" 
       AUTHOR="Talend" 
       RELEASE_DATE="20050320A" 
       STARTABLE="false"
   >
      <SIGNATURE></SIGNATURE>
   </HEADER>
   <DOCUMENTATION>
      <URL/>
   </DOCUMENTATION>
   
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MIN_INPUT="2" MAX_INPUT="2" MIN_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="1"
    >
      <TABLE READONLY="true">
        <COLUMN NAME="VALUE" TYPE="String" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="MATCHING" TYPE="String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>

    <PARAMETER
      NAME="MATCHING_TYPE"
      FIELD="CLOSED_LIST"
      REQUIRED="true"
      NUM_ROW="2"
    >
      <ITEMS DEFAULT="matchLevenshteinDistance">
        <ITEM NAME="LEVENSHTEIN" VALUE="matchLevenshteinDistance" />
        <ITEM NAME="METAPHONE" VALUE="matchMetaphone" />
        <ITEM NAME="DOUBLEMETAPHONE" VALUE="matchDoubleMetaphone" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="CASE_SENSITIVE"
      FIELD="CHECK"
      NOT_SHOW_IF="(MATCHING_TYPE!='LEVENSHTEIN')"
      REQUIRED="true"
      NUM_ROW="2"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="MIN"
      FIELD="TEXT"
      NOT_SHOW_IF="(MATCHING_TYPE!='LEVENSHTEIN')"
      REQUIRED="true"
      NUM_ROW="9"
    >
        <DEFAULT>0</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="MAX"
      FIELD="TEXT"
      REQUIRED="true"
      NOT_SHOW_IF="(MATCHING_TYPE!='LEVENSHTEIN')"
      NUM_ROW="9"
    >
        <DEFAULT>0</DEFAULT>
    </PARAMETER>
  

    <PARAMETER
      NAME="MATCH"
      FIELD="COLUMN_LIST"
      REQUIRED="true"
      NUM_ROW="11"
    >
    </PARAMETER>

    <PARAMETER
      NAME="GET_UNIQUE"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="12"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ITEMSEPARATOR"
      FIELD="TEXT"
      REQUIRED="false"
      NOT_SHOW_IF="(GET_UNIQUE=='true')"
      NUM_ROW="12"
    >
      <DEFAULT>','</DEFAULT>
    </PARAMETER>
  
  </PARAMETERS>

   <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="Text-LevenshteinXS" MODULE="Text::LevenshteinXS" REQUIRED="true" />
       <IMPORT NAME="Text-Metaphone" MODULE="Text::Metaphone" REQUIRED="true" />
       <IMPORT NAME="Text-DoubleMetaphone" MODULE="Text::DoubleMetaphone" REQUIRED="true" />
     </IMPORTS>
   </CODEGENERATION>

   <RETURNS>
      <RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>
