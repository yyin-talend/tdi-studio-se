<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070111A"
    STARTABLE="false"
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
    IS_MULTIPLYING_OUTPUTS="true"
  >
    <SIGNATURE />
  </HEADER>

  <FAMILIES>
    <FAMILY>Processing/Fields</FAMILY>
    <FAMILY>Data quality/Pattern matching</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="FIELD"
      FIELD="PREV_COLUMN_LIST"
      REQUIRED="true"
      NUM_ROW="10"
    >
    </PARAMETER>

    <PARAMETER
      NAME="REGEX_HELP"
      FIELD="LABEL"
      NUM_ROW="19"
    >
      <DEFAULT>The default regex will match "[Monday] meeting with John [priority: high] [place: office]"
      and "[Friday] diner with kids [priority: highest] [place: home]"
	  </DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="REGEX"
      FIELD="MEMO"
      NUM_ROW="20"
    >
      <DEFAULT>"^\\[(\\w+)\\]([^\\[]+)\\[priority:\\s*(\\w+)\\]\\s*\\[place:\\s*([^\\]]+)\\]"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="30"
    >
      <DEFAULT/>
    </PARAMETER>

  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>

    <PARAMETER
      NAME="DIE_ON_ERROR"
      FIELD="CHECK"
      NUM_ROW="5"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

  </ADVANCED_PARAMETERS>


  <CODEGENERATION/>

  <RETURNS/>

</COMPONENT>