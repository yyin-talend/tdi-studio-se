<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070314A"
    STARTABLE="true"
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
    PARTITIONING="AUTO"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/DB Specifics/Sybase</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:SYBASE"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    />

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="20"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      REQUIRED="true" FILTER="tSybaseConnection"
      NUM_ROW="20"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
      DYNAMIC_SETTINGS="true"
    />
    
    <PARAMETER NAME="DB_VERSION" FIELD="CLOSED_LIST" NUM_ROW="25"
    	REPOSITORY_VALUE="DB_VERSION"
		SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
		<ITEMS DEFAULT="SYBSEIQ_16">
			<ITEM NAME="SYBSEIQ_12_15" VALUE="SYBSEIQ_12_15" />
			<ITEM NAME="SYBSEIQ_16" VALUE="SYBSEIQ_16" />
			<ITEM NAME="SYBSEIQ_16_SA" VALUE="SYBSEIQ_16_SA" />
		</ITEMS>
	</PARAMETER>

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>"2648"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="50"
      REPOSITORY_VALUE="SID"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DB_SCHEMA"
      FIELD="TEXT"
      NUM_ROW="50"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="40"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>SybaseASE</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="60"
      REPOSITORY_VALUE="USERNAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="60"
      REPOSITORY_VALUE="PASSWORD"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="70"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SP_NAME"
      FIELD="TEXT"
      NUM_ROW="80"
      REQUIRED="true"
    >
      <DEFAULT>"myfunction"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="IS_FUNCTION"
      FIELD="CHECK"
      NUM_ROW="90"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="RETURN"
      FIELD="COLUMN_LIST"
      NUM_ROW="90"
      SHOW_IF="IS_FUNCTION == 'true'"
    >
    </PARAMETER>

    <PARAMETER
      NAME="TIMEOUT_INTERVAL"
      FIELD="TEXT"
      NUM_ROW="90"
    >
      <DEFAULT>600</DEFAULT>
    </PARAMETER>

    <PARAMETER
   	  NAME="NOTE"
   	  FIELD="LABEL"
   	  NUM_ROW="90"
    >
   	  <DEFAULT>second(s)</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="SP_ARGS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="100"
      NB_LINES="3"
    >
      <ITEMS>
        <ITEM NAME="COLUMN" FIELD="COLUMN_LIST"/>

        <ITEM NAME="TYPE" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="IN">
            <ITEM NAME="IN" VALUE="IN"/>
            <ITEM NAME="OUT" VALUE="OUT"/>
            <ITEM NAME="RECORDSET" VALUE="RECORDSET"/>
          </ITEMS>
        </ITEM>
      </ITEMS>
    </PARAMETER>
  </PARAMETERS>

  <ADVANCED_PARAMETERS>

  	<PARAMETER
		NAME="PROPERTIES"
		FIELD="TEXT"
		NUM_ROW="3"
		REPOSITORY_VALUE="PROPERTIES_STRING"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

  	<PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="7"
      REQUIRED="true"
      SHOW="false"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="CONTAIN_MULTI_SELECT"
      FIELD="CHECK"
      NUM_ROW="10"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
       <IMPORT NAME="Driver-Sybase3" MODULE="jconn3.jar" MVN="mvn:org.talend.libraries/jconn3/6.0.0"  BundleID="" REQUIRED_IF = "(DB_VERSION == 'SYBSEIQ_12_15') AND (USE_EXISTING_CONNECTION == 'false')" />
       <IMPORT NAME="Driver-Sybase4" MODULE="jconn4.jar" MVN="mvn:org.talend.libraries/jconn4/6.0.0"  BundleID="" REQUIRED_IF = "(DB_VERSION == 'SYBSEIQ_16') AND (USE_EXISTING_CONNECTION == 'false')" />
       <IMPORT NAME="Driver-SqlAnywhere" MODULE="sajdbc4.jar" MVN="mvn:sap.jdbc4.sqlanywhere/sajdbc4/17.0.0"  BundleID="" REQUIRED_IF = "(DB_VERSION == 'SYBSEIQ_16_SA') AND (USE_EXISTING_CONNECTION == 'false')" />
    </IMPORTS>
  </CODEGENERATION>

  <RETURNS/>
</COMPONENT>
