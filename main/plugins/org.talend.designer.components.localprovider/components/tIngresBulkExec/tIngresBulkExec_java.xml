<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20100308A"
    STARTABLE="true"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/DB Specifics/Ingres</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="RUN_IF" MAX_OUTPUT="1"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:INGRES"
    />


    <PARAMETER
      NAME="VNODE"
      FIELD="TEXT"
      NUM_ROW="30"
      REQUIRED="false"
      REPOSITORY_VALUE="VNODE_NAME"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>


    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="30"
      REQUIRED="false"
      REPOSITORY_VALUE="SID"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    

	<PARAMETER 
		NAME="TABLE" 
		FIELD="" 
		NUM_ROW="6" 
		REQUIRED="true">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

    <PARAMETER
      NAME="TABLE_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="60">
      <ITEMS DEFAULT="NONE">
        <ITEM NAME="NONE" VALUE="NONE" />
        <ITEM NAME="TRUNCATE" VALUE="TRUNCATE_BY_INGRESNET"/>
      </ITEMS>
    </PARAMETER>
	
	<PARAMETER NAME="NOTE" FIELD="LABEL" NUM_ROW="60"
			NOT_SHOW_IF="(TABLE_ACTION == 'NONE')"	
		>
		<DEFAULT>Note : Truncate Table statement will be rolled back if bulk insert fails</DEFAULT>
	</PARAMETER>	

    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="70"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/ingres_bulk.csv"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="80">
      <DEFAULT/> 
    </PARAMETER>
    
    <PARAMETER
	  NAME="DELETE_WORKING_FILES"
	  FIELD="CHECK"
	  NUM_ROW="90">
		<DEFAULT>true</DEFAULT>
	</PARAMETER> 

	<PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="90"
      SHOW="false"
    >       
      <DEFAULT>ingres_id</DEFAULT>
    </PARAMETER>

   </PARAMETERS>
   
   <ADVANCED_PARAMETERS>
       <PARAMETER
           NAME="DELIM_ACTION"
           FIELD="TEXT"
           NUM_ROW="1"
           MAX_LENGTH="1"
           REQUIRED="true"    >
           <DEFAULT>","</DEFAULT>
       </PARAMETER> 
       
       <PARAMETER
           NAME="NEWLINE_ACTION"
           FIELD="TEXT"
           NUM_ROW="1"
           REQUIRED="true"    >
           <DEFAULT>"\n"</DEFAULT>             
       </PARAMETER>

	<PARAMETER 
	       NAME="ONERROR_ACTION" 
	       FIELD="CLOSED_LIST" 
	       NUM_ROW="5">
		<ITEMS DEFAULT="TERMINATE">
			<ITEM NAME="CONTINUE"  VALUE="CONTINUE" />
			<ITEM NAME="TERMINATE" VALUE="TERMINATE" />
		</ITEMS>
	</PARAMETER>
	
	<PARAMETER NAME="ERROR_COUNT" FIELD="TEXT" NUM_ROW="6"
		REQUIRED="false" NOT_SHOW_IF="ONERROR_ACTION == 'CONTINUE')">
		<DEFAULT>"1"</DEFAULT>				
	</PARAMETER>

	<PARAMETER NAME="REJECT_ROW_FILE" FIELD="FILE" NUM_ROW="7"
		REQUIRED="false" NOT_SHOW_IF="ONERROR_ACTION == 'TERMINATE')">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>	
	
	<PARAMETER NAME="ROLLBACK_ACTION" FIELD="CLOSED_LIST" NUM_ROW="3">
		<ITEMS DEFAULT="ENABLED">
			<ITEM NAME="ENABLED" VALUE="ENABLED" />
			<ITEM NAME="DISABLED" VALUE="DISABLED" />
		</ITEMS>
	</PARAMETER>

	<PARAMETER NAME="NULL_INDICATOR" FIELD="TEXT" NUM_ROW="2"
		REQUIRED="false">
		<DEFAULT>"[NULL]"</DEFAULT>
	</PARAMETER>   
	
	<PARAMETER NAME="SESSION_USER" FIELD="TEXT" NUM_ROW="2" REQUIRED="false">
		<DEFAULT>"CURRENT_USER"</DEFAULT>
	</PARAMETER>
	
    <PARAMETER NAME="OUTPUT" FIELD="CLOSED_LIST" NUM_ROW="51">
       <ITEMS DEFAULT="OUTPUT_TO_CONSOLE">
          <ITEM NAME="OUTPUT_TO_CONSOLE" VALUE="OUTPUT_TO_CONSOLE" />
          <ITEM NAME="RETRIEVE_OUTPUT"   VALUE="RETRIEVE_OUTPUT" />
       </ITEMS>
    </PARAMETER>

	<PARAMETER NAME="ALLOCATION" FIELD="TEXT" NUM_ROW="20"
		REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="EXTEND" FIELD="TEXT" NUM_ROW="21" REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="FILLFACTOR" FIELD="TEXT" NUM_ROW="22" REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER> 
	
	<PARAMETER NAME="MINPAGES" FIELD="TEXT" NUM_ROW="23" REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER> 	
	
	<PARAMETER NAME="MAXPAGES" FIELD="TEXT" NUM_ROW="24" REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>	
	
	<PARAMETER NAME="LEAFFILL" FIELD="TEXT" NUM_ROW="25" REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>	
    
	<PARAMETER NAME="NONLEAFFILL" FIELD="TEXT" NUM_ROW="26" REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="ROW_ESTIMATE" FIELD="TEXT" NUM_ROW="27" REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="TRIM_SPACE" FIELD="CHECK" NUM_ROW="28">
		<DEFAULT>true</DEFAULT>
	</PARAMETER>	
   </ADVANCED_PARAMETERS>

   <CODEGENERATION></CODEGENERATION>
   
   <RETURNS>
      <RETURN NAME="NB_LINE_DATA" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="LINE_BAD" TYPE="id_Integer" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>
