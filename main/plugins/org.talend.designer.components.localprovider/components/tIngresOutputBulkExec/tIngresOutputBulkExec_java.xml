<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.1"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20100308A"
    STARTABLE="false"
	PARTITIONING="NONE"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/DB Specifics/Ingres</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

   <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:INGRES"
    />
       
   
    <PARAMETER
      NAME="VNODE"
      FIELD="TEXT"
      NUM_ROW="30"
      REQUIRED="false"
      REPOSITORY_VALUE="VNODE">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>


    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="30"
      REQUIRED="false"
      REPOSITORY_VALUE="SID"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
          
    <PARAMETER
      NAME="TABLE"
      FIELD=""
      NUM_ROW="6"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="60"
    >
      <ITEMS DEFAULT="NONE">
        <ITEM NAME="NONE" VALUE="NONE" />
        <ITEM NAME="TRUNCATE" VALUE="TRUNCATE_BY_INGRESNET"/>
      </ITEMS>
    </PARAMETER>
	
	<PARAMETER NAME="NOTE" FIELD="LABEL" NUM_ROW="60"
			NOT_SHOW_IF="(TABLE_ACTION == 'NONE')"	
		>
		<DEFAULT>Note : Truncate Table statement will be rolled back if bulk insert fails</DEFAULT>
	</PARAMETER>	

    <PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="70" REQUIRED="true" REPOSITORY_VALUE="FILE_PATH">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/ingres_bulk.csv"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="80">
      <DEFAULT/> 
    </PARAMETER>
    
    <PARAMETER
	  NAME="DELETE_WORKING_FILES"
	  FIELD="CHECK"
	  NUM_ROW="85">
		<DEFAULT>true</DEFAULT>
	</PARAMETER> 

	<PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="90"
      SHOW="false"
    >       
      <DEFAULT>ingres_id</DEFAULT>
    </PARAMETER>

   </PARAMETERS>
   
   <ADVANCED_PARAMETERS>
   
       <PARAMETER NAME="DELIM_ACTION" FIELD="TEXT" NUM_ROW="1"
       	REQUIRED="true" MAX_LENGTH="1" REPOSITORY_VALUE="DELIM_ACTION">
       	<DEFAULT>","</DEFAULT>
       </PARAMETER>

       <PARAMETER NAME="NEWLINE_ACTION" FIELD="TEXT" NUM_ROW="1"
       	REQUIRED="true" REPOSITORY_VALUE="NEWLINE_ACTION">
       	<DEFAULT>"\n"</DEFAULT>
       </PARAMETER>

       <PARAMETER NAME="ONERROR_ACTION" FIELD="CLOSED_LIST" NUM_ROW="2">
		<ITEMS DEFAULT="CONTINUE">
			<ITEM NAME="CONTINUE" VALUE="CONTINUE" />
			<ITEM NAME="TERMINATE" VALUE="TERMINATE" />
		</ITEMS>
	</PARAMETER>
	
	<PARAMETER NAME="ERROR_COUNT" FIELD="TEXT" NUM_ROW="2"
		REQUIRED="false" NOT_SHOW_IF="ONERROR_ACTION == 'CONTINUE')">
		<DEFAULT>"1"</DEFAULT>				
	</PARAMETER>

	<PARAMETER NAME="REJECT_ROW_FILE" FIELD="FILE" NUM_ROW="3"
		REQUIRED="false" NOT_SHOW_IF="ONERROR_ACTION == 'TERMINATE')">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>	
	
	<PARAMETER NAME="ROLLBACK_ACTION" FIELD="CLOSED_LIST" NUM_ROW="4">
		<ITEMS DEFAULT="ENABLED">
			<ITEM NAME="ENABLED" VALUE="ENABLED" />
			<ITEM NAME="DISABLED" VALUE="DISABLED" />
		</ITEMS>
	</PARAMETER>

	<PARAMETER NAME="NULL_INDICATOR" FIELD="TEXT" NUM_ROW="5"
		REQUIRED="false">
		<DEFAULT>"[NULL]"</DEFAULT>
	</PARAMETER>   
    
	<PARAMETER NAME="SESSION_USER" FIELD="TEXT" NUM_ROW="5" REQUIRED="false">
		<DEFAULT>"CURRENT_USER"</DEFAULT>
	</PARAMETER>
    
    <PARAMETER NAME="ENCODING"  FIELD="ENCODING_TYPE"  NUM_ROW="50" REQUIRED="true" >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="OUTPUT" FIELD="CLOSED_LIST" NUM_ROW="51">
       <ITEMS DEFAULT="OUTPUT_TO_CONSOLE">
          <ITEM NAME="OUTPUT_TO_CONSOLE" VALUE="OUTPUT_TO_CONSOLE" />
          <ITEM NAME="RETRIEVE_OUTPUT"   VALUE="RETRIEVE_OUTPUT" />
       </ITEMS>
    </PARAMETER>

	<PARAMETER NAME="ALLOCATION" FIELD="TEXT" NUM_ROW="20"
		REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="EXTEND" FIELD="TEXT" NUM_ROW="21" REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="FILLFACTOR" FIELD="TEXT" NUM_ROW="22" REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER> 
	
	<PARAMETER NAME="MINPAGES" FIELD="TEXT" NUM_ROW="23" REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER> 	
	
	<PARAMETER NAME="MAXPAGES" FIELD="TEXT" NUM_ROW="24" REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>	
	
	<PARAMETER NAME="LEAFFILL" FIELD="TEXT" NUM_ROW="25" REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>	
    
	<PARAMETER NAME="NONLEAFFILL" FIELD="TEXT" NUM_ROW="26" REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="ROW_ESTIMATE" FIELD="TEXT" NUM_ROW="27" REQUIRED="false">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="TRIM_SPACE" FIELD="CHECK" NUM_ROW="28">
		<DEFAULT>true</DEFAULT>
	</PARAMETER>		    
   </ADVANCED_PARAMETERS>


  <CODEGENERATION>
    <TEMPLATES INPUT="tIOB" OUTPUT="tIBE">

      <TEMPLATE NAME="tIOB" COMPONENT="tIngresOutputBulk">
        <LINK_TO NAME="tIBE" CTYPE="ROWS_END"/>
      </TEMPLATE>
      
      <TEMPLATE NAME="tIBE" COMPONENT="tIngresBulkExec"/>
	  <TEMPLATE_PARAM SOURCE="self.VNODE" TARGET="tIBE.VNODE"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.DBNAME" TARGET="tIBE.DBNAME"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.TABLE" TARGET="tIBE.TABLE"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.TABLE_ACTION" TARGET="tIBE.TABLE_ACTION"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.NOTE" TARGET="tIBE.NOTE"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.SESSION_USER" TARGET="tIBE.SESSION_USER"></TEMPLATE_PARAM>
	  
	  <TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tIOB.FILENAME" />
      <TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tIBE.FILENAME" />
      
	  <TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="tIOB.SCHEMA"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.DELETE_WORKING_FILES" TARGET="tIBE.DELETE_WORKING_FILES"></TEMPLATE_PARAM>  
	  <TEMPLATE_PARAM SOURCE="self.MAPPING" TARGET="tIBE.MAPPING"></TEMPLATE_PARAM>
	  
	  <TEMPLATE_PARAM SOURCE="self.DELIM_ACTION" TARGET="tIBE.DELIM_ACTION"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.DELIM_ACTION" TARGET="tIOB.DELIM_ACTION"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.NEWLINE_ACTION" TARGET="tIBE.NEWLINE_ACTION"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.NEWLINE_ACTION" TARGET="tIOB.NEWLINE_ACTION"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.ONERROR_ACTION" TARGET="tIBE.ONERROR_ACTION"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.ERROR_COUNT" TARGET="tIBE.ERROR_COUNT"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.REJECT_ROW_FILE" TARGET="tIBE.REJECT_ROW_FILE"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.ROLLBACK_ACTION" TARGET="tIBE.ROLLBACK_ACTION"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.NULL_INDICATOR" TARGET="tIBE.NULL_INDICATOR"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.ENCODING" TARGET="tIOB.ENCODING"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.OUTPUT" TARGET="tIBE.OUTPUT"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.ALLOCATION" TARGET="tIBE.ALLOCATION"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.EXTEND" TARGET="tIBE.EXTEND"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.FILLFACTOR" TARGET="tIBE.FILLFACTOR"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.MINPAGES" TARGET="tIBE.MINPAGES"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.MAXPAGES" TARGET="tIBE.MAXPAGES"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.LEAFFILL" TARGET="tIOB.LEAFFILL"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.NONLEAFFILL" TARGET="tIBE.NONLEAFFILL"></TEMPLATE_PARAM>
	  <TEMPLATE_PARAM SOURCE="self.ROW_ESTIMATE" TARGET="tIBE.ROW_ESTIMATE"></TEMPLATE_PARAM>      
	  <TEMPLATE_PARAM SOURCE="self.TRIM_SPACE" TARGET="tIBE.TRIM_SPACE"></TEMPLATE_PARAM>
    </TEMPLATES>
  </CODEGENERATION>
  <RETURNS/>
</COMPONENT>



