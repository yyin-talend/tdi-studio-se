<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    SCHEMA_AUTO_PROPAGATE="true"
    DATA_AUTO_PROPAGATE="false"
    HAS_CONDITIONAL_OUTPUTS="true"
    TECHNICAL="true"
    PARTITIONING="UNIQUE_KEY"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE />
  </HEADER>

  <FAMILIES>
    <FAMILY>Technical</FAMILY>
  </FAMILIES>
  
  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
	<CONNECTOR CTYPE="FLOW" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR
      NAME="UNIQUE"
      CTYPE="FLOW"
      MAX_OUTPUT="1"
      COLOR="086438"
      BASE_SCHEMA="FLOW"
    />
    <CONNECTOR
      NAME="DUPLICATE"
      CTYPE="FLOW"
      MAX_OUTPUT="1"
      LINE_STYLE="2"
      COLOR="f36300"
      BASE_SCHEMA="FLOW"
    />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1"/>

    <PARAMETER
      NAME="UNIQUE_KEY"
      FIELD="TABLE"
      REQUIRED="true"
      NUM_ROW="2"
      NB_LINES="5"
    >
      <ITEMS BASED_ON_SCHEMA="true">
		<ITEM NAME="KEY_ATTRIBUTE" FIELD="CHECK" VALUE="false"/>
		<ITEM NAME="CASE_SENSITIVE" FIELD="CHECK" VALUE="false"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER NAME="CID" FIELD="TEXT" REQUIRED="true" NUM_ROW="1" SHOW="false" >
    	<DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TEMP_DIRECTORY"
      FIELD="DIRECTORY"
      NUM_ROW="15"
    >
      <DEFAULT/>
    </PARAMETER>

	<PARAMETER
      NAME="BUFFER_SIZE"
      FIELD="OPENED_LIST"
      RAW="true"
      NUM_ROW="18"
    >
      <ITEMS DEFAULT="M">
        <ITEM NAME="SMALL" VALUE="S" />
        <ITEM NAME="MEDIUM" VALUE="M"/>
        <ITEM NAME="BIG" VALUE="B"/>
      </ITEMS>
    </PARAMETER>

  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
  
    <PARAMETER
      NAME="IGNORE_TRAILING_ZEROS_FOR_BIGDECIMAL"
      FIELD="CHECK"
      NUM_ROW="3"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
  </ADVANCED_PARAMETERS>

  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="NB_UNIQUES" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_DUPLICATES" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>  
</COMPONENT>
