<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20120406A"
		STARTABLE="false" HAS_CONDITIONAL_OUTPUTS="true" LOG4J_ENABLED="true"
		SCHEMA_AUTO_PROPAGATE="false" DATA_AUTO_PROPAGATE="false" PARTITIONING="AUTO">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Processing/Fields (Deprecated)</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"
			MIN_OUTPUT="1" />
		<CONNECTOR NAME="REJECT" NOT_SHOW_IF="(DIE_ON_ERROR == 'true')" CTYPE="FLOW" MAX_INPUT="0"
			MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="1">
			<DEFAULT />
		</PARAMETER>

		<PARAMETER NAME="FIELD" FIELD="PREV_COLUMN_LIST" REQUIRED="true"
			NUM_ROW="10">
		</PARAMETER>

		<PARAMETER NAME="MAPPING" FIELD="MAPPING_TYPE" NUM_ROW="120"
			SHOW="false">
			<DEFAULT>ebcdic_id</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="45">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA_REJECT" FIELD="SCHEMA_TYPE"
			REQUIRED="true" NUM_ROW="60" CONTEXT="REJECT" SHOW="true">
			<TABLE READONLY="true">
				<COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255"
					READONLY="false" CUSTOM="true" />
				<COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255"
					READONLY="false" CUSTOM="true" />
			</TABLE>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>

		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="25"
			REQUIRED="true">
			<DEFAULT>"Cp037"</DEFAULT>
		</PARAMETER>

 	<PARAMETER NAME="TRIMALL" FIELD="CHECK" NUM_ROW="30" >
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="TRIMSELECT" FIELD="TABLE" NUM_ROW="35"
		NB_LINES="5" SHOW_IF="TRIMALL=='false'">
		<ITEMS BASED_ON_SCHEMA="true">
			<ITEM NAME="TRIM" FIELD="CHECK"></ITEM>
		</ITEMS>
	</PARAMETER>

	<PARAMETER NAME="SCHEMA_OPT_NUM" FIELD="TEXT" NUM_ROW="60"
            REQUIRED="true" SHOW="false">
            <DEFAULT>100</DEFAULT>
    </PARAMETER>

	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="cobolX" MODULE="cobolX.jar" MVN="mvn:org.talend.libraries/cobolX/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.ebcdic/lib/cobolX.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>

</COMPONENT>
