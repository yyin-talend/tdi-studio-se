                    		client_<%=cid%>.insertEntity(entity_<%=cid%>);
                    	<%
                    	}else if("update".equals(action)){
                    		if(idColumnName!=null){
                    		%>
                 				client_<%=cid%>.updateEntity( entity_<%=cid%>, <%=connName%>.<%=idColumnName%>);
