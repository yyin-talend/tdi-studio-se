<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20090320A"
		TECHNICAL="true"
		STARTABLE="true">
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>Talend MDM</FAMILY>
  </FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

	   <PARAMETER
	      NAME="SCHEMA"
	      FIELD="SCHEMA_TYPE"
	      REQUIRED="true"
	      NUM_ROW="1"
	    >
	    </PARAMETER>

	    <PARAMETER
	    	NAME="DESTINATION"
	    	FIELD="TEXT"
	    	RAW="true"
	    	NUM_ROW="2"
	    	SHOW="false"
	    	>
	    	<DEFAULT>tMDMInputIn_1</DEFAULT>
	    </PARAMETER>

	    <PARAMETER
	      NAME="XMLFIELD"
	      FIELD="COLUMN_LIST"
	      REQUIRED="true"
	      NUM_ROW="5"
	    >
	    </PARAMETER>
	    
	    <PARAMETER NAME="SERVERVERSION" FIELD="CLOSED_LIST" NUM_ROW="9" REQUIRED="true">
            <ITEMS DEFAULT="SERVER_6_0">
                <ITEM NAME="SERVER_6_0" VALUE="s60" />
                <ITEM NAME="SERVER_5_6" VALUE="s56" />
            </ITEMS>
        </PARAMETER>

		<PARAMETER
	      NAME="USE_EXISTING_CONNECTION"
	      FIELD="CHECK"
	      NUM_ROW="10"
	   >
	      <DEFAULT>false</DEFAULT>
	   </PARAMETER>
	   <PARAMETER
	      NAME="CONNECTION"
	      FIELD="COMPONENT_LIST"
	      REQUIRED="true" FILTER="tMDMConnection"
	      NUM_ROW="15"
	      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
	    />

		<PARAMETER NAME="MDMURL" FIELD="TEXT" NUM_ROW="20"
			REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>"http://localhost:8080/talendmdm/services/soap"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="30" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>"userName"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASSWORD" FIELD="PASSWORD" REQUIRED="true"
			NUM_ROW="30" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>"password"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="UNIVERSE" FIELD="TEXT"
			NUM_ROW="30" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT></DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONCEPT" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="40">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

        <PARAMETER NAME="DATACLUSTER" FIELD="TEXT" REQUIRED="true"
            NUM_ROW="45">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="CONTAINER_TYPE" FIELD="CLOSED_LIST" NUM_ROW="45"
            REQUIRED="true">
            <ITEMS DEFAULT="MASTER">
                <ITEM NAME="MASTER" VALUE="MASTER" />
                <ITEM NAME="STAGING" VALUE="STAGING" />
            </ITEMS>
        </PARAMETER>    

	    <PARAMETER NAME="USE_ITEMS" FIELD="CHECK" REQUIRED="true" NUM_ROW="50">
	    	<DEFAULT>true</DEFAULT>
	    </PARAMETER>

	  	<PARAMETER NAME="NOTE" FIELD="LABEL" NUM_ROW="59" SHOW_IF="USE_ITEMS=='false'">
	    	<DEFAULT>Add all the key values below for the item</DEFAULT>
	    </PARAMETER>

	    <PARAMETER NAME="IDS" FIELD="TABLE" NUM_ROW="60" SHOW_IF="USE_ITEMS=='false'">
	    	<ITEMS>
	    		<ITEM NAME="IDVALUE" FIELD="TEXT" />
	    	</ITEMS>
	    </PARAMETER>

		<PARAMETER NAME="OPERATIONS" FIELD="TABLE" NUM_ROW="60" SHOW_IF="USE_ITEMS=='true'">
	      <ITEMS>
	        <ITEM NAME="XPATH" FIELD="TEXT" />

	        <ITEM NAME="FUNCTION" FIELD="CLOSED_LIST">
	          <ITEMS DEFAULT="CONTAINS">
	            <ITEM NAME="CONTAINS"  VALUE="CONTAINS"/>
	            <ITEM NAME="JOIN" VALUE="JOIN" />
	            <ITEM NAME="STARTSWITH"    VALUE="STARTSWITH"/>
	            <ITEM NAME="STRICTCONTAINS"    VALUE="STRICTCONTAINS"/>
	            <ITEM NAME="EQUAL"    VALUE="EQUALS"/>
	            <ITEM NAME="NOTEQUAL"    VALUE="NOT_EQUALS"/>
	            <ITEM NAME="GREATER"    VALUE="GREATER_THAN"/>
	            <ITEM NAME="GREATEROREQUAL"    VALUE="GREATER_THAN_OR_EQUAL"/>
	            <ITEM NAME="LOWER"    VALUE="LOWER_THAN"/>
	            <ITEM NAME="LOWEROREQUAL"    VALUE="LOWER_THAN_OR_EQUAL"/>
                <ITEM NAME="FULLTEXTSEARCH" VALUE="FULLTEXTSEARCH" />
                <ITEM NAME="EMPTYNULL" VALUE="EMPTY_NULL"/>
	          </ITEMS>
	        </ITEM>

	        <ITEM NAME="VALUE" FIELD="TEXT" />

	        <ITEM NAME="PREDICATE" FIELD="CLOSED_LIST">
	        	<ITEMS DEFAULT="NONE">
	        		<ITEM NAME="NONE" VALUE="NONE" />
	        		<ITEM NAME="OR" VALUE="OR" />
	        		<ITEM NAME="AND" VALUE="AND" />
	        		<ITEM NAME="STRICTAND" VALUE="STRICTAND" />
	        		<ITEM NAME="EXACTLY" VALUE="EXACTLY" />
	        		<ITEM NAME="NOT" VALUE="NOT" />
	        	</ITEMS>
	        </ITEM>

	      </ITEMS>
		</PARAMETER>

		<PARAMETER NAME="SKIP" FIELD="TEXT" NUM_ROW="70" SHOW_IF="USE_ITEMS=='true'" >
			<DEFAULT>0</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MAXVALUE" FIELD="TEXT" NUM_ROW="70" SHOW_IF="USE_ITEMS=='true'" >
			<DEFAULT>0</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="80" SHOW="false">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="SPELLTHREHOLD" FIELD="TEXT" NUM_ROW="10" SHOW_IF="USE_ITEMS=='true'">
			<DEFAULT>0</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FETCHSIZE" FIELD="TEXT" NUM_ROW="20" SHOW_IF="USE_ITEMS=='true'" >
			<DEFAULT>50</DEFAULT>
		</PARAMETER>

	</ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="MDM-WEBSERVICE-TOMCAT" MODULE="mdm_webservice_tomcat.jar" UrlPath="platform:/plugin/org.talend.libraries.mdm/lib/mdm_webservice_tomcat.jar" REQUIRED_IF="((SERVERVERSION == 's60') and (USE_EXISTING_CONNECTION == 'false'))" />
      <IMPORT NAME="MDM-Webservice" MODULE="mdm_webservice.jar" UrlPath="platform:/plugin/org.talend.libraries.mdm/lib/mdm_webservice.jar" REQUIRED_IF="((SERVERVERSION == 's56') and (USE_EXISTING_CONNECTION == 'false'))" />
      <IMPORT NAME="SOAP-axis" MODULE="axis.jar" UrlPath="platform:/plugin/org.talend.libraries.apache.axis2/lib/axis.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-jaxrpc" MODULE="jaxrpc.jar" UrlPath="platform:/plugin/org.talend.libraries.jax/lib/jaxrpc.jar" BundleID="" REQUIRED="true" />
      <IMPORT NAME="SOAP-jaxrpc-api" MODULE="jaxrpc-api.jar" UrlPath="platform:/plugin/org.talend.libraries.jax/lib/jaxrpc-api.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-saaj" MODULE="saaj.jar" UrlPath="platform:/plugin/org.talend.libraries.saaj/lib/saaj.jar" BundleID="" REQUIRED="true" />
      <IMPORT NAME="WSDL4J-1_6_3" MODULE="wsdl4j-1.6.3.jar" BundleID="org.apache.servicemix.bundles.wsdl4j" REQUIRED="true" />
      <IMPORT NAME="SOAP-discovery" MODULE="commons-discovery-0.2.jar" UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-discovery-0.2.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-logging" MODULE="commons-logging-1.1.jar" UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-logging-1.1.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-JavaMail" MODULE="mail.jar" UrlPath="platform:/plugin/org.talend.libraries.apache.axis2/lib/mail.jar" BundleID="" REQUIRED="true" />
      <IMPORT NAME="SOAP-JAF" MODULE="activation.jar" UrlPath="platform:/plugin/org.talend.libraries.apache.axis2/lib/activation.jar" BundleID="" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>
