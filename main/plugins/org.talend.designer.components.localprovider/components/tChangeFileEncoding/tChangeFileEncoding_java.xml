<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
       SERIAL=""
      VERSION="0.101"
       STATUS="ALPHA"
COMPATIBILITY="ALL"
       AUTHOR="Talend"
 RELEASE_DATE="200901220A"
    STARTABLE="true"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>File/Management</FAMILY>
    <FAMILY>Data_Quality</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
	
	<PARAMETER NAME="USE_INENCODING" FIELD="CHECK" REQUIRED="true"
		NUM_ROW="20">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="INENCODING" FIELD="ENCODING_TYPE" NUM_ROW="20"
		 SHOW_IF="USE_INENCODING=='true'">
		<DEFAULT>"ISO-8859-15"</DEFAULT>
	</PARAMETER>
	
    <PARAMETER NAME="INFILE_NAME" FIELD="FILE" NUM_ROW="30" REQUIRED="true" >
	  <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.csv"</DEFAULT>
	</PARAMETER>
	
    <PARAMETER NAME="OUTFILE_NAME" FIELD="FILE" NUM_ROW="40" REQUIRED="true" >
	  <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.csv"</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="50"
		REQUIRED="true">
		<DEFAULT>"ISO-8859-15"</DEFAULT>
	</PARAMETER>
    <PARAMETER NAME="BUFFERSIZE" FIELD="TEXT" REQUIRED="true"
        NUM_ROW="50">
        <DEFAULT>8192</DEFAULT>
    </PARAMETER>
  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
 	<PARAMETER NAME="CREATE" FIELD="CHECK" REQUIRED="true"
		NUM_ROW="10">
		<DEFAULT>true</DEFAULT>
	</PARAMETER>
  </ADVANCED_PARAMETERS>
  

  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="EXISTS" TYPE="id_Boolean" AVAILABILITY="FLOW"/>
    <RETURN NAME="FILENAME" TYPE="id_String" AVAILABILITY="AFTER"/>
  </RETURNS>

</COMPONENT>
