<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT> 
  <HEADER AUTHOR="Talend" COMPATIBILITY="ALL" 
  		DATA_AUTO_PROPAGATE="false" PLATEFORM="ALL" 
  		RELEASE_DATE="20080229A" SERIAL=""  LOG4J_ENABLED="true"
  		STARTABLE="true" STATUS="ALPHA" VERSION="0.102"> 
    <SIGNATURE/> 
  </HEADER>  
  <FAMILIES> 
    <FAMILY>Talend MDM</FAMILY>
	<FAMILY>Data_Quality</FAMILY>
  </FAMILIES>  
  <DOCUMENTATION> 
    <URL/> 
  </DOCUMENTATION>  
  <CONNECTORS> 
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/> 
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
	<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
	<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
	<CONNECTOR CTYPE="COMPONENT_OK" />
	<CONNECTOR CTYPE="COMPONENT_ERROR" />
	<CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>  
  <PARAMETERS> 
   	<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" SHOW="true" REQUIRED="true" NUM_ROW="1">
		<TABLE READONLY="false" IF="TARGET_RECORD_ONLY=='true'">
	        <COLUMN NAME="TASK_ID" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true" />
	        <COLUMN NAME="TASK_TYPE" TYPE="id_String" READONLY="true" LENGTH="20" CUSTOM="true" />
	        <COLUMN NAME="TASK_CREATEDBY" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true" />
	        <COLUMN NAME="TASK_CREATEDON" TYPE="id_Date" PATTERN="&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss&apos;.000Z&apos;&quot;" READONLY="true" LENGTH="20" CUSTOM="true" />
	        <COLUMN NAME="TASK_OWNER" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true" />
	        <COLUMN NAME="TASK_STATUS" TYPE="id_String" READONLY="true" LENGTH="20" CUSTOM="true" />
	        <COLUMN NAME="TASK_STARS" TYPE="id_Integer" READONLY="true" LENGTH="20" CUSTOM="true" />
	    </TABLE>
	    <TABLE READONLY="false" IF="TARGET_RECORD_ONLY=='false'">
	        <COLUMN NAME="TASK_ID" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true" />
	        <COLUMN NAME="TASK_TYPE" TYPE="id_String" READONLY="true" LENGTH="20" CUSTOM="true" />
	        <COLUMN NAME="TASK_CREATEDBY" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true" />
	        <COLUMN NAME="TASK_CREATEDON" TYPE="id_Date" PATTERN="&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss&apos;.000Z&apos;&quot;" READONLY="true" LENGTH="20" CUSTOM="true" />
	        <COLUMN NAME="TASK_OWNER" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true" />
	        <COLUMN NAME="TASK_STATUS" TYPE="id_String" READONLY="true" LENGTH="20" CUSTOM="true" />
	        <COLUMN NAME="TASK_STARS" TYPE="id_Integer" READONLY="true" LENGTH="20" CUSTOM="true" />
	        <COLUMN NAME="TARGET" TYPE="id_Boolean" READONLY="true" CUSTOM="true" />
	        <COLUMN NAME="SOURCE" TYPE="id_String" READONLY="true" LENGTH="255" CUSTOM="true" />
	    </TABLE>
	</PARAMETER>
	
	<PARAMETER NAME="SERVERVERSION" FIELD="CLOSED_LIST" NUM_ROW="2" REQUIRED="true">
        <ITEMS DEFAULT="SERVER_6_0">
            <ITEM NAME="SERVER_6_0" VALUE="s60" />
            <ITEM NAME="SERVER_5_6" VALUE="s56" />
        </ITEMS>
    </PARAMETER>
	
	<PARAMETER NAME="HOST" FIELD="TEXT" NUM_ROW="3" REQUIRED="true">
    	<DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>  
    <PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="3" REQUIRED="true">
    	<DEFAULT>8080</DEFAULT>
    </PARAMETER>  
   	
    <PARAMETER NAME="USERNAME" FIELD="TEXT" NUM_ROW="5">
    	<DEFAULT>"username"</DEFAULT>
    </PARAMETER>  
    <PARAMETER NAME="PASSWORD" FIELD="PASSWORD" NUM_ROW="5">
    	<DEFAULT>"password"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="TASKTYPE" FIELD="CLOSED_LIST" NUM_ROW="10">
	    <ITEMS DEFAULT="RESOLUTION">
			<ITEM NAME="RESOLUTION" VALUE="1" />
			<ITEM NAME="DATA" VALUE="2" />
		</ITEMS>  
	</PARAMETER>
	<PARAMETER NAME="OWNER" FIELD="TEXT" NUM_ROW="10">
    	<DEFAULT>"owner"</DEFAULT>
    </PARAMETER>  
    <PARAMETER NAME="TAG" FIELD="TEXT" NUM_ROW="10">
    	<DEFAULT>"tag"</DEFAULT>
    </PARAMETER> 
    <PARAMETER NAME="START_DATETIME" FIELD="TEXT" NUM_ROW="20">
    	 <DEFAULT>"yyyy-MM-dd HH:mm:ss"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="END_DATETIME" FIELD="TEXT" NUM_ROW="20">
		 <DEFAULT>"yyyy-MM-dd HH:mm:ss"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="STATUS" FIELD="CLOSED_LIST" NUM_ROW="30">
		 <ITEMS DEFAULT="All">
	    	<ITEM NAME="All" VALUE="null" />
			<ITEM NAME="NEW" VALUE="new" />
			<ITEM NAME="RESOLVED" VALUE="resolved" />
			<ITEM NAME="LOCKED" VALUE="locked" />
			<ITEM NAME="PENDING" VALUE="pending" />
		 </ITEMS> 
    </PARAMETER>
    <PARAMETER NAME="STAR_RANKING" FIELD="CLOSED_LIST" NUM_ROW="30">
	    <ITEMS DEFAULT="All">
	    	<ITEM NAME="All" VALUE="null" />
			<ITEM NAME="0" VALUE="0" />
			<ITEM NAME="1" VALUE="1" />
			<ITEM NAME="2" VALUE="2" />
			<ITEM NAME="3" VALUE="3" />
			<ITEM NAME="4" VALUE="4" />
			<ITEM NAME="5" VALUE="5" />
		</ITEMS>  
	</PARAMETER>
    <PARAMETER NAME="LIMIT" FIELD="TEXT" NUM_ROW="30" REQUIRED="true">
    	<DEFAULT>50</DEFAULT>
    </PARAMETER> 
    <PARAMETER NAME="TARGET_RECORD_ONLY" FIELD="CHECK" NUM_ROW="40">
    	<DEFAULT>true</DEFAULT>
   	</PARAMETER> 
  </PARAMETERS>  
  
  <ADVANCED_PARAMETERS> 
     
  </ADVANCED_PARAMETERS>  
  <CODEGENERATION>
  	<IMPORTS>
  		<IMPORT NAME="talend-stewardship-task" MODULE="talend-stewardshipTask.jar" REQUIRED="true" />
  	</IMPORTS>
  </CODEGENERATION>  
  <RETURNS> 
   <RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer"/> 
  </RETURNS> 
</COMPONENT>
