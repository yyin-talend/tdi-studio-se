<COMPONENT>
   
     <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20100112A"
    STARTABLE="true"
     EXTENSION="org.talend.designer.dbmap" 
     SCHEMA_AUTO_PROPAGATE="false" 
     DATA_AUTO_PROPAGATE="false" 
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>ELT/Map/PostgreSQL</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
<!--		<CONNECTOR CTYPE="FLOW" BUILTIN="true" MIN_OUTPUT="1" MAX_INPUT="0"/> -->
	    <CONNECTOR CTYPE="TABLE" BUILTIN="true" MIN_INPUT="1" MIN_OUTPUT="1"/>
	    <CONNECTOR CTYPE="TABLE_REF" BUILTIN="true" MIN_INPUT="1" MIN_OUTPUT="1" MAX_OUTPUT="1" />
	    
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK"/>
		<CONNECTOR CTYPE="COMPONENT_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
       
	   <PARAMETER
	     NAME="USE_EXISTING_CONNECTION"
	     FIELD="CHECK"
	     NUM_ROW="2"
	     SHOW_IF="#LINK@CONNECTOR.OUT.TABLE_REF == '0'"
	   >
	     <DEFAULT>false</DEFAULT>
	   </PARAMETER>    
	   
	   <PARAMETER
	     NAME="CONNECTION"
	     FIELD="COMPONENT_LIST"
	     REQUIRED="true" FILTER="tPostgresqlConnection"
	     NUM_ROW="2"
	     SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'true')"
	     DYNAMIC_SETTINGS="true"/>

      <PARAMETER NAME="MAP" FIELD="EXTERNAL" NUM_ROW="10">
         <DEFAULT/>  
      </PARAMETER>

      <PARAMETER
        NAME="LINK_STYLE"
        FIELD="CLOSED_LIST"
        NUM_ROW="10"
        SHOW_IF="#LINK@CONNECTOR.OUT.TABLE_REF == '0'"
      >
        <ITEMS DEFAULT="">
          <ITEM NAME="AUTO"   VALUE="AUTO" />
          <ITEM NAME="BEZIER_CURVE"   VALUE="BEZIER_CURVE" />
          <ITEM NAME="LINE"   VALUE="LINE" />
        </ITEMS>
      </PARAMETER>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="20"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')"
      REPOSITORY_VALUE="DATABASE:POSTGRESQL"
    />
    
    <PARAMETER
      NAME="DB_VERSION"
      FIELD="CLOSED_LIST"
      NUM_ROW="30"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')"
      REPOSITORY_VALUE="DB_VERSION"
    >
      <ITEMS DEFAULT="V9_X">
        <ITEM NAME="PRIOR_TO_V9" VALUE="PRIOR_TO_V9" />
        <ITEM NAME="V9_X" VALUE="V9_X" />
      </ITEMS>
    </PARAMETER>
    

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>"1433"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="50"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="60"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="60"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="70"
      REQUIRED="true"
      SHOW="false"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>
 
    <PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="70"
      SHOW="true"
      READONLY="true"
      SHOW_IF="#LINK@CONNECTOR.OUT.TABLE_REF == '0'"
    >
      <DEFAULT>postgres_id</DEFAULT>
    </PARAMETER>
    
      <PARAMETER NAME="PREVIEW" FIELD="IMAGE" SHOW="true" NUM_ROW="80">
         <DEFAULT/>
      </PARAMETER>

   </PARAMETERS>
    
  <ADVANCED_PARAMETERS>
  		<PARAMETER NAME="PROPERTIES" FIELD="TEXT" NUM_ROW="10" REPOSITORY_VALUE="PROPERTIES_STRING" SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
  </ADVANCED_PARAMETERS>
  
   <CODEGENERATION>
      <IMPORTS>
        <IMPORT NAME="Driver-Postgres9" MODULE="postgresql-42.2.14.jar" MVN="mvn:org.postgresql/postgresql/42.2.14" REQUIRED_IF="DB_VERSION =='V9_X'" />
        <IMPORT NAME="Driver-Postgres" MODULE="postgresql-8.4-703.jdbc4.jar" MVN="mvn:postgresql/postgresql/8.4-703.jdbc4" REQUIRED_IF="DB_VERSION =='PRIOR_TO_V9'" />
      </IMPORTS>
  </CODEGENERATION>

   <RETURNS>
   </RETURNS>
   
   <PLUGINDEPENDENCIES>
   	<PLUGINDEPENDENCY ID="org.talend.designer.abstractmap" />
<!--   	<PLUGINDEPENDENCY ID="org.talend.designer.dbmap" />  --> 
   </PLUGINDEPENDENCIES>
</COMPONENT>
