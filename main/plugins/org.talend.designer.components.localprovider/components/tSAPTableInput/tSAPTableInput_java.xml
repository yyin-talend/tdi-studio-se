<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20140711A"
    STARTABLE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
  	<FAMILY>Databases/SAP</FAMILY>
    <FAMILY>Business/SAP</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MIN_OUTPUT="1" MAX_OUTPUT="1" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
  	<PARAMETER
    	NAME="NOTE"
    	FIELD="LABEL"
    	NUM_ROW="1"
    >
    	<DEFAULT>(To use this component, you need first to add the SAP Java Connector sapjco3.jar in the Modules view)</DEFAULT>
    </PARAMETER>
  	<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="5" REPOSITORY_VALUE="SAP" />
	
    <PARAMETER NAME="SERVERTYPE" FIELD="CLOSED_LIST" NUM_ROW="9" GROUP="CONNECTION" >
      		<ITEMS DEFAULT="APPLICATIONSERVER">
        		<ITEM NAME="APPLICATIONSERVER"   VALUE="ApplicationServer" />
        		<ITEM NAME="MESSAGESERVER"   VALUE="MessageServer" />
        	</ITEMS>
    </PARAMETER>			
  	<PARAMETER NAME="CLIENT" FIELD="TEXT" NUM_ROW="10" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="CLIENT" >
		<DEFAULT>"000"</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="USERID" FIELD="TEXT" NUM_ROW="20" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="USERID" >
		<DEFAULT>"TALEND"</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="PASSWORD" FIELD="PASSWORD" NUM_ROW="30" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="PASSWORD" >
		<DEFAULT>"FRANCE"</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="LANGUAGE" FIELD="TEXT" NUM_ROW="40" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="LANGUAGE" >
		<DEFAULT>"EN"</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="HOSTNAME" FIELD="TEXT" NUM_ROW="50" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="HOSTNAME" >
		<DEFAULT>"HOST"</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="SYSTEMNUMBER" FIELD="TEXT" NUM_ROW="60" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="SYSTEMNUMBER" SHOW_IF="SERVERTYPE=='APPLICATIONSERVER'">
		<DEFAULT>"00"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="SYSTEMID" FIELD="TEXT" NUM_ROW="60" GROUP="CONNECTION" REQUIRED="true" SHOW_IF="SERVERTYPE=='MESSAGESERVER'">
		<DEFAULT>"PRD"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="GROUPNAME" FIELD="TEXT" NUM_ROW="70" GROUP="CONNECTION" REQUIRED="true" SHOW_IF="SERVERTYPE=='MESSAGESERVER'">
		<DEFAULT>"tGroup"</DEFAULT>
	</PARAMETER>
	
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="80"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    	
  	<PARAMETER NAME="TABLE" FIELD="TEXT" NUM_ROW="100" REQUIRED="true" GROUP="TABLE_CONFIG">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	
  	<PARAMETER NAME="FILTER" FIELD="TEXT" NUM_ROW="110" REQUIRED="true" GROUP="TABLE_CONFIG">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	
  	<PARAMETER NAME="START_ROW" FIELD="TEXT" NUM_ROW="120" REQUIRED="true" GROUP="TABLE_CONFIG">
		<DEFAULT>-1</DEFAULT>
	</PARAMETER>
	
  	<PARAMETER NAME="MAX_ROW_COUNT" FIELD="TEXT" NUM_ROW="120" REQUIRED="true" GROUP="TABLE_CONFIG">
		<DEFAULT>-1</DEFAULT>
	</PARAMETER>
	
  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
	<PARAMETER NAME="SAP_PROPERTIES" FIELD="TABLE" REQUIRED="false" NUM_ROW="1" NB_LINES="3">
		<ITEMS>
			<ITEM NAME="PROPERTY" />
			<ITEM NAME="VALUE" />
		</ITEMS>
	</PARAMETER>  
  </ADVANCED_PARAMETERS>
  
  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="sapjco-ntintel-3" MODULE="sapjco3.jar" REQUIRED="true" BundleID="" />
      <IMPORT NAME="sap-api-0.0.1-SNAPSHOT.jar" MODULE="sap-api-0.0.1-SNAPSHOT.jar" REQUIRED="true" BundleID="" />
      <IMPORT NAME="sap-impl-0.0.1-SNAPSHOT.jar" MODULE="sap-impl-0.0.1-SNAPSHOT.jar" REQUIRED="true" BundleID="" />
    </IMPORTS>
  </CODEGENERATION>
  
  <RETURNS/>
  
</COMPONENT>
