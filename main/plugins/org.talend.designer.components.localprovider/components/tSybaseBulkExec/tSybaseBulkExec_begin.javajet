%>

StringBuilder command_<%=cid %> =  new StringBuilder();
command_<%=cid %>.append(<%=bcpUtility%> + " ");
<%
if(("UPDATE").equals(dataAction)) {
    %>
    String tmpTableName_<%=cid%> = tableName_<%=cid%>;
    tableName_<%=cid%> = "tmp_<%=cid%>" + "_" + pid + Thread.currentThread().getId();
