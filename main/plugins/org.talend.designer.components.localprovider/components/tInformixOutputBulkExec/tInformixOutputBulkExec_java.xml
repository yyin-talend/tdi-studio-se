<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="200903"
    STARTABLE="false"
	PARTITIONING="NONE"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/DB Specifics/Informix</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:INFORMIX"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    />
    
    <PARAMETER
      NAME="EXECUTION_PLATFORM"
      FIELD="CLOSED_LIST"
      NUM_ROW="5"
      REQUIRED="true"
    >
      <ITEMS DEFAULT="WINDOWS">
    		<ITEM NAME="WINDOWS" VALUE="Windows" />
    		<ITEM NAME="LINUX" VALUE="Linux" />
    	</ITEMS>
    </PARAMETER>

  	<PARAMETER 
  	  NAME="USE_EXISTING_CONNECTION" 
  	  FIELD="CHECK"
	  NUM_ROW="10">
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>

	<PARAMETER 
	  NAME="CONNECTION" 
	  FIELD="COMPONENT_LIST"
	  REQUIRED="true" FILTER="tInformixConnection" 
	  NUM_ROW="10"
	  SHOW_IF="USE_EXISTING_CONNECTION == 'true'" 
	  DYNAMIC_SETTINGS="true"
	 />
  

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="15"
      REQUIRED="true"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
	<PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="3"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>Informix</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="15"
      REPOSITORY_VALUE="PORT"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>"1533"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="20"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER 
      NAME="SCHEMA_DB"
      FIELD="TEXT"
      NUM_ROW="20"
      REPOSITORY_VALUE="SCHEMA"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="25"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="25"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBSERVER"
      FIELD="TEXT"
      NUM_ROW="27"
      REQUIRED="true"
      REPOSITORY_VALUE="DATASOURCE"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>    
    
    <PARAMETER
      NAME="TABLE"
      FIELD="DBTABLE"
      NUM_ROW="30"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER> 


    <PARAMETER
      NAME="TABLE_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="33"
      NOT_SHOW_IF="PARALLELIZE == 'true'"
    >
      <ITEMS DEFAULT="NONE">
        <ITEM NAME="NONE" VALUE="NONE" />
        <ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE"/>
        <ITEM NAME="CREATE" VALUE="CREATE"/>
        <ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS"/>
        <ITEM NAME="DROP_IF_EXISTS_AND_CREATE" VALUE="DROP_IF_EXISTS_AND_CREATE" />
        <ITEM NAME="CLEAR" VALUE="CLEAR"/>
      </ITEMS>
    </PARAMETER>

	<PARAMETER 
		NAME="NOTE" 
		FIELD="LABEL" 
		NUM_ROW="33"
		NOT_SHOW_IF="(TABLE_ACTION == 'NONE') OR (USE_EXISTING_CONNECTION == 'false')"	
		>
		<DEFAULT>Warning : this component configuration will automatically generate a commit before bulk insert</DEFAULT>
	</PARAMETER>

    <PARAMETER 
    	NAME="SCHEMA" 
    	FIELD="SCHEMA_TYPE" 
    	REQUIRED="true" 
    	NUM_ROW="35">
      <DEFAULT/> 
    </PARAMETER>
    
    <PARAMETER
      NAME="INFORMIX_DIR"
      FIELD="DIRECTORY"
      NUM_ROW="37"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DATA"
      FIELD="FILE"
      NUM_ROW="40"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.csv"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER 
    	NAME="APPEND" 
    	FIELD="CHECK" 
    	REQUIRED="true" 
    	NUM_ROW="41" 
    	REPOSITORY_VALUE="APPEND">
      <DEFAULT>false</DEFAULT>
    </PARAMETER> 
     
    <PARAMETER
      NAME="DATA_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="43"
    >
      <ITEMS DEFAULT="INSERT">
        <ITEM NAME="INSERT"    VALUE="INSERT" />
      </ITEMS>
    </PARAMETER>
    
	<PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="50"
      SHOW="false"
    >       
      <DEFAULT>informix_id</DEFAULT>
    </PARAMETER>      
    
  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
  
  	<PARAMETER 
  		NAME="PROPERTIES" 
  		FIELD="TEXT" 
  		NUM_ROW="1" 
  		REPOSITORY_VALUE="PROPERTIES_STRING"
  		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	
	<PARAMETER 
		NAME="ROWSEPARATOR" 
		FIELD="TEXT" 
		REQUIRED="true" 
		NUM_ROW="10" 
		REPOSITORY_VALUE="ROW_SEPARATOR">
      <DEFAULT>"\n"</DEFAULT>
    </PARAMETER>
    
  	<PARAMETER
      NAME="FIELDS_TERMINATED_BY"
      FIELD="TEXT"
      NUM_ROW="10"
      MAX_LENGTH="1"
      REQUIRED="true"
    >
      <DEFAULT>"|"</DEFAULT>
    </PARAMETER> 
    
  	<PARAMETER 
  		NAME="SET_DBMONEY" 
  		FIELD="CHECK" 
  		REQUIRED="true" 
  		NUM_ROW="20" >
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
		
	<PARAMETER 
		NAME="DECIMAL_SEPARATOR" 
		FIELD="TEXT" REQUIRED="true" 
		NUM_ROW="20"
		SHOW_IF="SET_DBMONEY == 'true')">
		<DEFAULT>"."</DEFAULT>
	</PARAMETER>
	
	<PARAMETER 
		NAME="DATE_FORMAT" 
		FIELD="CLOSED_LIST" 
		REQUIRED="true" 
		NUM_ROW="25">
	   <ITEMS DEFAULT="Y4MD/">
	    <ITEM NAME="Y4MD/" VALUE="yyyy/MM/dd,Y4MD/" />
      	<ITEM NAME="Y2MD/" VALUE="yy/MM/dd,Y2MD/" />
        <ITEM NAME="MDY4/" VALUE="MM/dd/yyyy,MDY4/" />
        <ITEM NAME="MDY2/" VALUE="MM/dd/yy,MDY2/" />
        <ITEM NAME="DMY4/" VALUE="dd/MM/yyyy,DMY4/" />
        <ITEM NAME="DMY2/" VALUE="dd/MM/yy,DMY2/" />
        <ITEM NAME="Y4MD-" VALUE="yyyy-MM-dd,Y4MD-" />
      	<ITEM NAME="Y2MD-" VALUE="yy-MM-dd,Y2MD-" />
        <ITEM NAME="MDY4-" VALUE="MM-dd-yyyy,MDY4-" />
        <ITEM NAME="MDY2-" VALUE="MM-dd-yy,MDY2-" />
        <ITEM NAME="DMY4-" VALUE="dd-MM-yyyy,DMY4-" />
        <ITEM NAME="DMY2-" VALUE="dd-MM-yy,DMY2-" />
        <ITEM NAME="CUSTOM" VALUE="CUSTOM" />
      </ITEMS>
	</PARAMETER>
	
	<PARAMETER 
		NAME="DATE_FORMAT_CUSTOM" 
		FIELD="TEXT" 
		NUM_ROW="25" 
		SHOW_IF="DATE_FORMAT == 'CUSTOM'">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	
	<PARAMETER 
		NAME="ROWS_BEFORE_COMMIT" 
		FIELD="TEXT" 
		REQUIRED="true" 
		NUM_ROW="30">
		<DEFAULT>"10000"</DEFAULT>
	</PARAMETER>
	
	<PARAMETER 
		NAME="BAD_ROWS_BEFORE_ABORT" 
		FIELD="TEXT" 
		REQUIRED="true" 
		NUM_ROW="35">
		<DEFAULT>"10"</DEFAULT>
	</PARAMETER>
	
	<PARAMETER 
		NAME="CREATE" 
		FIELD="CHECK" 
		REQUIRED="true" 
		NUM_ROW="40">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
    
    <PARAMETER 
    	NAME="FLUSHONROW" 
    	FIELD="CHECK" 
    	REQUIRED="true" 
    	NUM_ROW="40">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER 
	    NAME="FLUSHONROW_NUM" 
	    FIELD="TEXT" 
	    REQUIRED="true" 
	    NUM_ROW="50"
	    SHOW_IF="FLUSHONROW == 'true'"
    >
      <DEFAULT>1</DEFAULT>
    </PARAMETER>
     
    
    <PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="60" REQUIRED="true" REPOSITORY_VALUE="ENCODING">
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>
	
    <PARAMETER NAME="OUTPUT" FIELD="CLOSED_LIST" NUM_ROW="100">
       <ITEMS DEFAULT="OUTPUT_TO_CONSOLE">
          <ITEM NAME="OUTPUT_TO_CONSOLE" VALUE="OUTPUT_TO_CONSOLE" />
          <ITEM NAME="RETRIEVE_OUTPUT"   VALUE="RETRIEVE_OUTPUT" />
       </ITEMS>
    </PARAMETER>
          
  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
         <TEMPLATES INPUT="tOOB" OUTPUT="tOBE">

      <TEMPLATE NAME="tOOB" COMPONENT="tInformixOutputBulk">
        <LINK_TO NAME="tOBE" CTYPE="ROWS_END"/>
      </TEMPLATE>
      
      <TEMPLATE NAME="tOBE" COMPONENT="tInformixBulkExec"/>
      <TEMPLATE_PARAM SOURCE="self.EXECUTION_PLATFORM"   TARGET="tOBE.EXECUTION_PLATFORM" />
      <TEMPLATE_PARAM SOURCE="self.USE_EXISTING_CONNECTION"   TARGET="tOBE.USE_EXISTING_CONNECTION" />
      <TEMPLATE_PARAM SOURCE="self.CONNECTION"   TARGET="tOBE.CONNECTION" />
      <TEMPLATE_PARAM SOURCE="self.HOST"   TARGET="tOBE.HOST" />
      <TEMPLATE_PARAM SOURCE="self.DBNAME" TARGET="tOBE.DBNAME" />
      <TEMPLATE_PARAM SOURCE="self.SCHEMA_DB" TARGET="tOBE.SCHEMA_DB" />
      <TEMPLATE_PARAM SOURCE="self.PORT"   TARGET="tOBE.PORT"   />
      <TEMPLATE_PARAM SOURCE="self.USER"   TARGET="tOBE.USER" />
      <TEMPLATE_PARAM SOURCE="self.PASS"   TARGET="tOBE.PASS" />
      <TEMPLATE_PARAM SOURCE="self.TABLE_ACTION"  TARGET="tOBE.TABLE_ACTION" />
      <TEMPLATE_PARAM SOURCE="self.TABLE"  TARGET="tOBE.TABLE" />
	  <TEMPLATE_PARAM SOURCE="self.DATA_ACTION"  TARGET="tOBE.DATA_ACTION" />
	  <TEMPLATE_PARAM SOURCE="self.MAPPING"  TARGET="tOBE.MAPPING" />
	  <TEMPLATE_PARAM SOURCE="self.DBSERVER"  TARGET="tOBE.DBSERVER" />  
	  <TEMPLATE_PARAM SOURCE="self.INFORMIX_DIR"  TARGET="tOBE.INFORMIX_DIR" />
	  <TEMPLATE_PARAM SOURCE="self.PROPERTIES"  TARGET="tOBE.PROPERTIES" />
	  <TEMPLATE_PARAM SOURCE="self.ROWS_BEFORE_COMMIT"  TARGET="tOBE.ROWS_BEFORE_COMMIT" />
	  <TEMPLATE_PARAM SOURCE="self.BAD_ROWS_BEFORE_ABORT"  TARGET="tOBE.BAD_ROWS_BEFORE_ABORT" />
	  <TEMPLATE_PARAM SOURCE="self.OUTPUT"  TARGET="tOBE.OUTPUT" />
 	  <TEMPLATE_PARAM SOURCE="self.TYPE"  TARGET="tOBE.TYPE" />
	
	  <TEMPLATE_PARAM SOURCE="self.APPEND" TARGET="tOOB.APPEND" />
	  <TEMPLATE_PARAM SOURCE="self.ROWSEPARATOR" TARGET="tOOB.ROWSEPARATOR" />
	  <TEMPLATE_PARAM SOURCE="self.CREATE" TARGET="tOOB.CREATE" />
	  <TEMPLATE_PARAM SOURCE="self.FLUSHONROW" TARGET="tOOB.FLUSHONROW" />
	  <TEMPLATE_PARAM SOURCE="self.FLUSHONROW_NUM" TARGET="tOOB.FLUSHONROW_NUM" />
	  <TEMPLATE_PARAM SOURCE="self.ENCODING" TARGET="tOOB.ENCODING" />
	
	  <TEMPLATE_PARAM SOURCE="self.DATE_FORMAT" TARGET="tOBE.DATE_FORMAT" />
	  <TEMPLATE_PARAM SOURCE="self.DATE_FORMAT" TARGET="tOOB.DATE_FORMAT" />
	  <TEMPLATE_PARAM SOURCE="self.DATE_FORMAT_CUSTOM" TARGET="tOBE.DATE_FORMAT_CUSTOM" />
	  <TEMPLATE_PARAM SOURCE="self.DATE_FORMAT_CUSTOM" TARGET="tOOB.DATE_FORMAT_CUSTOM" />
	
	  <TEMPLATE_PARAM SOURCE="self.SCHEMA"  TARGET="tOBE.SCHEMA" />
      <TEMPLATE_PARAM SOURCE="self.SCHEMA"  TARGET="tOOB.SCHEMA" />
      
      <TEMPLATE_PARAM SOURCE="self.DATA" TARGET="tOOB.FILENAME" />
      <TEMPLATE_PARAM SOURCE="self.DATA" TARGET="tOBE.DATA" />
      
      <TEMPLATE_PARAM SOURCE="self.FIELDS_TERMINATED_BY"  TARGET="tOOB.FIELDSEPARATOR" />
      <TEMPLATE_PARAM SOURCE="self.FIELDS_TERMINATED_BY"  TARGET="tOBE.FIELDS_TERMINATED_BY" />
      
      <TEMPLATE_PARAM SOURCE="self.SET_DBMONEY"  TARGET="tOOB.SET_DBMONEY" />
      <TEMPLATE_PARAM SOURCE="self.SET_DBMONEY"  TARGET="tOBE.SET_DBMONEY" />
      <TEMPLATE_PARAM SOURCE="self.DECIMAL_SEPARATOR"  TARGET="tOOB.DECIMAL_SEPARATOR" />
      <TEMPLATE_PARAM SOURCE="self.DECIMAL_SEPARATOR"  TARGET="tOBE.DECIMAL_SEPARATOR" />
      
  </TEMPLATES>
  </CODEGENERATION>
   
  <RETURNS/>
  
</COMPONENT>