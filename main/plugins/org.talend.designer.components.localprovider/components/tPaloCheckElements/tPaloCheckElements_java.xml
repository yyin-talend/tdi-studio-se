<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20091219A"
		STARTABLE="false" DATA_AUTO_PROPAGATE="false" LOG4J_ENABLED="true"
		HAS_CONDITIONAL_OUTPUTS="true" PARTITIONING="AUTO">
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>Business_Intelligence/OLAP_Cube/Palo</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
		<CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

<PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="2"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      REQUIRED="true" FILTER="tPaloConnection"
      NUM_ROW="3"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />

	<PARAMETER
		NAME="SERVER"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="5"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
	  <DEFAULT>"localhost"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="SERVERPORT"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="6"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
	  <DEFAULT>"7777"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="USERNAME"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="7"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
	  <DEFAULT>"admin"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="PASS"
		FIELD="PASSWORD"
		REQUIRED="true"
		NUM_ROW="8"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
	>
	  <DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="DATABASE"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="10"
	>
	  <DEFAULT>"Database"</DEFAULT>
	</PARAMETER>


	<PARAMETER
		NAME="CUBE"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="11"
 	>
	  <DEFAULT>"Cube"</DEFAULT>
	</PARAMETER>


	<PARAMETER
      	NAME="ERROR_HANDLING"
	      FIELD="CLOSED_LIST"
      	NUM_ROW="12"
    	>
	<ITEMS DEFAULT="REJECT_ROW">
        <ITEM NAME="REJECT_ROW" VALUE="REJECT_ROW" />
        <ITEM NAME="USE_DEFAULT" VALUE="USE_DEFAULT" />
        <ITEM NAME="DIE_ON_MISSING_ELEMENT" VALUE="DIE_ON_MISSING_ELEMENT"/>
      </ITEMS>
    </PARAMETER>


		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="20" SHOW="true">
		</PARAMETER>


		<PARAMETER NAME="SCHEMA_REJECT" FIELD="SCHEMA_TYPE"
			REQUIRED="true" NUM_ROW="30" CONTEXT="REJECT" SHOW="true">
			<TABLE READONLY="true">
				<COLUMN NAME="errorMessage" TYPE="id_String"
					LENGTH="255" READONLY="false" CUSTOM="true" />
			</TABLE>
		</PARAMETER>
	<PARAMETER
		NAME="MEASURE_COLUMN"
      	FIELD="PREV_COLUMN_LIST"
      	REQUIRED="true"
      	NUM_ROW="31"
		SHOW="false"
    	>
    	</PARAMETER>

    <PARAMETER
		  NAME="ELEMENTS_TO_CHECK"
		  FIELD="TABLE"
		  REQUIRED="true"
		  NUM_ROW="35"
		  NB_LINES="5"
		>
		  <ITEMS BASED_ON_SCHEMA="true">
			<ITEM NAME="ELEMENT_TYPE" FIELD="CLOSED_LIST">
				<ITEMS DEFAULT="ELEMENT_ELEMENT">
					<ITEM NAME="ELEMENT_ELEMENT" VALUE="ELEMENT_ELEMENT" />
					<ITEM NAME="ELEMENT_MEASURE" VALUE="ELEMENT_MEASURE" />
				</ITEMS>
			</ITEM>
			<ITEM NAME="DEFAULT_ELEMENT" FIELD="TEXT" VALUE="&quot;&quot;" />
		  </ITEMS>
	</PARAMETER>


	</PARAMETERS>
	 <ADVANCED_PARAMETERS>
	<PARAMETER NAME="DEPLOY_PALO_LIBS" FIELD="CHECK" NUM_ROW="200" SHOW="false">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

  </ADVANCED_PARAMETERS>
    <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="paloIX3" MODULE="paloIX3-1.0.1.jar" MVN="mvn:org.talend.components/paloIX3/1.0.1" REQUIRED="true" />
      <IMPORT NAME="httpclient-4.5.13" MODULE="httpclient-4.5.13.jar" MVN="mvn:org.apache.httpcomponents/httpclient/4.5.13" REQUIRED="true" />
      <IMPORT NAME="httpmime-4.1.2" MODULE="httpmime-4.1.2.jar" MVN="mvn:org.talend.libraries/httpmime-4.1.2/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.apache.http/lib/httpmime-4.1.2.jar" REQUIRED="true" />
      <IMPORT NAME="httpcore-4.4.13" MODULE="httpcore-4.4.13.jar" MVN="mvn:org.apache.httpcomponents/httpcore/4.4.13" REQUIRED="true" />
      <IMPORT NAME="commons-logging-1.2" MODULE="commons-logging-1.2.jar" MVN="mvn:commons-logging/commons-logging/1.2" REQUIRED="true" />
      <IMPORT NAME="commons-codec-1.14" MODULE="commons-codec-1.14.jar" MVN="mvn:commons-codec/commons-codec/1.14" REQUIRED="true" />
      <IMPORT NAME="Talend_CSV" MODULE="talendcsv.jar" MVN="mvn:org.talend.libraries/talendcsv/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.csv/lib/talendcsv.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>


	<RETURNS></RETURNS>
</COMPONENT>
