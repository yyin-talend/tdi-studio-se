                netscape.ldap.util.LDIFAddContent addContent_<%=cid %> = (netscape.ldap.util.LDIFAddContent) content_<%=cid %>;
                attributes_<%=cid %> = addContent_<%=cid %>.getAttributes();
                
                for (int i_<%=cid %> = 0; i_<%=cid %> < filters_<%=cid %>.length; i_<%=cid %>++) {
                
