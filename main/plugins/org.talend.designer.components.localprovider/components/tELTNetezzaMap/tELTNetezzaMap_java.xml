<COMPONENT>
     <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070314A"
    STARTABLE="true"
     EXTENSION="org.talend.designer.dbmap" 
     SCHEMA_AUTO_PROPAGATE="false" 
     DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>ELT/Map/Netezza</FAMILY>
  </FAMILIES>
  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
	    <CONNECTOR CTYPE="TABLE" BUILTIN="true" MIN_INPUT="1" MIN_OUTPUT="1"/>
	    <CONNECTOR CTYPE="TABLE_REF" BUILTIN="true" MIN_INPUT="1" MIN_OUTPUT="1" MAX_OUTPUT="1" />
	    
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK"/>
		<CONNECTOR CTYPE="COMPONENT_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

	   <PARAMETER
	     NAME="USE_EXISTING_CONNECTION"
	     FIELD="CHECK"
	     NUM_ROW="10"
	     SHOW_IF="#LINK@CONNECTOR.OUT.TABLE_REF == '0'"
	   >
	     <DEFAULT>false</DEFAULT>
	   </PARAMETER>    
	   
	   <PARAMETER
	     NAME="CONNECTION"
	     FIELD="COMPONENT_LIST"
	     REQUIRED="true" FILTER="tNetezzaConnection"
	     NUM_ROW="10"
	     SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'true')"
	     DYNAMIC_SETTINGS="true"/>	  

      <PARAMETER NAME="MAP" FIELD="EXTERNAL" NUM_ROW="20">
         <DEFAULT/>  
      </PARAMETER>

      <PARAMETER
        NAME="LINK_STYLE"
        FIELD="CLOSED_LIST"
        NUM_ROW="20"
        SHOW_IF="#LINK@CONNECTOR.OUT.TABLE_REF == '0'"
      >
        <ITEMS DEFAULT="">
          <ITEM NAME="AUTO"   VALUE="AUTO" />
          <ITEM NAME="BEZIER_CURVE"   VALUE="BEZIER_CURVE" />
          <ITEM NAME="LINE"   VALUE="LINE" />
        </ITEMS>
      </PARAMETER>
   

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="30"
      SHOW_IF="#LINK@CONNECTOR.OUT.TABLE_REF == '0'"
      REPOSITORY_VALUE="DATABASE:NETEZZA"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>"5480"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="50"
      REQUIRED="true"
      REPOSITORY_VALUE="SID"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="60"
      REQUIRED="true"
      REPOSITORY_VALUE="USERNAME"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="60"
      REQUIRED="true"
      REPOSITORY_VALUE="PASSWORD"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="70"
      REQUIRED="true"
      SHOW="false"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="70"
      SHOW="true"
      READONLY="true"
      SHOW_IF="#LINK@CONNECTOR.OUT.TABLE_REF == '0'"
    >
      <DEFAULT>netezza_id</DEFAULT>
    </PARAMETER>

	<PARAMETER NAME="PREVIEW" FIELD="IMAGE" SHOW="true" NUM_ROW="60">
	  <DEFAULT/>
	</PARAMETER>

   </PARAMETERS>
   
   <ADVANCED_PARAMETERS>
	
	<PARAMETER 
		NAME="PROPERTIES" 
		FIELD="TEXT" 
		NUM_ROW="1" 
		SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
   
   </ADVANCED_PARAMETERS>
   
   <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="Driver-Netezza" MODULE="nzjdbc.jar" MVN="mvn:org.talend.libraries/nzjdbc/6.0.0"  REQUIRED="true" />
     </IMPORTS>
   </CODEGENERATION>
   
   <RETURNS>
   </RETURNS>
   
   <PLUGINDEPENDENCIES>
   	<PLUGINDEPENDENCY ID="org.talend.designer.abstractmap" />
   </PLUGINDEPENDENCIES>
</COMPONENT>
