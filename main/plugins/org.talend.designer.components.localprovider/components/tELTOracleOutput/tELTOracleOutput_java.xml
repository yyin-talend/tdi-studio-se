<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070314A"
    STARTABLE="false"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>ELT/Map/Oracle</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>

    <CONNECTOR CTYPE="TABLE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="DATA_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="2"
    >
      <ITEMS DEFAULT="INSERT">
        <ITEM NAME="INSERT"           VALUE="INSERT" />
        <ITEM NAME="UPDATE"           VALUE="UPDATE"/>
        <ITEM NAME="DELETE"           VALUE="DELETE"/>
		<ITEM NAME="MERGE"           VALUE="MERGE"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="3"
    >
    </PARAMETER>
    
    <PARAMETER NAME="USE_WHERE_CONDITIONS_TABLE" FIELD="CHECK" NUM_ROW="5"
		SHOW_IF="(DATA_ACTION == 'UPDATE') OR (DATA_ACTION == 'DELETE')">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
    <PARAMETER NAME="WHERE_CLAUSE" FIELD="MEMO" NUM_ROW="6"
    	SHOW_IF="((DATA_ACTION == 'UPDATE') OR (DATA_ACTION == 'DELETE')) AND (USE_WHERE_CONDITIONS_TABLE == 'false')">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
	<PARAMETER NAME="WHERE_CONDITIONS_TABLE" FIELD="TABLE" NUM_ROW="7"
		SHOW_IF="((DATA_ACTION == 'UPDATE') OR (DATA_ACTION == 'DELETE')) AND (USE_WHERE_CONDITIONS_TABLE == 'true')">
		<ITEMS>
			<ITEM NAME="COLUMN" VALUE='""'/>
			<ITEM NAME="FUNCTION" FIELD="CLOSED_LIST">
				<ITEMS DEFAULT="EQUAL">
					<ITEM NAME="EQUAL" VALUE="&quot;=&quot;"/>
					<ITEM NAME="GREATER" VALUE="&quot;&gt;&quot;"/>
					<ITEM NAME="LESS" VALUE="&quot;&lt;&quot;"/>
					<ITEM NAME="GREATER_OR_EQUAL" VALUE="&quot;&gt;=&quot;"/>
					<ITEM NAME="LESS_OR_EQUAL" VALUE="&quot;&lt;=&quot;"/>
					<ITEM NAME="NOT_EQUAL" VALUE="&quot;&lt;&gt;&quot;"/>
					<ITEM NAME="BETWEEN" VALUE="&quot;BETWEEN&quot;"/>
					<ITEM NAME="LIKE" VALUE="&quot;LIKE&quot;"/>
					<ITEM NAME="IN" VALUE="&quot;IN&quot;"/>
				</ITEMS>
			</ITEM>
			<ITEM NAME="VALUE_SQL" VALUE='""'/>
		</ITEMS>	
	</PARAMETER>
	
	<PARAMETER
      NAME="USE_MERGE_UPDATE"
      FIELD="CHECK"
      NUM_ROW="9"
	  SHOW_IF="DATA_ACTION == 'MERGE'"
    >
    	<DEFAULT>false</DEFAULT>
    </PARAMETER>
	
	 <PARAMETER
      NAME="SCHEMA_UPDATE"
      FIELD="TABLE"
      REQUIRED="true"
      NUM_ROW="9"
      NB_LINES="5"
	  SHOW_IF="(DATA_ACTION == 'MERGE') AND (USE_MERGE_UPDATE == 'true')"
    >
      <ITEMS BASED_ON_SCHEMA="true">      	
		<ITEM NAME="UPDATE" FIELD="CHECK" VALUE="false"/>
      </ITEMS>
    </PARAMETER>	
	
	
	<PARAMETER
      NAME="USE_MERGE_UPDATE_WHERE"
      FIELD="CHECK"
      NUM_ROW="10"
	  SHOW_IF="(DATA_ACTION == 'MERGE') AND (USE_MERGE_UPDATE == 'true')"
    >
    	<DEFAULT>false</DEFAULT>
    </PARAMETER>
	
	<PARAMETER
      NAME="MERGE_UPDATE_WHERE_STRING"
      FIELD="TEXT"
      NUM_ROW="10"
	  SHOW_IF="(DATA_ACTION == 'MERGE') AND (USE_MERGE_UPDATE == 'true') AND (USE_MERGE_UPDATE_WHERE == 'true') "
    >
	<DEFAULT>" "</DEFAULT>	
	</PARAMETER>
		
	<PARAMETER
      NAME="USE_MERGE_UPDATE_DELETE_WHERE"
      FIELD="CHECK"
      NUM_ROW="11"
	  SHOW_IF="(DATA_ACTION == 'MERGE') AND (USE_MERGE_UPDATE == 'true')"
    >
    	<DEFAULT>false</DEFAULT>
    </PARAMETER>
	
	<PARAMETER
      NAME="MERGE_UPDATE_DELETE_WHERE_STRING"
      FIELD="TEXT"
      NUM_ROW="11"
	  SHOW_IF="(DATA_ACTION == 'MERGE') AND (USE_MERGE_UPDATE == 'true') AND (USE_MERGE_UPDATE_DELETE_WHERE == 'true') "
    >
	<DEFAULT>" "</DEFAULT>	
	</PARAMETER>
	
	<PARAMETER
      NAME="USE_MERGE_INSERT"
      FIELD="CHECK"
      NUM_ROW="13"
	  SHOW_IF="DATA_ACTION == 'MERGE'"
    >
    	<DEFAULT>false</DEFAULT>
    </PARAMETER>

	 <PARAMETER
      NAME="SCHEMA_INSERT"
      FIELD="TABLE"
      REQUIRED="true"
      NUM_ROW="13"
      NB_LINES="5"
	  SHOW_IF="(DATA_ACTION == 'MERGE') AND (USE_MERGE_INSERT == 'true')"
    >
      <ITEMS BASED_ON_SCHEMA="true">
		<ITEM NAME="INSERT" FIELD="CHECK" VALUE="false"/>
      </ITEMS>
    </PARAMETER>	
	
	<PARAMETER
      NAME="USE_MERGE_INSERT_WHERE"
      FIELD="CHECK"
      NUM_ROW="14"
	  SHOW_IF="(DATA_ACTION == 'MERGE') AND (USE_MERGE_INSERT == 'true')"
    >
    	<DEFAULT>false</DEFAULT>
    </PARAMETER>
	
	  <PARAMETER
      NAME="MERGE_INSERT_WHERE_STRING"
      FIELD="TEXT"
      NUM_ROW="14"
	  SHOW_IF="(DATA_ACTION == 'MERGE') AND (USE_MERGE_INSERT == 'true') AND (USE_MERGE_INSERT_WHERE == 'true')"
    >
	<DEFAULT>" "</DEFAULT>	
	</PARAMETER>
    
	<PARAMETER
      NAME="ELT_TABLE_NAME"
      FIELD="TEXT"
      NUM_ROW="15"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    	
	<PARAMETER
      NAME="ELT_SCHEMA_NAME"
      FIELD="TEXT"
      NUM_ROW="20"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    
    <PARAMETER
      NAME="IS_TABLE_NAME_VARIABLE"
      FIELD="CHECK"
      NUM_ROW="25"
      SHOW_IF="USE_DIFFERENT_TABLE=='false'">
        <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USE_DIFFERENT_TABLE"
      FIELD="CHECK"
      NUM_ROW="30"
      SHOW="true">
	<DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DIFFERENT_TABLE_NAME"
      FIELD="TEXT"
      NUM_ROW="30"
      REQUIRED="true"
      SHOW_IF="USE_DIFFERENT_TABLE=='true'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER> 
    
    <PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="99"
      SHOW="true"
      READONLY="true"
    >
      <DEFAULT>oracle_id</DEFAULT>
    </PARAMETER>

  </PARAMETERS>
  
    <ADVANCED_PARAMETERS>
      <PARAMETER
      NAME="USE_HINT_OPTIONS"
      FIELD="CHECK"
      NUM_ROW="10"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="HINT_OPTIONS"
      FIELD="TABLE"
      NUM_ROW="10"
      NB_LINES="5"      
	  SHOW_IF="USE_HINT_OPTIONS == 'true'"
    >
      <ITEMS>
        <ITEM NAME="HINT" VALUE="&quot;/*+ */&quot;" FIELD="TEXT" />
        <ITEM NAME="POSITION" VALUE="AFTER" FIELD="CLOSED_LIST">
        	<ITEMS DEFAULT="AFTER">
        		<ITEM NAME="AFTER"  VALUE="AFTER"/>
        	</ITEMS>
		</ITEM>
        <ITEM NAME="SQL_STMT"  VALUE="true"  FIELD="CLOSED_LIST">
        	<ITEMS DEFAULT="INSERT">
        		<ITEM NAME="INSERT"  VALUE="INSERT"/>
				<ITEM NAME="UPDATE"  VALUE="UPDATE"/>
				<ITEM NAME="DELETE"  VALUE="DELETE"/>
				<ITEM NAME="MERGE"  VALUE="MERGE"/>
        	</ITEMS>
		</ITEM>
      </ITEMS>
    </PARAMETER>
  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
  </CODEGENERATION>
   
  <RETURNS>
    <RETURN NAME="NB_LINE"          TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_UPDATED"  TYPE="id_Integer" AVAILABILITY="AFTER" SHOW_IF="DATA_ACTION=='UPDATE'"/>
    <RETURN NAME="NB_LINE_INSERTED" TYPE="id_Integer" AVAILABILITY="AFTER" SHOW_IF="DATA_ACTION=='INSERT'"/>
    <RETURN NAME="NB_LINE_DELETED"  TYPE="id_Integer" AVAILABILITY="AFTER" SHOW_IF="DATA_ACTION=='DELETE'"/>
    <RETURN NAME="NB_LINE_MERGED"   TYPE="id_Integer" AVAILABILITY="AFTER" SHOW_IF="DATA_ACTION=='MERGE'"/>
  </RETURNS>
</COMPONENT>
