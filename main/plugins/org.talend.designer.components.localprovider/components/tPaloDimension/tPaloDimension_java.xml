<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="2.0"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20091023"
    STARTABLE="true"
	PARTITIONING="AUTO"
	LOG4J_ENABLED="true"
	>
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
	<FAMILY>Business_Intelligence/OLAP_Cube/Palo</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

<CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>




  <PARAMETERS>

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="2"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      REQUIRED="true" FILTER="tPaloConnection"
      NUM_ROW="3"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />

	<PARAMETER
		NAME="SERVER"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="5"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
	  <DEFAULT>"localhost"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="SERVERPORT"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="6"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
	  <DEFAULT>"7777"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="USERNAME"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="7"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
	  <DEFAULT>"admin"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="PASS"
		FIELD="PASSWORD"
		REQUIRED="true"
		NUM_ROW="8"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
	>
	  <DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="DATABASE"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="10"
	>
	  <DEFAULT>"Database"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="DIMENSION"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="11"
	>
	  <DEFAULT>"Dimension"</DEFAULT>
	</PARAMETER>

	<PARAMETER
      	NAME="DIMENSION_ACTION"
	      FIELD="CLOSED_LIST"
      	NUM_ROW="11"
    	>
	<ITEMS DEFAULT="NONE">
	  <ITEM NAME="NONE" VALUE="NONE" />
        <ITEM NAME="CREATE" VALUE="CREATE" />
        <ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS"/>
        <ITEM NAME="DELETE_IF_EXISTS_AND_CREATE" VALUE="DELETE_IF_EXISTS_AND_CREATE"/>
        <ITEM NAME="DELETE" VALUE="DELETE"/>
      </ITEMS>
    </PARAMETER>


    <PARAMETER
      NAME="CREATE_ELEMENTS_BASED_ON_INPUT"
      FIELD="CHECK"
      NUM_ROW="20"
	 SHOW_IF="(DIMENSION_ACTION != 'DELETE')"

    >
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER
      	NAME="DIMENSION_TYPE"
	      FIELD="CLOSED_LIST"
      	NUM_ROW="26"
		SHOW_IF="(DIMENSION_ACTION == 'NONE') AND (CREATE_ELEMENTS_BASED_ON_INPUT == 'true')"
    	>
	<ITEMS DEFAULT="NORMAL">
        <ITEM NAME="NORMAL" VALUE="NORMAL" />
        <ITEM NAME="USER_INFO" VALUE="USER_INFO"/>
		<ITEM NAME="SYSTEM" VALUE="SYSTEM"/>
		<ITEM NAME="ATTRIBUTE" VALUE="ATTRIBUTE"/>
      </ITEMS>
    </PARAMETER>

	<PARAMETER
      	NAME="COMMIT_SIZE"
	      FIELD="TEXT"
      	NUM_ROW="26"
		SHOW_IF="(CREATE_ELEMENTS_BASED_ON_INPUT == 'true') AND (DIMENSION_ACTION != 'DELETE')"

    	><DEFAULT>1000</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DELETE_ALL_ELEMENTS_BEFORE"
      FIELD="CHECK"
      NUM_ROW="20"
	SHOW="false"
	SHOW_IF="CREATE_ELEMENTS_BASED_ON_INPUT == 'true'"
    >
	  <DEFAULT>false</DEFAULT>
    </PARAMETER>



    <PARAMETER
      NAME="CREATE_CONSOLIDATIONS_BASED_ON_INPUT"
      FIELD="CHECK"
      NUM_ROW="25"
	SHOW="false"
	SHOW_IF="CREATE_ELEMENTS_BASED_ON_INPUT == 'true'"
    >
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>



	<PARAMETER NAME="HIERACHIE_NONE" FIELD="RADIO" NUM_ROW="27" GROUP="HIERACHIE_INPUT_TYPE" SHOW_IF="(CREATE_ELEMENTS_BASED_ON_INPUT == 'true') AND (DIMENSION_ACTION != 'DELETE')"
>
		<DEFAULT>true</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="HIERACHIE_NORMAL" FIELD="RADIO" NUM_ROW="28" GROUP="HIERACHIE_INPUT_TYPE" SHOW_IF="(CREATE_ELEMENTS_BASED_ON_INPUT == 'true') AND (DIMENSION_ACTION != 'DELETE')"
>
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="SELF_REFERENCED" FIELD="RADIO" NUM_ROW="29" GROUP="HIERACHIE_INPUT_TYPE" SHOW_IF="(CREATE_ELEMENTS_BASED_ON_INPUT == 'true') AND (DIMENSION_ACTION != 'DELETE')"
>
		<DEFAULT>false</DEFAULT>
	</PARAMETER>


    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="30"
	SHOW_IF="(CREATE_ELEMENTS_BASED_ON_INPUT == 'true') AND (DIMENSION_ACTION != 'DELETE')"
	GROUP="HIERACHIE_INPUT_TYPE"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
		  NAME="ELEMENT_HIERACHIE"
		  FIELD="TABLE"
		  REQUIRED="false"
		  NUM_ROW="35"
		  NB_LINES="5"
	SHOW_IF="(HIERACHIE_NONE == 'true' OR HIERACHIE_NORMAL == 'true' ) and (CREATE_ELEMENTS_BASED_ON_INPUT == 'true') AND (DIMENSION_ACTION != 'DELETE')"
	GROUP="HIERACHIE_INPUT_TYPE"
		>
		  <ITEMS>
			<ITEM NAME="SOURCE_COLUMN" FIELD="COLUMN_LIST" CONTEXT="SCHEMA"/>
			<ITEM NAME="ELEMENT_TYPE" FIELD="CLOSED_LIST">
				<ITEMS DEFAULT="ELEMENT_NUMERIC">
					<ITEM NAME="ELEMENT_NUMERIC" VALUE="ELEMENT_NUMERIC" />
					<ITEM NAME="ELEMENT_STRING" VALUE="ELEMENT_STRING" />
				</ITEMS>
			</ITEM>
			<ITEM NAME="CREATION_MODE" FIELD="CLOSED_LIST">
				<ITEMS DEFAULT="MODE_ADD_OR_UPDATE">
					<ITEM NAME="MODE_ADD" VALUE="MODE_ADD" />
					<ITEM NAME="MODE_FORCE_ADD" VALUE="MODE_FORCE_ADD" />
					<ITEM NAME="MODE_UPDATE" VALUE="MODE_UPDATE" />
					<ITEM NAME="MODE_ADD_OR_UPDATE" VALUE="MODE_ADD_OR_UPDATE" />
					<ITEM NAME="DELETE" VALUE="DELETE" SHOW_IF="(HIERACHIE_NONE == 'true')" />
				</ITEMS>
			</ITEM>

		  </ITEMS>
	</PARAMETER>

	<PARAMETER
      	NAME="ELEMENT_TYPE"
	      FIELD="CLOSED_LIST"
      	NUM_ROW="34"
		SHOW_IF="(SELF_REFERENCED == 'true') and (CREATE_ELEMENTS_BASED_ON_INPUT == 'true') AND (DIMENSION_ACTION != 'DELETE')"
	GROUP="HIERACHIE_INPUT_TYPE"
	>
	<ITEMS DEFAULT="ELEMENT_NUMERIC">
		<ITEM NAME="ELEMENT_NUMERIC" VALUE="ELEMENT_NUMERIC" />
		<ITEM NAME="ELEMENT_STRING" VALUE="ELEMENT_STRING" />
		</ITEMS>
    </PARAMETER>

	<PARAMETER
      	NAME="CREATION_MODE"
	      FIELD="CLOSED_LIST"
      	NUM_ROW="34"
		 SHOW_IF="(SELF_REFERENCED == 'true') and (CREATE_ELEMENTS_BASED_ON_INPUT == 'true') AND (DIMENSION_ACTION != 'DELETE')"
	GROUP="HIERACHIE_INPUT_TYPE"
	    	>
		<ITEMS DEFAULT="MODE_ADD_OR_UPDATE">
			<ITEM NAME="MODE_ADD" VALUE="MODE_ADD" />
			<ITEM NAME="MODE_FORCE_ADD" VALUE="MODE_FORCE_ADD" />
			<ITEM NAME="MODE_UPDATE" VALUE="MODE_UPDATE" />
			<ITEM NAME="MODE_ADD_OR_UPDATE" VALUE="MODE_ADD_OR_UPDATE" />
		</ITEMS>

    </PARAMETER>

    <PARAMETER
		  NAME="ELEMENT_HIERACHIE_SELFREFERENCED"
		  FIELD="TABLE"
		  REQUIRED="false"
		  NUM_ROW="35"
		  NB_LINES="5"
	SHOW_IF="(SELF_REFERENCED == 'true') and (CREATE_ELEMENTS_BASED_ON_INPUT == 'true') AND (DIMENSION_ACTION != 'DELETE')"
	GROUP="HIERACHIE_INPUT_TYPE"
		>
		  <ITEMS>


			<ITEM NAME="SOURCE_COLUMN" FIELD="COLUMN_LIST" CONTEXT="SCHEMA"/>
			<ITEM NAME="ELEMENT_HIERACHIE_TYPE" FIELD="CLOSED_LIST">
				<ITEMS>
					<ITEM NAME="ELEMENT_PARENT" VALUE="ELEMENT_PARENT" />
					<ITEM NAME="ELEMENT_CHILD" VALUE="ELEMENT_CHILD" />
					<ITEM NAME="ELEMENT_CHILD_FACTOR" VALUE="ELEMENT_CHILD_FACTOR" />
				</ITEMS>
			</ITEM>


		  </ITEMS>
	</PARAMETER>

  </PARAMETERS>

  <ADVANCED_PARAMETERS>
	<PARAMETER NAME="DEPLOY_PALO_LIBS" FIELD="CHECK" NUM_ROW="200" SHOW="false">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

  </ADVANCED_PARAMETERS>
    <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="paloIX3" MODULE="paloIX3.jar" MVN="mvn:org.talend.libraries/paloIX3/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.palo/lib/paloIX3.jar" REQUIRED="true" />
      <IMPORT NAME="httpclient-4.5.12" MODULE="httpclient-4.5.12.jar" MVN="mvn:org.apache.httpcomponents/httpclient/4.5.12" REQUIRED="true" />
      <IMPORT NAME="httpmime-4.1.2" MODULE="httpmime-4.1.2.jar" MVN="mvn:org.talend.libraries/httpmime-4.1.2/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.apache.http/lib/httpmime-4.1.2.jar" REQUIRED="true" />
      <IMPORT NAME="httpcore-4.4.13" MODULE="httpcore-4.4.13.jar" MVN="mvn:org.apache.httpcomponents/httpcore/4.4.13" REQUIRED="true" />
      <IMPORT NAME="commons-logging-1.2" MODULE="commons-logging-1.2.jar" MVN="mvn:commons-logging/commons-logging/1.2" REQUIRED="true" />
      <IMPORT NAME="commons-codec-1.14" MODULE="commons-codec-1.14.jar" MVN="mvn:commons-codec/commons-codec/1.14" REQUIRED="true" />
      <IMPORT NAME="Talend_CSV" MODULE="talendcsv.jar" MVN="mvn:org.talend.libraries/talendcsv/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.csv/lib/talendcsv.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>

    <RETURNS>
    <RETURN NAME="DIMENSIONNAME" TYPE="id_String" AVAILABILITY="AFTER"/>
  </RETURNS>


</COMPONENT>
