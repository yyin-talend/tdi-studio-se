<COMPONENT>
	<HEADER
	    PLATEFORM="ALL"
	    SERIAL=""
	    VERSION="0.101"
	    STATUS="ALPHA"
	    COMPATIBILITY="ALL"
	    AUTHOR="Talend"
	    RELEASE_DATE="20050320A"
	    STARTABLE="true"
	    LOG4J_ENABLED="true"
	>
    	<SIGNATURE />
	</HEADER>

  <FAMILIES>
    <FAMILY>Databases/DB Specifics/MS_SQL_Server</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL/>
	</DOCUMENTATION>

  	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="COMPONENT_OK"/>
	    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
	    <CONNECTOR CTYPE="RUN_IF"/>
  	</CONNECTORS>

	<PARAMETERS>
		
		<!-- Oracle -->
		<PARAMETER 
			NAME="CONNECTION" 
			FIELD="COMPONENT_LIST"
			REQUIRED="true" FILTER="tMSSqlConnection" 
			NUM_ROW="5"
			DYNAMIC_SETTINGS="true"
		/>
		
		<PARAMETER 
			NAME="TABLE_NAME" 
			FIELD="TEXT" 
			NUM_ROW="7"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

	</PARAMETERS>
  
	<ADVANCED_PARAMETERS/>	

  	<CODEGENERATION/>

 	<RETURNS>
 		<RETURN NAME="COLUMN_NAME" TYPE="id_String" AVAILABILITY="FLOW" />
 		<RETURN NAME="COLUMN_DEFAULT" TYPE="id_String" AVAILABILITY="FLOW" />
 		<RETURN NAME="IS_NULLABLE" TYPE="id_String" AVAILABILITY="FLOW" />
 		<RETURN NAME="DATA_TYPE" TYPE="id_String" AVAILABILITY="FLOW" />
 		<RETURN NAME="CHARACTER_MAXIMUM_LENGTH" TYPE="id_String" AVAILABILITY="FLOW" />
 		<RETURN NAME="CHARACTER_OCTET_LENGTH" TYPE="id_String" AVAILABILITY="FLOW" />
 		<RETURN NAME="NUMERIC_PRECISION" TYPE="id_String" AVAILABILITY="FLOW" />
 		<RETURN NAME="NUMERIC_PRECISION_RADIX" TYPE="id_String" AVAILABILITY="FLOW" />
 		<RETURN NAME="NUMERIC_SCALE" TYPE="id_String" AVAILABILITY="FLOW" />
 		<RETURN NAME="DATETIME_PRECISION" TYPE="id_String" AVAILABILITY="FLOW" />
 		<RETURN NAME="COLUMN_KEY" TYPE="id_String" AVAILABILITY="FLOW" />
		<RETURN NAME="NB_COLUMN" TYPE="id_Integer" AVAILABILITY="AFTER" />
 	</RETURNS>
</COMPONENT>
