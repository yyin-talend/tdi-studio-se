<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT> 
	<HEADER AUTHOR="Talend" 
			COMPATIBILITY="ALL" 
			DATA_AUTO_PROPAGATE="false"
			STARTABLE="false"
			PLATEFORM="ALL" 
			RELEASE_DATE="20080229A" 
			SERIAL="" 
			STATUS="ALPHA" 
			VERSION="0.102"
			PARTITIONING="AUTO"
			> 
		<SIGNATURE/> 
	</HEADER>
	
	<FAMILIES> 
		<FAMILY>File/NamedPipe</FAMILY> 
	</FAMILIES>
	
	<DOCUMENTATION> 
		<URL/> 
	</DOCUMENTATION>  
	
	<CONNECTORS> 
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>  
		<CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" MAX_OUTPUT="1" />  
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK"/>
		<CONNECTOR CTYPE="COMPONENT_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>
	
	<PARAMETERS>
	
		<PARAMETER
			NAME="USE_EXISTING_PIPE"
			FIELD="CHECK"
			NUM_ROW="10"
			REQUIRED="true"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>    

		<PARAMETER
			NAME="PIPE"
			FIELD="COMPONENT_LIST"
			REQUIRED="true" FILTER="tNamedPipeOpen"
			NUM_ROW="10"
			SHOW_IF="USE_EXISTING_PIPE == 'true'"
			DYNAMIC_SETTINGS="true"
		/>	
		
		<PARAMETER
			NAME="NAMEDPIPE_NAME"
			FIELD="TEXT"
			NUM_ROW="20"
			SHOW_IF="USE_EXISTING_PIPE == 'false'"
			REQUIRED="true"
		>
			<DEFAULT>"tPipe"</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="ROWSEPARATOR"
			FIELD="TEXT"
			NUM_ROW="30"
			REQUIRED="true"
			REPOSITORY_VALUE="ROW_SEPARATOR"
		>
			<DEFAULT>"\n"</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="FIELDSEPARATOR"
			FIELD="TEXT"
			NUM_ROW="30"
			REQUIRED="true"
			REPOSITORY_VALUE="FIELD_SEPARATOR"
		>
			<DEFAULT>"|"</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="CSV"
			FIELD="CHECK"
			NUM_ROW="40"
			REQUIRED="true"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="ESCAPE_CHAR"
			FIELD="TEXT"
			NUM_ROW="45"
			REQUIRED="true"
			REPOSITORY_VALUE="ESCAPE_CHAR"
			SHOW_IF="CSV == 'true'"
		>
			<DEFAULT>"\\"</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="TEXT_ENCLOSURE"
			FIELD="TEXT"
			NUM_ROW="45"
			REQUIRED="true"
			REPOSITORY_VALUE="TEXT_ENCLOSURE"
			SHOW_IF="CSV == 'true'"
		>
			<DEFAULT>"\""</DEFAULT>
		</PARAMETER>         

		<PARAMETER
			NAME="TEXT_ENCLOSURE_OPTION"
			FIELD="CLOSED_LIST"
			NUM_ROW="45"
			REQUIRED="true"
			SHOW_IF="CSV == 'true'"
		>
			<ITEMS DEFAULT="TEXT_ONLY">
				<ITEM NAME="TEXT_ONLY" VALUE="TEXT_ONLY" />
				<ITEM NAME="ALL" VALUE="ALL"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="SCHEMA"
			FIELD="SCHEMA_TYPE"
			REQUIRED="true"
			NUM_ROW="50"
		>
			<DEFAULT/> 
		</PARAMETER>
	
		<PARAMETER
			NAME="DELETE_IF_EXISTS"
			FIELD="CHECK"
			NUM_ROW="60"
			REQUIRED="true"
		>
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

	</PARAMETERS>
	
	
	
	
	<ADVANCED_PARAMETERS> 	
		<PARAMETER
			NAME="NULL_TEXT"
			FIELD="TEXT"
			NUM_ROW="10"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="BOOLEAN_TYPE"
			FIELD="CLOSED_LIST"
			NUM_ROW="20"
			REQUIRED="true"
		>
			<ITEMS DEFAULT="1_0">
				<ITEM NAME="1_0" VALUE="1_0" />
				<ITEM NAME="T_F" VALUE="T_F"/>
				<ITEM NAME="Y_N" VALUE="Y_N"/>
				<ITEM NAME="TRUE_FALSE" VALUE="TRUE_FALSE"/>
				<ITEM NAME="YES_NO" VALUE="YES_NO"/>
			</ITEMS>
		</PARAMETER>

	</ADVANCED_PARAMETERS>
	
	<CODEGENERATION> 
		<IMPORTS> 
			<IMPORT MODULE="namedpipe-1.0.jar" MVN="mvn:org.talend.libraries/namedpipe-1.0/6.0.0"  NAME="namedpipe-1.0.jar" REQUIRED="true"/> 
			<IMPORT MODULE="namedpipe_jni_64bit.dll" MVN="mvn:org.talend.libraries/namedpipe_jni_64bit/6.0.0"  NAME="namedpipe_jni_64bit.dll" REQUIRED="false"/> 
			<IMPORT MODULE="namedpipe_jni.dll" MVN="mvn:org.talend.libraries/namedpipe_jni/6.0.0"  NAME="namedpipe_jni.dll" REQUIRED="false"/> 
		</IMPORTS> 
	</CODEGENERATION>
	
	<RETURNS> 
		<RETURN AVAILABILITY="FLOW" NAME="NB_LINE" TYPE="id_Integer"/>
		<RETURN AVAILABILITY="FLOW" NAME="PIPE_NAME" TYPE="id_String"/>
		<RETURN AVAILABILITY="FLOW" NAME="PIPE_NATIVE_NAME" TYPE="id_String"/>
		<RETURN AVAILABILITY="FLOW" NAME="PIPE_OUTPUTSTREAM" TYPE="id_Object"/>
	</RETURNS> 
</COMPONENT>
