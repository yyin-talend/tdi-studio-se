<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    PARTITIONING="NONE"
    LOG4J_ENABLED="true"
  >
      <SIGNATURE></SIGNATURE>
  </HEADER>
   <FAMILIES>
    <FAMILY>Business/Salesforce</FAMILY>
	<FAMILY>Cloud/Salesforce</FAMILY>
  </FAMILIES>
  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>
   <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>
  <PARAMETERS>
  	<PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="20" REQUIRED="true">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/sforce_bulk.csv"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="APPEND" FIELD="CHECK" REQUIRED="true" NUM_ROW="30">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="50">
      <DEFAULT/>
    </PARAMETER>
    <PARAMETER NAME="IGNORE_NULL" FIELD="CHECK" REQUIRED="true" NUM_ROW="60">
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>
   </PARAMETERS>

   <ADVANCED_PARAMETERS>
	<PARAMETER NAME="UPSERT_RELATION" FIELD="TABLE" NUM_ROW="10" NB_LINES="5">
		<ITEMS>
			<ITEM NAME="COLUMN_NAME" FIELD="PREV_COLUMN_LIST"/>
			<ITEM NAME="LOOKUP_FIELD_NAME" />
			<ITEM NAME="LOOKUP_FIELD_EXTERNAL_ID_NAME" />
			<ITEM NAME="POLYMORPHIC" FIELD="CHECK"/>
			<ITEM NAME="LOOKUP_FIELD_MODULE_NAME" />
		</ITEMS>
	</PARAMETER>
   </ADVANCED_PARAMETERS>
   <CODEGENERATION>
	  <IMPORTS>
	    <IMPORT NAME="Talend_CSV" MODULE="talendcsv.jar" MVN="mvn:org.talend.libraries/talendcsv/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.csv/lib/talendcsv.jar" REQUIRED="true" />
	  </IMPORTS>
	</CODEGENERATION>
	<RETURNS>
     <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>
