    sourceTable_<%=cid%> + " t, " + tableName_<%=cid%> + " d WHERE <%=joinClauseString%> AND (<%=whereClauseString%>) AND d.<%=endDateField%> = " + changeDateTime_<%=cid%>;
    java.sql.Statement stmtType2Insert_<%=cid%> = conn_<%=cid%>.createStatement();
