<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20141229A"
    STARTABLE="false"
    DATA_AUTO_PROPAGATE="true"
    HAS_CONDITIONAL_OUTPUTS="true"
    PARALLELIZE="true"
    PARTITIONING="AUTO"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/Amazon/Aurora</FAMILY>
    <FAMILY>Cloud/Amazon/Aurora</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW"
          NOT_SHOW_IF="((((DATA_ACTION == 'INSERT') AND (EXTENDINSERT == 'true')) OR ((DATA_ACTION == 'UPDATE') AND (USE_BATCH_SIZE == 'true'))
                  OR ((DATA_ACTION == 'DELETE') AND (USE_BATCH_SIZE == 'true'))) OR (DIE_ON_ERROR == 'true'))"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:AMAZON_AURORA"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    />

    <PARAMETER
      NAME="DB_VERSION"
      FIELD="CLOSED_LIST"
      NUM_ROW="10"
      READONLY="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      SHOW="false">
    <ITEMS DEFAULT="MYSQL_5">
      <ITEM NAME="MYSQL_5" VALUE="MYSQL_5" />
    </ITEMS>
  </PARAMETER>

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="15"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      REQUIRED="true" FILTER="tAmazonAuroraConnection"
      NUM_ROW="15"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
      DYNAMIC_SETTINGS="true"/>

  <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="20"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>MySQL</DEFAULT>
    </PARAMETER>
    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="20"
      REPOSITORY_VALUE="SERVER_NAME"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="20"
      REPOSITORY_VALUE="PORT"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>"3306"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="20"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

  <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="30"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="DBTABLE"
      NUM_ROW="35"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="40"
      NOT_SHOW_IF="PARALLELIZE == 'true'"
    >
      <ITEMS DEFAULT="NONE">
        <ITEM NAME="NONE" VALUE="NONE" />
        <ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE"/>
        <ITEM NAME="CREATE" VALUE="CREATE"/>
        <ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS"/>
        <ITEM NAME="DROP_IF_EXISTS_AND_CREATE" VALUE="DROP_IF_EXISTS_AND_CREATE" />
        <ITEM NAME="CLEAR" VALUE="CLEAR"/>
        <ITEM NAME="TRUNCATE" VALUE="TRUNCATE"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="DATA_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="40"
    >
      <ITEMS DEFAULT="INSERT">
        <ITEM NAME="INSERT" VALUE="INSERT" />
        <ITEM NAME="UPDATE" VALUE="UPDATE"/>
        <ITEM NAME="INSERT_OR_UPDATE" VALUE="INSERT_OR_UPDATE"/>
        <ITEM NAME="UPDATE_OR_INSERT" VALUE="UPDATE_OR_INSERT"/>
        <ITEM NAME="DELETE" VALUE="DELETE"/>
        <ITEM NAME="REPLACE" VALUE="REPLACE"/>
        <ITEM NAME="INSERT_ON_DUPLICATE_KEY_UPDATE" VALUE="INSERT_ON_DUPLICATE_KEY_UPDATE"/>
        <ITEM NAME="INSERT_IGNORE" VALUE="INSERT_IGNORE"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA_FLOW"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="45"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA_REJECT"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="45"
      CONTEXT="REJECT"
      SHOW="true">
      <TABLE READONLY="true">
        <COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>

  <PARAMETER
    NAME="NOTE"
      FIELD="LABEL"
      NUM_ROW="63"
    GROUP="DATASOURCE"
    SHOW_IF="USE_EXISTING_CONNECTION=='false'"
    >
      <DEFAULT>This option only applies when deploying and running in the Talend Runtime</DEFAULT>
    </PARAMETER>
  <PARAMETER
      NAME="SPECIFY_DATASOURCE_ALIAS"
      FIELD="CHECK"
      NUM_ROW="64"
      REQUIRED="true"
      GROUP="DATASOURCE"
      SHOW_IF="USE_EXISTING_CONNECTION=='false'"
    >
    </PARAMETER>
  <PARAMETER
      NAME="DATASOURCE_ALIAS"
      FIELD="TEXT"
      NUM_ROW="65"
      REQUIRED="true"
      GROUP="DATASOURCE"
      SHOW_IF="(SPECIFY_DATASOURCE_ALIAS=='true' AND USE_EXISTING_CONNECTION=='false')"
    >
  <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DIE_ON_ERROR"
      FIELD="CHECK"
      NUM_ROW="100"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="120"
      SHOW="false"
    >
      <DEFAULT>mysql_id</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <ADVANCED_PARAMETERS>
    <PARAMETER
      NAME="PROPERTIES"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PROPERTIES_STRING"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
    <DEFAULT>"noDatetimeStringSync=true"</DEFAULT>
  </PARAMETER>
  <PARAMETER
      NAME="EXTENDINSERT"
      FIELD="CHECK"
      NUM_ROW="8"
      SHOW_IF="DATA_ACTION == 'INSERT'"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
    <PARAMETER
      NAME="NOTE"
      FIELD="LABEL"
      NUM_ROW="8"
      SHOW_IF="DATA_ACTION == 'INSERT'"
    >
      <DEFAULT>(Not compatible with reject links)</DEFAULT>
    </PARAMETER>
  <PARAMETER
      NAME="NB_ROWS_PER_INSERT"
      FIELD="TEXT"
      NUM_ROW="8"
      REQUIRED="true"
      SHOW_IF="(EXTENDINSERT == 'true' AND DATA_ACTION == 'INSERT')"
    >
      <DEFAULT>100</DEFAULT>
    </PARAMETER>

  <PARAMETER
    NAME="USE_BATCH_SIZE"
    FIELD="CHECK"
    NUM_ROW="8"
    SHOW_IF="(DB_VERSION =='MYSQL_5') AND ((DATA_ACTION == 'UPDATE') OR (DATA_ACTION == 'DELETE')) "
  >
    <DEFAULT>true</DEFAULT>
  </PARAMETER>

  <PARAMETER
    NAME="BATCH_SIZE"
    FIELD="TEXT"
    NUM_ROW="8"
    SHOW_IF="(USE_BATCH_SIZE == 'true' AND DB_VERSION =='MYSQL_5') AND ((DATA_ACTION == 'UPDATE') OR (DATA_ACTION == 'DELETE')) "
  >
  <DEFAULT>10000</DEFAULT>
  </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="10"
      REQUIRED="true"
      SHOW="false"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="COMMIT_EVERY"
      FIELD="TEXT"
      NUM_ROW="10"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>10000</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ADD_COLS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="11"
      NB_LINES="3"
    >
      <ITEMS>

        <ITEM NAME="NAME" VALUE='"colname"'/>
    <ITEM NAME="DATATYPE" VALUE="VARCHAR(50)" RAW="true" NOT_SHOW_IF="(TABLE_ACTION=='NONE') or (TABLE_ACTION=='CLEAR')"/>
        <ITEM NAME="SQL" VALUE='"now()"' />

        <ITEM NAME="POS" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="BEFORE">
            <ITEM NAME="BEFORE" VALUE="BEFORE"/>
            <ITEM NAME="AFTER" VALUE="AFTER"/>
            <ITEM NAME="REPLACE" VALUE="REPLACE"/>
          </ITEMS>
        </ITEM>

        <ITEM NAME="REFCOL" FIELD="COLUMN_LIST" />

      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="USE_FIELD_OPTIONS"
      FIELD="CHECK"
      NUM_ROW="12"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELD_OPTIONS"
      FIELD="TABLE"
      REQUIRED="true"
      NUM_ROW="13"
      NB_LINES="5"
      SHOW_IF="USE_FIELD_OPTIONS == 'true'"
    >
      <ITEMS BASED_ON_SCHEMA="true">
        <ITEM NAME="UPDATE_KEY" VALUE="false" FIELD="CHECK" />
        <ITEM NAME="DELETE_KEY" VALUE="false" FIELD="CHECK" />
        <ITEM NAME="UPDATABLE"  VALUE="true"  FIELD="CHECK" />
        <ITEM NAME="INSERTABLE" VALUE="true"  FIELD="CHECK" />
      </ITEMS>
    </PARAMETER>

     <PARAMETER
      NAME="USE_HINT_OPTIONS"
      FIELD="CHECK"
      NUM_ROW="15"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="HINT_OPTIONS"
      FIELD="TABLE"
      REQUIRED="true"
      NUM_ROW="16"
      NB_LINES="5"
      SHOW_IF="USE_HINT_OPTIONS == 'true'"
    >
      <ITEMS>
        <ITEM NAME="HINT" VALUE="&quot;/*! */&quot;" FIELD="TEXT" />
        <ITEM NAME="POSITION" VALUE="AFTER" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="AFTER">
            <ITEM NAME="AFTER"  VALUE="AFTER"/>
          </ITEMS>
    </ITEM>
        <ITEM NAME="SQL_STMT"  VALUE="true"  FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="INSERT">
            <ITEM NAME="INSERT"  VALUE="INSERT"/>
        <ITEM NAME="UPDATE"  VALUE="UPDATE"/>
        <ITEM NAME="DELETE"  VALUE="DELETE"/>
          </ITEMS>
    </ITEM>
      </ITEMS>
    </PARAMETER>
    <PARAMETER
      NAME="ENABLE_DEBUG_MODE"
      FIELD="CHECK"
      NUM_ROW="17">
    <DEFAULT>false</DEFAULT>
  </PARAMETER>

    <PARAMETER
      NAME="ON_DUPLICATE_KEY_UPDATE"
      FIELD="CHECK"
      NUM_ROW="20"
      SHOW_IF="(DATA_ACTION == 'INSERT')"
    >
    <DEFAULT>false</DEFAULT>
  </PARAMETER>

  <PARAMETER
      NAME="DUPLICATED_KEYS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="20"
      NB_LINES="2"
      SHOW_IF="(DATA_ACTION == 'INSERT') AND (ON_DUPLICATE_KEY_UPDATE == 'true') "
    >
      <ITEMS>
        <ITEM NAME="DUPLICATED_KEY" VALUE='"colname"'/>
    <ITEM NAME="DUPLICATED_VALUE" VALUE='""'/>
      </ITEMS>
    </PARAMETER>

  <PARAMETER
    NAME="SUPPORT_NULL_WHERE"
    FIELD="CHECK"
    NUM_ROW="30"
    SHOW_IF="(DATA_ACTION == 'UPDATE') OR (DATA_ACTION == 'INSERT_OR_UPDATE')
        OR (DATA_ACTION == 'UPDATE_OR_INSERT') OR (DATA_ACTION == 'DELETE') ">
      <DEFAULT>false</DEFAULT>
  </PARAMETER>

  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <TEMPLATES INPUT="in" OUTPUT="out">
    <TEMPLATE NAME="in" COMPONENT="tDummyRow">
       <LINK_TO NAME="out" CTYPE="FLOW"/>
    </TEMPLATE>
      <TEMPLATE NAME="out" COMPONENT="tMysqlOutput"/>
      <TEMPLATE_PARAM SOURCE="self.PROPERTY" TARGET="out.PROPERTY"/>
      <TEMPLATE_PARAM SOURCE="self.DB_VERSION" TARGET="out.DB_VERSION"/>
      <TEMPLATE_PARAM SOURCE="self.USE_EXISTING_CONNECTION" TARGET="out.USE_EXISTING_CONNECTION"/>
      <TEMPLATE_PARAM SOURCE="self.CONNECTION" TARGET="out.CONNECTION"/>
      <TEMPLATE_PARAM SOURCE="self.TYPE" TARGET="out.TYPE"/>
      <TEMPLATE_PARAM SOURCE="self.HOST" TARGET="out.HOST"/>
      <TEMPLATE_PARAM SOURCE="self.PORT" TARGET="out.PORT"/>
      <TEMPLATE_PARAM SOURCE="self.DBNAME" TARGET="out.DBNAME"/>
      <TEMPLATE_PARAM SOURCE="self.USER" TARGET="out.USER"/>
      <TEMPLATE_PARAM SOURCE="self.PASS" TARGET="out.PASS"/>
      <TEMPLATE_PARAM SOURCE="self.TABLE" TARGET="out.TABLE"/>
      <TEMPLATE_PARAM SOURCE="self.TABLE_ACTION" TARGET="out.TABLE_ACTION"/>
      <TEMPLATE_PARAM SOURCE="self.NONE" TARGET="out.NONE"/>
      <TEMPLATE_PARAM SOURCE="self.DROP_CREATE" TARGET="out.DROP_CREATE"/>
      <TEMPLATE_PARAM SOURCE="self.CREATE" TARGET="out.CREATE"/>
      <TEMPLATE_PARAM SOURCE="self.CREATE_IF_NOT_EXISTS" TARGET="out.CREATE_IF_NOT_EXISTS"/>
      <TEMPLATE_PARAM SOURCE="self.DROP_IF_EXISTS_AND_CREATE" TARGET="out.DROP_IF_EXISTS_AND_CREATE"/>
      <TEMPLATE_PARAM SOURCE="self.CLEAR" TARGET="out.CLEAR"/>
      <TEMPLATE_PARAM SOURCE="self.TRUNCATE" TARGET="out.TRUNCATE"/>
      <TEMPLATE_PARAM SOURCE="self.DATA_ACTION" TARGET="out.DATA_ACTION"/>
      <TEMPLATE_PARAM SOURCE="self.INSERT" TARGET="out.INSERT"/>
      <TEMPLATE_PARAM SOURCE="self.UPDATE" TARGET="out.UPDATE"/>
      <TEMPLATE_PARAM SOURCE="self.INSERT_OR_UPDATE" TARGET="out.INSERT_OR_UPDATE"/>
      <TEMPLATE_PARAM SOURCE="self.UPDATE_OR_INSERT" TARGET="out.UPDATE_OR_INSERT"/>
      <TEMPLATE_PARAM SOURCE="self.DELETE" TARGET="out.DELETE"/>
      <TEMPLATE_PARAM SOURCE="self.REPLACE" TARGET="out.REPLACE"/>
      <TEMPLATE_PARAM SOURCE="self.INSERT_ON_DUPLICATE_KEY_UPDATE" TARGET="out.INSERT_ON_DUPLICATE_KEY_UPDATE"/>
      <TEMPLATE_PARAM SOURCE="self.INSERT_IGNORE" TARGET="out.INSERT_IGNORE"/>
      <TEMPLATE_PARAM SOURCE="self.SCHEMA_FLOW" TARGET="out.SCHEMA_FLOW"/>
      <TEMPLATE_PARAM SOURCE="self.SCHEMA_REJECT" TARGET="out.SCHEMA_REJECT"/>
      <TEMPLATE_PARAM SOURCE="self.SPECIFY_DATASOURCE_ALIAS" TARGET="out.SPECIFY_DATASOURCE_ALIAS"/>
      <TEMPLATE_PARAM SOURCE="self.DATASOURCE_ALIAS" TARGET="out.DATASOURCE_ALIAS"/>
      <TEMPLATE_PARAM SOURCE="self.DIE_ON_ERROR" TARGET="out.DIE_ON_ERROR"/>
      <TEMPLATE_PARAM SOURCE="self.MAPPING" TARGET="out.MAPPING"/>
      <TEMPLATE_PARAM SOURCE="self.PROPERTIES" TARGET="out.PROPERTIES"/>
      <TEMPLATE_PARAM SOURCE="self.EXTENDINSERT" TARGET="out.EXTENDINSERT"/>
      <TEMPLATE_PARAM SOURCE="self.NB_ROWS_PER_INSERT" TARGET="out.NB_ROWS_PER_INSERT"/>
      <TEMPLATE_PARAM SOURCE="self.USE_BATCH_SIZE" TARGET="out.USE_BATCH_SIZE"/>
      <TEMPLATE_PARAM SOURCE="self.BATCH_SIZE" TARGET="out.BATCH_SIZE"/>
      <TEMPLATE_PARAM SOURCE="self.ENCODING" TARGET="out.ENCODING"/>
      <TEMPLATE_PARAM SOURCE="self.COMMIT_EVERY" TARGET="out.COMMIT_EVERY"/>
      <TEMPLATE_PARAM SOURCE="self.ADD_COLS" TARGET="out.ADD_COLS"/>
      <TEMPLATE_PARAM SOURCE="self.NAME" TARGET="out.NAME"/>
      <TEMPLATE_PARAM SOURCE="self.DATATYPE" TARGET="out.DATATYPE"/>
      <TEMPLATE_PARAM SOURCE="self.SQL" TARGET="out.SQL"/>
      <TEMPLATE_PARAM SOURCE="self.POS" TARGET="out.POS"/>
      <TEMPLATE_PARAM SOURCE="self.BEFORE" TARGET="out.BEFORE"/>
      <TEMPLATE_PARAM SOURCE="self.AFTER" TARGET="out.AFTER"/>
      <TEMPLATE_PARAM SOURCE="self.REFCOL" TARGET="out.REFCOL"/>
      <TEMPLATE_PARAM SOURCE="self.USE_FIELD_OPTIONS" TARGET="out.USE_FIELD_OPTIONS"/>
      <TEMPLATE_PARAM SOURCE="self.FIELD_OPTIONS" TARGET="out.FIELD_OPTIONS"/>
      <TEMPLATE_PARAM SOURCE="self.UPDATE_KEY" TARGET="out.UPDATE_KEY"/>
      <TEMPLATE_PARAM SOURCE="self.DELETE_KEY" TARGET="out.DELETE_KEY"/>
      <TEMPLATE_PARAM SOURCE="self.UPDATABLE" TARGET="out.UPDATABLE"/>
      <TEMPLATE_PARAM SOURCE="self.INSERTABLE" TARGET="out.INSERTABLE"/>
      <TEMPLATE_PARAM SOURCE="self.USE_HINT_OPTIONS" TARGET="out.USE_HINT_OPTIONS"/>
      <TEMPLATE_PARAM SOURCE="self.HINT_OPTIONS" TARGET="out.HINT_OPTIONS"/>
      <TEMPLATE_PARAM SOURCE="self.HINT" TARGET="out.HINT"/>
      <TEMPLATE_PARAM SOURCE="self.POSITION" TARGET="out.POSITION"/>
      <TEMPLATE_PARAM SOURCE="self.SQL_STMT" TARGET="out.SQL_STMT"/>
      <TEMPLATE_PARAM SOURCE="self.ENABLE_DEBUG_MODE" TARGET="out.ENABLE_DEBUG_MODE"/>
      <TEMPLATE_PARAM SOURCE="self.ON_DUPLICATE_KEY_UPDATE" TARGET="out.ON_DUPLICATE_KEY_UPDATE"/>
      <TEMPLATE_PARAM SOURCE="self.DUPLICATED_KEYS" TARGET="out.DUPLICATED_KEYS"/>
      <TEMPLATE_PARAM SOURCE="self.DUPLICATED_KEY" TARGET="out.DUPLICATED_KEY"/>
      <TEMPLATE_PARAM SOURCE="self.DUPLICATED_VALUE" TARGET="out.DUPLICATED_VALUE"/>
      <TEMPLATE_PARAM SOURCE="self.SUPPORT_NULL_WHERE" TARGET="out.SUPPORT_NULL_WHERE"/>
      <TEMPLATE_PARAM SOURCE="self.PARALLELIZE" TARGET="out.PARALLELIZE"/>
      <TEMPLATE_PARAM SOURCE="self.PARALLELIZE_NUMBER" TARGET="out.PARALLELIZE_NUMBER"/>
    </TEMPLATES>
     <IMPORTS>
       <IMPORT NAME="Driver-MysqlConnector" MODULE="mysql-connector-java-5.1.30-bin.jar" MVN="mvn:org.talend.libraries/mysql-connector-java-5.1.30-bin/6.0.0"  REQUIRED_IF="USE_EXISTING_CONNECTION == 'false'" />
     </IMPORTS>
    </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE"          TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_UPDATED"  TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_INSERTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_DELETED"  TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_REJECTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW" SHOW_IF="ENABLE_DEBUG_MODE=='true'"/>
  </RETURNS>
</COMPONENT>
