<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070314A"
    STARTABLE="false"
    DATA_AUTO_PROPAGATE="false"
    HAS_CONDITIONAL_OUTPUTS="true"
    PARALLELIZE="true"
    PARTITIONING="AUTO"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/DB Specifics/Oracle</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW"
    			NOT_SHOW_IF="(DIE_ON_ERROR == 'true') OR (isShow[BATCH_SIZE])"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:ORACLE"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    />

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="15"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      REQUIRED="true" FILTER="tOracleConnection"
      NUM_ROW="15"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
      DYNAMIC_SETTINGS="true"
    />

    <PARAMETER
      NAME="CONNECTION_TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="20"
      REPOSITORY_VALUE="CONNECTION_TYPE"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <ITEMS DEFAULT="ORACLE_SID">
        <ITEM NAME="ORACLE_SID" VALUE="ORACLE_SID" />
        <ITEM NAME="ORACLE_SERVICE_NAME" VALUE="ORACLE_SERVICE_NAME"/>
        <ITEM NAME="ORACLE_OCI" VALUE="ORACLE_OCI"/>
		<ITEM NAME="ORACLE_RAC" VALUE="ORACLE_RAC"/>
		<ITEM NAME="ORACLE_WALLET" VALUE="ORACLE_WALLET"/>
      </ITEMS>
    </PARAMETER>

	<PARAMETER NAME="DB_VERSION" FIELD="CLOSED_LIST" NUM_ROW="25"
		REPOSITORY_VALUE="DB_VERSION" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
		<ITEMS DEFAULT="ORACLE_18">
            <ITEM NAME="ORACLE_18" VALUE="ORACLE_18" />
			<ITEM NAME="ORACLE_12" VALUE="ORACLE_12" />
			<ITEM NAME="ORACLE_11" VALUE="ORACLE_11" />
			<ITEM NAME="ORACLE_10" VALUE="ORACLE_10" />
			<ITEM NAME="ORACLE_9" VALUE="ORACLE_9" />
			<ITEM NAME="ORACLE_8" VALUE="ORACLE_8" />
		</ITEMS>
	</PARAMETER>

	<PARAMETER
		NAME="RAC_URL"
		FIELD="TEXT"
		NUM_ROW="27"
		REPOSITORY_VALUE="RAC_URL"
		REQUIRED="true"
		SHOW_IF="(USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE == 'ORACLE_RAC')"
		>
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="((USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_OCI') and (CONNECTION_TYPE != 'ORACLE_WALLET') and (CONNECTION_TYPE != 'ORACLE_RAC'))"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="((USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_OCI') and (CONNECTION_TYPE != 'ORACLE_WALLET') and (CONNECTION_TYPE != 'ORACLE_RAC'))"
    >
      <DEFAULT>"1521"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>Oracle</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      SHOW_IF="(USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_OCI') and (CONNECTION_TYPE != 'ORACLE_WALLET') and (CONNECTION_TYPE != 'ORACLE_RAC')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="LOCAL_SERVICE_NAME"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      SHOW_IF="(USE_EXISTING_CONNECTION == 'false' and CONNECTION_TYPE == 'ORACLE_OCI')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLESCHEMA"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SCHEMA"
      REQUIRED="false"
      SHOW_IF="(USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="45"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
      SHOW_IF="((USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_WALLET'))"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="45"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
      SHOW_IF="((USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_WALLET'))"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="DBTABLE"
      NUM_ROW="50"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="55"
      NOT_SHOW_IF="PARALLELIZE == 'true'"
    >
      <ITEMS DEFAULT="NONE">
        <ITEM NAME="NONE" VALUE="NONE" />
        <ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE"/>
        <ITEM NAME="CREATE" VALUE="CREATE"/>
        <ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS"/>
        <ITEM NAME="DROP_IF_EXISTS_AND_CREATE" VALUE="DROP_IF_EXISTS_AND_CREATE" />
        <ITEM NAME="CLEAR" VALUE="CLEAR"/>
        <ITEM NAME="TRUNCATE" VALUE="TRUNCATE"/>
        <ITEM NAME="TRUNCATE_REUSE_STORAGE" VALUE="TRUNCATE_REUSE_STORAGE"/>

      </ITEMS>
    </PARAMETER>

	<PARAMETER NAME="NOTE" FIELD="LABEL" NUM_ROW="56"
			NOT_SHOW_IF="(PARALLELIZE == 'true') OR (TABLE_ACTION == 'NONE') OR (USE_EXISTING_CONNECTION == 'false')"
		>
		<DEFAULT>Warning : this component configuration will automatically generate a commit before data insert/update/delete</DEFAULT>
	</PARAMETER>

    <PARAMETER
      NAME="DATA_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="55"
    >
      <ITEMS DEFAULT="INSERT">
        <ITEM NAME="INSERT"           VALUE="INSERT" />
        <ITEM NAME="UPDATE"           VALUE="UPDATE"/>
        <ITEM NAME="INSERT_OR_UPDATE" VALUE="INSERT_OR_UPDATE"/>
        <ITEM NAME="UPDATE_OR_INSERT" VALUE="UPDATE_OR_INSERT"/>
        <ITEM NAME="DELETE"           VALUE="DELETE"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA_FLOW"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="60"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA_REJECT"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="60"
      CONTEXT="REJECT"
      SHOW="true">
      <TABLE READONLY="true">
        <COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>

	<PARAMETER
	  NAME="NOTE"
      FIELD="LABEL"
      NUM_ROW="63"
	  GROUP="DATASOURCE"
	  SHOW_IF="USE_EXISTING_CONNECTION=='false'"
    >
    	<DEFAULT>This option only applies when deploying and running in the Talend Runtime</DEFAULT>
    </PARAMETER>
	<PARAMETER
      NAME="SPECIFY_DATASOURCE_ALIAS"
	  FIELD="CHECK"
      NUM_ROW="64"
      REQUIRED="true"
	  GROUP="DATASOURCE"
	  SHOW_IF="USE_EXISTING_CONNECTION=='false'"
    >
    </PARAMETER>
	<PARAMETER
      NAME="DATASOURCE_ALIAS"
	  FIELD="TEXT"
      NUM_ROW="65"
      REQUIRED="true"
	  GROUP="DATASOURCE"
	  SHOW_IF="(SPECIFY_DATASOURCE_ALIAS=='true' AND USE_EXISTING_CONNECTION=='false')"
    >
	<DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DIE_ON_ERROR"
      FIELD="CHECK"
      NUM_ROW="90"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
	<PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="100"
      SHOW="false"
    >
      <DEFAULT>oracle_id</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="JDBC_URL"
	  FIELD="TEXT"
      NUM_ROW="35"
      REQUIRED="true"
	  SHOW_IF="(USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE == 'ORACLE_WALLET')"
    >
	<DEFAULT>"jdbc:oracle:oci:/@talend"</DEFAULT>
	 </PARAMETER>

  </PARAMETERS>

  <ADVANCED_PARAMETERS>
	<PARAMETER
		NAME="PROPERTIES"
		FIELD="TEXT"
		NUM_ROW="10"
		REPOSITORY_VALUE="PROPERTIES_STRING"
		SHOW_IF="(USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_WALLET')">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="NOTE"
		FIELD="LABEL"
		NUM_ROW="11"
		SHOW_IF="(USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_WALLET')">
		<DEFAULT>*Note: Example for Additional JDBC Parameters: "parameterName1=value1&amp;&amp;parameterName2=value2"</DEFAULT>
	</PARAMETER>

    <PARAMETER
      NAME="USE_ALTERNATE_SCHEMA"
      FIELD="CHECK"
      NUM_ROW="15"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ALTERNATE_SCHEMA"
      FIELD="TEXT"
      NUM_ROW="16"
      REQUIRED="true"
      SHOW_IF="((USE_EXISTING_CONNECTION == 'true') AND (USE_ALTERNATE_SCHEMA == 'true'))"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

  	<PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="20"
      REQUIRED="true"
      SHOW="false"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="COMMIT_EVERY"
      FIELD="TEXT"
      NUM_ROW="30"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>10000</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ADD_COLS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="35"
      NB_LINES="3"
    >
      <ITEMS>
        <ITEM NAME="NAME" VALUE='"colname"' />
		<ITEM NAME="DATATYPE" VALUE="VARCHAR2(50)" RAW="true" NOT_SHOW_IF="(TABLE_ACTION=='NONE') or (TABLE_ACTION=='CLEAR')"/>
        <ITEM NAME="SQL" VALUE='"now()"' />
        <ITEM NAME="POS" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="BEFORE">
            <ITEM NAME="BEFORE" VALUE="BEFORE"/>
            <ITEM NAME="AFTER" VALUE="AFTER"/>
            <ITEM NAME="REPLACE" VALUE="REPLACE"/>
          </ITEMS>
        </ITEM>
        <ITEM NAME="REFCOL" FIELD="COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="USE_FIELD_OPTIONS"
      FIELD="CHECK"
      NUM_ROW="40"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELD_OPTIONS"
      FIELD="TABLE"
      REQUIRED="true"
      NUM_ROW="45"
      NB_LINES="5"
      SHOW_IF="USE_FIELD_OPTIONS == 'true'"
    >
      <ITEMS BASED_ON_SCHEMA="true">
        <ITEM NAME="UPDATE_KEY" VALUE="false" FIELD="CHECK" />
        <ITEM NAME="DELETE_KEY" VALUE="false" FIELD="CHECK" />
        <ITEM NAME="UPDATABLE"  VALUE="true"  FIELD="CHECK" />
        <ITEM NAME="INSERTABLE" VALUE="true"  FIELD="CHECK" />
      </ITEMS>
    </PARAMETER>


    <PARAMETER
      NAME="USE_HINT_OPTIONS"
      FIELD="CHECK"
      NUM_ROW="50"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="HINT_OPTIONS"
      FIELD="TABLE"
      REQUIRED="true"
      NUM_ROW="51"
      NB_LINES="5"
	  SHOW_IF="USE_HINT_OPTIONS == 'true'"
    >
      <ITEMS>
        <ITEM NAME="HINT" VALUE="&quot;/*+ */&quot;" FIELD="TEXT" />
        <ITEM NAME="POSITION" VALUE="AFTER" FIELD="CLOSED_LIST">
        	<ITEMS DEFAULT="AFTER">
        		<ITEM NAME="AFTER"  VALUE="AFTER"/>
        	</ITEMS>
		</ITEM>
        <ITEM NAME="SQL_STMT"  VALUE="true"  FIELD="CLOSED_LIST">
        	<ITEMS DEFAULT="INSERT">
        		<ITEM NAME="INSERT"  VALUE="INSERT"/>
				<ITEM NAME="UPDATE"  VALUE="UPDATE"/>
				<ITEM NAME="DELETE"  VALUE="DELETE"/>
				<ITEM NAME="TABLE_NAME"  VALUE="TABLE_NAME"/>
        	</ITEMS>
		</ITEM>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="CONVERT_COLUMN_TABLE_TO_UPPERCASE"
      FIELD="CHECK"
      NUM_ROW="55"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

  	<PARAMETER
  	  NAME="ENABLE_DEBUG_MODE"
  	  FIELD="CHECK"
	  NUM_ROW="60">
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>

	<!-- show_if should consider REJECT != null, but don't support it now -->
  	<PARAMETER
  	  NAME="USE_BATCH_SIZE"
  	  FIELD="CHECK"
	    NUM_ROW="65"
	    SHOW_IF="((DATA_ACTION == 'INSERT') OR (DATA_ACTION == 'UPDATE') OR (DATA_ACTION == 'DELETE'))
	           AND (USE_EXISTING_CONNECTION == 'false') AND (DB_VERSION != 'ORACLE_8')"
	  >
	  <DEFAULT>true</DEFAULT>
	</PARAMETER>

    <PARAMETER
        NAME="USE_BATCH_AND_USE_CONN"
        FIELD="CHECK"
        NUM_ROW="65"
        SHOW_IF="((DATA_ACTION == 'INSERT') OR (DATA_ACTION == 'UPDATE') OR (DATA_ACTION == 'DELETE'))
               AND (((USE_EXISTING_CONNECTION == 'true') AND (DB_VERSION != 'ORACLE_8'))
                      OR ((USE_EXISTING_CONNECTION == 'true') AND (#LINK@NODE.CONNECTION.DB_VERSION != 'ORACLE_8')))"
      >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

  	<PARAMETER
  	  NAME="BATCH_SIZE"
  	  FIELD="TEXT"
  	  NUM_ROW="65"
  	  SHOW_IF="((USE_BATCH_SIZE == 'true') AND (isShow[USE_BATCH_SIZE]))
            OR ((USE_BATCH_AND_USE_CONN == 'true') AND (isShow[USE_BATCH_AND_USE_CONN]))"
	  >
	  <DEFAULT>10000</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="SUPPORT_NULL_WHERE"
		FIELD="CHECK"
		NUM_ROW="70">
		  <DEFAULT>false</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="USE_TIMESTAMP_FOR_DATE_TYPE"
		FIELD="CHECK"
		NUM_ROW="80">
		  <DEFAULT>true</DEFAULT>
	</PARAMETER>

   <PARAMETER
       NAME="TRIM_CHAR"
       FIELD="CHECK"
       NUM_ROW="90">
         <DEFAULT>true</DEFAULT>
    </PARAMETER>

  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
       <IMPORTS>
		   <IMPORT NAME="Driver-Oracle-XDB" MODULE="xdb.jar" MVN="mvn:org.talend.libraries/xdb/6.0.0"  REQUIRED_IF="SCHEMA.DB_TYPE IN ['XMLTYPE']" />
		   <IMPORT NAME="Driver-Oracle-XMLPARSERV2" MODULE="xmlparserv2.jar" MVN="mvn:org.talend.libraries/xmlparserv2/6.0.0"  BundleID="" REQUIRED_IF="SCHEMA.DB_TYPE IN ['XMLTYPE']" />
           <IMPORT NAME="Talend-oracle-timestamptz" MODULE="talend-oracle-timestamptz.jar" MVN="mvn:org.talend.libraries/talend-oracle-timestamptz/6.0.0"
                   UrlPath="platform:/plugin/org.talend.libraries.custom/lib/talend-oracle-timestamptz.jar"
                   REQUIRED_IF="(DB_VERSION=='ORACLE_10') OR (DB_VERSION=='ORACLE_11') OR (DB_VERSION=='ORACLE_12') OR (DB_VERSION=='ORACLE_18')" />
           <IMPORT NAME="ORACLE_18" MODULE="ojdbc8-19.3.0.0.jar" MVN="mvn:com.oracle.ojdbc/ojdbc8/19.3.0.0" REQUIRED_IF="(DB_VERSION == 'ORACLE_18') AND (USE_EXISTING_CONNECTION == 'false')" />
           <IMPORT NAME="ORACLE_12"  MODULE="ojdbc7.jar" MVN="mvn:org.talend.libraries/ojdbc7/6.0.0"  BundleID="" REQUIRED_IF="(DB_VERSION == 'ORACLE_12') AND (USE_EXISTING_CONNECTION == 'false')" />
           <IMPORT NAME="ORACLE_11"  MODULE="ojdbc6.jar" MVN="mvn:org.talend.libraries/ojdbc6/6.0.0"  BundleID="" REQUIRED_IF="(DB_VERSION == 'ORACLE_11') AND (USE_EXISTING_CONNECTION == 'false')" />
           <IMPORT NAME="ORACLE_10"  MODULE="ojdbc14.jar" MVN="mvn:org.talend.libraries/ojdbc14/6.0.0"  BundleID="" REQUIRED_IF="(DB_VERSION == 'ORACLE_10') AND (USE_EXISTING_CONNECTION == 'false')" />
           <IMPORT NAME="ORACLE_9"  MODULE="ojdbc14-9i.jar" MVN="mvn:org.talend.libraries/ojdbc14-9i/6.0.0"  BundleID="" REQUIRED_IF="(DB_VERSION == 'ORACLE_9') AND (USE_EXISTING_CONNECTION == 'false')" />
           <IMPORT NAME="ORACLE_8"  MODULE="ojdbc12.jar" MVN="mvn:org.talend.libraries/ojdbc12/6.0.0"  BundleID="" REQUIRED_IF="(DB_VERSION == 'ORACLE_8') AND (USE_EXISTING_CONNECTION == 'false')" />
     </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE"          TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_UPDATED"  TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_INSERTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_DELETED"  TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_REJECTED"  TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW" SHOW_IF="ENABLE_DEBUG_MODE=='true'"/>
  </RETURNS>
</COMPONENT>
