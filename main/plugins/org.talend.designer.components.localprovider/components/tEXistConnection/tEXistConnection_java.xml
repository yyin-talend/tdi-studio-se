<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
       SERIAL=""
      VERSION="0.101"
       STATUS="ALPHA"

COMPATIBILITY="ALL"
       AUTHOR="Talend"
 RELEASE_DATE="2010012"
    STARTABLE="true"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/DB Specifics/eXist</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
    	NAME="URI"
    	FIELD="TEXT"
    	REQUIRED="true"
    	NUM_ROW="1"
    	>
      <DEFAULT>"xmldb:exist://localhost:8080/exist/xmlrpc"</DEFAULT>
    </PARAMETER>

    <PARAMETER
    	NAME="COLLECTION"
    	FIELD="TEXT"
    	REQUIRED="true"
    	NUM_ROW="2"
    	>
      <DEFAULT>"/db"</DEFAULT>
    </PARAMETER>

    <PARAMETER
    	NAME="DRIVER"
    	FIELD="TEXT"
    	REQUIRED="true"
    	NUM_ROW="3"
    	>
      <DEFAULT>"org.exist.xmldb.DatabaseImpl"</DEFAULT>
    </PARAMETER>

    <PARAMETER
    	NAME="USERNAME"
    	FIELD="TEXT"
    	REQUIRED="true"
    	NUM_ROW="4"
    	>
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
    	NAME="PASSWORD"
    	FIELD="PASSWORD"
    	REQUIRED="true"
    	NUM_ROW="5"
    	>
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

	<ADVANCED_PARAMETERS>

	</ADVANCED_PARAMETERS>

  <CODEGENERATION>
