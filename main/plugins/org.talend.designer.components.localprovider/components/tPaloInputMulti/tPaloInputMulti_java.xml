<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="2.0"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20091023"
    STARTABLE="true"
    LOG4J_ENABLED="true"
	>
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
<FAMILY>Business_Intelligence/OLAP_Cube/Palo</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

<CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>




  <PARAMETERS>

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="2"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      REQUIRED="true" FILTER="tPaloConnection"
      NUM_ROW="3"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />

	<PARAMETER
		NAME="SERVER"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="5"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
	  <DEFAULT>"localhost"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="SERVERPORT"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="6"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
	  <DEFAULT>"7777"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="USERNAME"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="7"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
	  <DEFAULT>"admin"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="PASS"
		FIELD="PASSWORD"
		REQUIRED="true"
		NUM_ROW="8"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
	>
	  <DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="DATABASE"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="10"
	>
	  <DEFAULT>"Database"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="CUBE"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="11"
	>
	  <DEFAULT>"Cube"</DEFAULT>
	</PARAMETER>

	<PARAMETER
      	NAME="CUBE_TYPE"
	      FIELD="CLOSED_LIST"
      	NUM_ROW="11"
    	>
	<ITEMS DEFAULT="CUBE_NORMAL">
        <ITEM NAME="CUBE_NORMAL" VALUE="CUBE_NORMAL" />
        <ITEM NAME="CUBE_SYSTEM" VALUE="CUBE_SYSTEM"/>
        <ITEM NAME="CUBE_ATTRIBUT" VALUE="CUBE_ATTRIBUT"/>
        <ITEM NAME="CUBE_USER_INFO" VALUE="CUBE_USER_INFO"/>
      </ITEMS>
    </PARAMETER>


	<PARAMETER
		NAME="COMMITSIZE"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="12"
	>
	  <DEFAULT>10000</DEFAULT>
	</PARAMETER>



		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="20">
			 <TABLE READONLY="false">
				<COLUMN NAME="MEASURE" TYPE="id_Double" LENGTH="255" READONLY="true" CUSTOM="true" />
				<COLUMN NAME="TEXT" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true" />
			</TABLE>
		</PARAMETER>

		<PARAMETER NAME="DIMENSION_ELEMENTS" FIELD="TABLE" REQUIRED="true" NUM_ROW="25" NB_LINES="5">
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="DIMENSIONS" FIELD="TEXT" VALUE='""'  />
				<ITEM NAME="ELEMENTS" FIELD="TEXT" VALUE='""'  />
			</ITEMS>
		</PARAMETER>





  </PARAMETERS>

  <ADVANCED_PARAMETERS>
	<PARAMETER NAME="DEPLOY_PALO_LIBS" FIELD="CHECK" NUM_ROW="200" SHOW="false">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

  </ADVANCED_PARAMETERS>
    <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="paloIX3" MODULE="paloIX3-1.0.1.jar" MVN="mvn:org.talend.components/paloIX3/1.0.1" REQUIRED="true" />
      <IMPORT NAME="httpclient-4.5.13" MODULE="httpclient-4.5.13.jar" MVN="mvn:org.apache.httpcomponents/httpclient/4.5.13" REQUIRED="true" />
      <IMPORT NAME="httpmime-4.1.2" MODULE="httpmime-4.1.2.jar" MVN="mvn:org.talend.libraries/httpmime-4.1.2/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.apache.http/lib/httpmime-4.1.2.jar" REQUIRED="true" />
      <IMPORT NAME="httpcore-4.4.13" MODULE="httpcore-4.4.13.jar" MVN="mvn:org.apache.httpcomponents/httpcore/4.4.13" REQUIRED="true" />
      <IMPORT NAME="commons-logging-1.2" MODULE="commons-logging-1.2.jar" MVN="mvn:commons-logging/commons-logging/1.2" REQUIRED="true" />
      <IMPORT NAME="commons-codec-1.14" MODULE="commons-codec-1.14.jar" MVN="mvn:commons-codec/commons-codec/1.14" REQUIRED="true" />
      <IMPORT NAME="Talend_CSV" MODULE="talendcsv.jar" MVN="mvn:org.talend.libraries/talendcsv/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.csv/lib/talendcsv.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>
    <RETURNS/>


</COMPONENT>
