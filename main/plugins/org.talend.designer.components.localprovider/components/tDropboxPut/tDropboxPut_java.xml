<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20140417A"
    STARTABLE="true"
    
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Cloud/Dropbox</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1"  />
    <CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="1"  />
    <CONNECTOR CTYPE="COMPONENT_ERROR" MAX_INPUT="1"  />
    <CONNECTOR CTYPE="RUN_IF" MAX_INPUT="1" />
  </CONNECTORS>

  <PARAMETERS>

	<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK" NUM_ROW="44">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST"  FILTER="tDropboxConnection" NUM_ROW="44"  DYNAMIC_SETTINGS="true"
    	SHOW_IF="USE_EXISTING_CONNECTION == 'true'" REQUIRED="true"/>

    <PARAMETER  NAME="ACCESS_TOKEN"  FIELD="TEXT"  NUM_ROW="48" REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'" GROUP="CONNECTION_GROUP" >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER  NAME="USE_PROXY"  FIELD="CHECK"  NUM_ROW="51" REQUIRED="true" GROUP="CONNECTION_GROUP" SHOW_IF="USE_EXISTING_CONNECTION == 'false' ">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER  NAME="PROXY_HOST"  FIELD="TEXT"  NUM_ROW="55" SHOW_IF=" (USE_EXISTING_CONNECTION == 'false') AND (USE_PROXY == 'true')" GROUP="CONNECTION_GROUP">
      <DEFAULT>"127.0.0.1"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER  NAME="PROXY_PORT"  FIELD="TEXT"  NUM_ROW="55" SHOW_IF=" (USE_EXISTING_CONNECTION == 'false') AND (USE_PROXY == 'true')" GROUP="CONNECTION_GROUP">
      <DEFAULT>8087</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="FILE_PATH"  FIELD="TEXT"  NUM_ROW="64">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="ADD_MODE" FIELD="RADIO" NUM_ROW="74" REQUIRED="false" GROUP="MODE_GROUP">
			<DEFAULT>true</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="FORCE_MODE" FIELD="RADIO" NUM_ROW="74" REQUIRED="false" GROUP="MODE_GROUP">
			<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="UPDATE_MODE" FIELD="RADIO" NUM_ROW="74" REQUIRED="false" GROUP="MODE_GROUP">
			<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="REVISION"  FIELD="TEXT"  NUM_ROW="76" REQUIRED="true" SHOW_IF="UPDATE_MODE=='true'"  GROUP="MODE_GROUP">
	     <DEFAULT>""</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="READ_CONTENT_FROM_INPUT" FIELD="RADIO" NUM_ROW="80" GROUP="UPLOAD_MODE">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
    
	<PARAMETER NAME="UPLOAD_LOCAL_FILE" FIELD="RADIO" NUM_ROW="80" GROUP="UPLOAD_MODE">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
	<PARAMETER NAME="EXPOSE_OUTPUT_STREAM" FIELD="RADIO" NUM_ROW="80" GROUP="UPLOAD_MODE">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
	<PARAMETER NAME="LOCAL_FILE_PATH" FIELD="FILE" NUM_ROW="86" SHOW_IF="UPLOAD_LOCAL_FILE == 'true' " GROUP="UPLOAD_MODE">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
	<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="99" SHOW_IF="READ_CONTENT_FROM_INPUT == 'true'" GROUP="UPLOAD_MODE">
		<TABLE READONLY="true">
			<COLUMN NAME="content" TYPE="id_Object" />
		</TABLE>
	</PARAMETER>
	
   </PARAMETERS>
   
	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>
	
   <CODEGENERATION>
      	<IMPORTS>
               <IMPORT NAME="dropbox-core-sdk"
                    MODULE="dropbox-core-sdk-2.1.2.jar" MVN="mvn:org.talend.libraries/dropbox-core-sdk-2.1.2/6.3.0" 
                    REQUIRED_IF="USE_EXISTING_CONNECTION == 'false'" />
               <IMPORT NAME="jackson-core"
                    MODULE="jackson-core-2.11.4.jar" MVN="mvn:com.fasterxml.jackson.core/jackson-core/2.11.4" 
                    REQUIRED_IF="USE_EXISTING_CONNECTION == 'false'" />
		</IMPORTS>
   </CODEGENERATION>

  <RETURNS>
  	  <RETURN NAME="OUTPUTSTREAM" TYPE="id_Object" AVAILABILITY="FLOW" SHOW_IF="EXPOSE_OUTPUT_STREAM=='true'"/>
  </RETURNS>
</COMPONENT>
