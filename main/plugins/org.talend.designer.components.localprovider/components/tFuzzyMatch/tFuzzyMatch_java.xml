<COMPONENT>
   <HEADER
       PLATEFORM="ALL"
       SERIAL=""
       VERSION="0.103"
       STATUS="ALPHA"
       COMPATIBILITY="WIN32"
       AUTHOR="Talend"
       RELEASE_DATE="20050320A"
       STARTABLE="false"
       DATA_AUTO_PROPAGATE="false"
       LOG4J_ENABLED="true"
   >
      <SIGNATURE></SIGNATURE>
   </HEADER>

  <FAMILIES>
    <FAMILY>Data_Quality/Matching</FAMILY>
  </FAMILIES>
   <DOCUMENTATION>
      <URL/>
   </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MIN_INPUT="2" MAX_INPUT="2" MIN_OUTPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="LOOKUP"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      READONLY="true" 
      REQUIRED="true"
      NUM_ROW="1"
    >
      <TABLE READONLY="true">
        <COLUMN NAME="VALUE" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="MATCHING" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>

    <PARAMETER
      NAME="MATCHING_TYPE"
      FIELD="CLOSED_LIST"
      REQUIRED="true"
      NUM_ROW="2"
    >
      <ITEMS DEFAULT="matchLevenshteinDistance">
        <ITEM NAME="LEVENSHTEIN" VALUE="matchLevenshteinDistance" />
        <ITEM NAME="METAPHONE" VALUE="matchMetaphone" />
        <ITEM NAME="DOUBLEMETAPHONE" VALUE="matchDoubleMetaphone" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="CASE_SENSITIVE"
      FIELD="CHECK"
      NOT_SHOW_IF="(MATCHING_TYPE!='LEVENSHTEIN')"
      REQUIRED="true"
      NUM_ROW="2"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="LOOKUP_COLUMN"
      FIELD="LOOKUP_COLUMN_LIST"
      REQUIRED="true"
      NUM_ROW="2"
    >
    </PARAMETER>

    <PARAMETER
      NAME="MIN"
      FIELD="TEXT"
      NOT_SHOW_IF="(MATCHING_TYPE!='LEVENSHTEIN')"
      REQUIRED="true"
      NUM_ROW="9"
    >
        <DEFAULT>0</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="MAX"
      FIELD="TEXT"
      REQUIRED="true"
      NOT_SHOW_IF="(MATCHING_TYPE!='LEVENSHTEIN')"
      NUM_ROW="9"
    >
        <DEFAULT>0</DEFAULT>
    </PARAMETER>


    <PARAMETER
      NAME="MATCH"
      FIELD="PREV_COLUMN_LIST"
      REQUIRED="true"
      NUM_ROW="11"
    >
    </PARAMETER>

    <PARAMETER
      NAME="GET_UNIQUE"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="12"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ITEMSEPARATOR"
      FIELD="TEXT"
      REQUIRED="false"
      NOT_SHOW_IF="(GET_UNIQUE=='true')"
      NUM_ROW="12"
    >
      <DEFAULT>","</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

   <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="commons-codec-1.14" MODULE="commons-codec-1.14.jar" MVN="mvn:commons-codec/commons-codec/1.14" REQUIRED="true" />
       <IMPORT NAME="Commons-Lang" MODULE="commons-lang-2.6.jar" MVN="mvn:commons-lang/commons-lang/2.6"  UrlPath="platform:/base/plugins/org.apache.commons.lang_2.6.0.v201404270220.jar" REQUIRED="true" />
     </IMPORTS>
   </CODEGENERATION>

   <RETURNS>
      <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>
