<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="BETA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20091208"
    STARTABLE="false"
    PARTITIONING="NONE"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/DB Specifics/Informix</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true" NUM_ROW="10" REPOSITORY_VALUE="DELIMITED" />

    <PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="20" REQUIRED="true" REPOSITORY_VALUE="FILE_PATH">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/informix_bulk.txt"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="APPEND" FIELD="CHECK" REQUIRED="true" NUM_ROW="40" REPOSITORY_VALUE="APPEND">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="50">
      <DEFAULT/>
    </PARAMETER>
   </PARAMETERS>

   <ADVANCED_PARAMETERS>
   	<PARAMETER NAME="ROWSEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="10" REPOSITORY_VALUE="ROW_SEPARATOR">
      <DEFAULT>"\n"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="FIELDSEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="10" MAX_LENGTH="1" REPOSITORY_VALUE="FIELD_SEPARATOR">
      <DEFAULT>"|"</DEFAULT>
    </PARAMETER>

  	<PARAMETER NAME="SET_DBMONEY" FIELD="CHECK" REQUIRED="true" NUM_ROW="20" >
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="DECIMAL_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="20"
		SHOW_IF="SET_DBMONEY == 'true')">
		<DEFAULT>"."</DEFAULT>

	</PARAMETER>
        <PARAMETER NAME="DATE_FORMAT" FIELD="CLOSED_LIST" REQUIRED="true" NUM_ROW="25">
	   <ITEMS DEFAULT="Y4MD/">
	    <ITEM NAME="Y4MD/" VALUE="yyyy/MM/dd,Y4MD/" />
      	<ITEM NAME="Y2MD/" VALUE="yy/MM/dd,Y2MD/" />
        <ITEM NAME="MDY4/" VALUE="MM/dd/yyyy,MDY4/" />
        <ITEM NAME="MDY2/" VALUE="MM/dd/yy,MDY2/" />
        <ITEM NAME="DMY4/" VALUE="dd/MM/yyyy,DMY4/" />
        <ITEM NAME="DMY2/" VALUE="dd/MM/yy,DMY2/" />
        <ITEM NAME="Y4MD-" VALUE="yyyy-MM-dd,Y4MD-" />
      	<ITEM NAME="Y2MD-" VALUE="yy-MM-dd,Y2MD-" />
        <ITEM NAME="MDY4-" VALUE="MM-dd-yyyy,MDY4-" />
        <ITEM NAME="MDY2-" VALUE="MM-dd-yy,MDY2-" />
        <ITEM NAME="DMY4-" VALUE="dd-MM-yyyy,DMY4-" />
        <ITEM NAME="DMY2-" VALUE="dd-MM-yy,DMY2-" />
        <ITEM NAME="CUSTOM" VALUE="CUSTOM" />
      </ITEMS>
	</PARAMETER>

	<PARAMETER NAME="DATE_FORMAT_CUSTOM" FIELD="TEXT" NUM_ROW="25" SHOW_IF="DATE_FORMAT == 'CUSTOM'">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

    <PARAMETER NAME="CREATE" FIELD="CHECK" REQUIRED="true" NUM_ROW="30">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="FLUSHONROW" FIELD="CHECK" REQUIRED="true" NUM_ROW="30">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="FLUSHONROW_NUM" FIELD="TEXT" REQUIRED="true" NUM_ROW="40" SHOW_IF="FLUSHONROW == 'true'">
      <DEFAULT>1</DEFAULT>
    </PARAMETER>


    <PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="60" REQUIRED="true" REPOSITORY_VALUE="ENCODING">
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>
   </ADVANCED_PARAMETERS>

	<CODEGENERATION>

	  <IMPORTS>
	    <IMPORT NAME="Talend_CSV" MODULE="talendcsv.jar" MVN="mvn:org.talend.libraries/talendcsv/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.csv/lib/talendcsv.jar" REQUIRED="true" />
	  </IMPORTS>

	</CODEGENERATION>

   <RETURNS>
     <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>
